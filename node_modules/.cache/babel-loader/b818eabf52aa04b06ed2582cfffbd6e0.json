     <b-col cols=\"12\" sm=\"4\">\r\n                <b-form-group label=\"Gaiola\" label-for=\"input-codigo\">\r\n                    <b-form-input\r\n                        type=\"number\"\r\n                        id=\"input-codigo\"\r\n                        v-model=\"value.codigo\"\r\n                        class=\"text-right\">\r\n                    </b-form-input>\r\n                </b-form-group>\r\n            </b-col>\r\n            <b-col cols=\"12\" sm=\"4\">\r\n                <b-form-group label=\"Endereço\" label-for=\"input-endereco\">\r\n                    <b-form-input\r\n                        type=\"text\"\r\n                        id=\"input-endereco\"\r\n                        v-model=\"value.endereco\"\r\n                        class=\"text-right\">\r\n                    </b-form-input>\r\n                </b-form-group>\r\n            </b-col>\r\n            <b-col cols=\"12\" sm=\"4\">\r\n                <b-form-group :label=\"`Nível ${value.nivel}`\" label-for=\"input-nivel\">\r\n                    <b-form-input\r\n                        type=\"number\"\r\n                        id=\"input-nivel\"\r\n                        v-model=\"value.nivel\"\r\n                        class=\"text-right\">\r\n                    </b-form-input>\r\n                </b-form-group>\r\n            </b-col>\r\n        </b-row>\r\n\r\n        <b-row>\r\n            <b-col cols=\"12\" md=\"8\">\r\n                <!--\r\n                    <b-form-input type=\"text\" id=\"input-item\" v-model=\"value.item\"></b-form-input>\r\n                -->\r\n                <b-form-group label=\"Item\" label-for=\"input-itens\">\r\n                    <!-- <SearchApi :run=\"runSearchItems\" id=\"input-itens\" route=\"Servicos/Itens/MateriaPrima\" :value=\"itenssearch\"/> --> \r\n                    <InputSearch2 \r\n                        id=\"input-itens\"\r\n                        ref=\"itemRef\"\r\n                        routeapi=\"Servicos/Itens/MateriaPrima\"\r\n                        v-model=\"value.item\"\r\n                        :pk=\"value.item\"/>\r\n                </b-form-group>\r\n            </b-col>\r\n            <b-col cols=\"12\" md=\"4\">\r\n                <b-form-group label=\"Quantidade\" label-for=\"input-quant\">\r\n                    <b-form-input\r\n                        type=\"number\"\r\n                        id=\"input-quant\"\r\n                        v-model=\"value.quant\"\r\n                        class=\"text-right\">\r\n                    </b-form-input>\r\n                </b-form-group>\r\n            </b-col>\r\n        </b-row>\r\n\r\n        <hr/>\r\n\r\n        <b-button\r\n            variant=\"primary\"\r\n            @click=\"btnGaiolasItensUpdate\"\r\n            class=\"float-right\">\r\n            Incluir/Alterar\r\n        </b-button>\r\n\r\n        <!-- value:\r\n        <pre>\r\n            {{ value }}\r\n        </pre> --> \r\n       \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions, mapGetters } from 'vuex'\r\nimport SearchApi from '../../../components/SearchApi.vue'\r\nimport InputSearch2 from '../../../components/InputSearch2.vue'\r\nexport default {\r\n\r\n    name: 'GaiolasItens',\r\n\r\n    components: {\r\n\r\n        SearchApi,\r\n        InputSearch2\r\n\r\n    },\r\n\r\n    props: {\r\n        value: {\r\n            type: Object,\r\n            default() {\r\n                return {\r\n                    codigo: '0',\r\n                    endereco: '0',\r\n                    emp: '0',\r\n                    dep: '0',\r\n                    item: [],\r\n                    quant: '0',\r\n                    deleta:  '0',\r\n                    nivel: '1'\r\n                }\r\n            } \r\n        },\r\n\r\n        runSearchItems: {\r\n            type: Number,\r\n            default() {\r\n                return 0\r\n            }\r\n        }\r\n    },\r\n\r\n    data() {\r\n\r\n        return {\r\n            run: 0,\r\n            itenssearch: {\r\n                    value: this.value.item,\r\n                    text: this.value.item\r\n            }\r\n        }\r\n\r\n    },\r\n\r\n    beforeMount() {\r\n\r\n        this.actAlmox()\r\n\r\n        if (this.value.item !== '') {\r\n           this.searchItem(this.value.item)\r\n        }\r\n\r\n    },\r\n\r\n    computed: {\r\n\r\n        ...mapGetters([\r\n            'gtxGaiolasItensData',\r\n            'gtxempresas',\r\n            'gtxAlmoxData',\r\n            'gtxAlmoxDataEmpresas',\r\n            'gtxAlmoxDataAlmox'\r\n        ]),\r\n\r\n        item() {\r\n            return this.itenssearch.value\r\n        }\r\n\r\n    },\r\n\r\n    methods: {\r\n\r\n        ...mapActions([\r\n            'actGaiolaItens',\r\n            'actAlmox'\r\n        ]),\r\n\r\n        searchItem(item) {\r\n\r\n            this.$nextTick(() => {\r\n                this.$refs.itemRef.search(this, item)\r\n            })\r\n\r\n        },\r\n\r\n        btnGaiolasItensUpdate() {\r\n\r\n            this.$emit('etv-gaiolasitensupdate', this.value)\r\n\r\n            console.log('btnGaiolasItensUpdate:', this.value)\r\n\r\n        }\r\n\r\n    }\r\n\r\n\r\n}\r\n</script>\r\n"]}]}