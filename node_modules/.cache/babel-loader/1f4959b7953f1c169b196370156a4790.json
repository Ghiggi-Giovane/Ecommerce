ocomplete=\"current-password\"\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n      <b-form-group>\r\n        <b-button block type=\"submit\" size=\"sm\" variant=\"outline-info\">\r\n          Entrar\r\n        </b-button>\r\n      </b-form-group>\r\n    </form>\r\n-->\r\n\r\n\r\n<!--\r\n    <CardCollape title=\"Opções\" icon=\"xxxx\">\r\n      <template slot=\"header\">\r\n        header no slot\r\n      </template>\r\n      <h1>\r\n        Título\r\n      </h1>\r\n      <div>\r\n        corpo do slot\r\n      </div>\r\n    </CardCollape>\r\n    <CardCollape title=\"Opção Danger\" icon=\"caret-down\" badge=\"19\">\r\n      <label>\r\n        <input type=\"text\" />\r\n      </label>\r\n    </CardCollape>\r\n    <CardCollape title=\"Configurações\" icon=\"caret-down-square\">\r\n      <template slot=\"footer\">\r\n        <hr />\r\n        <div class=\"text-right\">\r\n          <b-button variant=\"secondary\" class=\"mr-1\">Cancelar</b-button>\r\n          <b-button variant=\"primary\">Salvar</b-button>\r\n        </div>\r\n      </template>\r\n    </CardCollape>\r\n\r\n    <GaiolasItens />\r\n\r\n    <a href=\"http://servicos.kappesberg.com.br\" target=\"_nova\">Kappes</a>\r\n\r\n    <input v-model=\"barcodeValue\" /><br />\r\n    <barcode v-bind:value=\"barcodeValue\">\r\n      Show this if the rendering fails.\r\n    </barcode>\r\n\r\n    <QrCode :value=\"qrcode\" />\r\n-->\r\n    <!-- <vue-qrcode :value=\"qrcode\"/> -->\r\n\r\n<!--\r\n    <div class=\"container\">\r\n      <b-row>\r\n        <b-col>\r\n          <Dashcard\r\n            value=\"8350\"\r\n            details=\"Estoque\"\r\n            icon=\"cart-check\"\r\n            css=\"bg-primary-light\"\r\n          />\r\n        </b-col>\r\n        <b-col>\r\n          <Dashcard\r\n            value=\"298\"\r\n            details=\"Entradas\"\r\n            icon=\"cart-plus\"\r\n            css=\"bg-success-light\"\r\n          />\r\n        </b-col>\r\n        <b-col>\r\n          <Dashcard\r\n            value=\"687\"\r\n            details=\"Saidas\"\r\n            icon=\"cart-dash\"\r\n            css=\"bg-danger-light\"\r\n          />\r\n        </b-col>\r\n        <b-col>\r\n          <Dashcard\r\n            value=\"24840\"\r\n            details=\"Cubagem\"\r\n            icon=\"minecart-loaded\"\r\n            css=\"bg-info-light\"\r\n          />\r\n        </b-col>\r\n      </b-row>\r\n    </div>\r\n-->\r\n<!--\r\n    <GridLayout />\r\n-->\r\n    <!--         <div class=\"deck\">\r\n            <b-row>\r\n                <b-col cols=\"12\" sm=\"6\" lg=\"4\">\r\n                    <Dashcard \r\n                        value=\"8350\"\r\n                        details=\"Estoque\"\r\n                        icon=\"cart-check\"\r\n                        variant=\"primary\"\r\n                    />\r\n                </b-col>\r\n                <b-col cols=\"12\" sm=\"6\" lg=\"4\">\r\n                    <Dashcard \r\n                        value=\"298\"\r\n                        details=\"Entradas\"\r\n                        icon=\"cart-plus\"\r\n                        variant=\"success\"\r\n                    />\r\n                </b-col>\r\n                <b-col cols=\"12\" sm=\"6\" lg=\"4\">\r\n                    <Dashcard \r\n                        value=\"687\"\r\n                        details=\"Saidas\"\r\n                        icon=\"cart-dash\"\r\n                        variant=\"danger\"\r\n                    />\r\n                </b-col>\r\n                <b-col cols=\"12\" sm=\"6\" lg=\"4\">\r\n                    <Dashcard \r\n                        value=\"24840\"\r\n                        details=\"Cubagem\"\r\n                        icon=\"minecart-loaded\"\r\n                        variant=\"info\"\r\n                    />\r\n                </b-col>\r\n            </b-row>\r\n        </div>\r\n -->\r\n    <!--\r\n        gtxParempBusy: {{gtxParempBusy}}         \r\n        gtxParempResponse: {{gtxParempResponse}}\r\n        gtxParempSuccess: {{gtxParempSuccess}}\r\n        gtxParempError: {{gtxParempError}}\r\n        gtxParempData: {{gtxParempData}}\r\n        gtxParempDataEmpresas: {{gtxParempDataEmpresas}}\r\n        gtxParempDataDepositos: {{gtxParempDataDepositos}}\r\n        gtxParempDataAlmoxarifados: {{gtxParempDataAlmoxarifados}}\r\n        gtxParempDataDivisoes: {{gtxParempDataDivisoes}}\r\n-->\r\n<!--\r\n  <CardCollapsed ref=\"CardCollapsed\" v-model=\"collapseformopen\" title=\"Pedidos\" @evtBusca=\"arquivosGet\">\r\n    <template #collapsedform>\r\n    <div>\r\n      <label for=\"form-dataleitura\">Data de Leitura</label>\r\n      <b-form-datepicker\r\n        id=\"form-dataleitura\"\r\n        v-model=\"database\"\r\n        class=\"mb-2\"\r\n        locale=\"pt\"\r\n        :date-format-options=\"gtxConfigDatetimePickerFormat\"\r\n      ></b-form-datepicker>\r\n    </div>\r\n    <div>\r\n      <label for=\"form-idacao\">Processo</label>\r\n      <b-form-select\r\n        id=\"form-idacao\"\r\n        v-model=\"idacao\"\r\n        :options=\"acoes\"\r\n      ></b-form-select>\r\n    </div>\r\n    <div>\r\n      <label for=\"form-filename\">Nome do Arquivo</label>\r\n      <b-form-input id=\"form-filename\" v-model=\"filename\"></b-form-input>\r\n    </div>\r\n    <div class=\"mt-2 text-right\">\r\n      <b-button-group>\r\n        <b-button variant=\"primary\" @click=\"arquivosGet\">\r\n          <b-icon-search class=\"mr-1\"/>\r\n          Buscar\r\n        </b-button>\r\n      </b-button-group>\r\n    </div>\r\n  </template>\r\n\r\n  <template #collapsedbody>\r\n    <div>\r\n      <b-row>\r\n        <b-col>\r\n          <Dashcard\r\n            :value=\"nrodangers\"\r\n            details=\"Com erros\"\r\n            icon=\"bug\"\r\n            css=\"bg-danger-light\"\r\n            textlink=\"Filtrar com erros\"\r\n            @evt_footerclick=\"dashcardfooterclick($event, true)\"\r\n          />\r\n        </b-col>\r\n        <b-col>\r\n          <Dashcard\r\n            :value=\"nrosuccess\"\r\n            details=\"Processados\"\r\n            icon=\"check2\"\r\n            css=\"bg-success-light\"\r\n            textlink=\"Filtrar com sucesso\"\r\n            @evt_footerclick=\"dashcardfooterclick($event, false)\"\r\n          />\r\n        </b-col>\r\n        <b-col>\r\n          <Dashcard\r\n            :value=\"nrowarnings\"\r\n            details=\"Não processados\"\r\n            icon=\"slash-circle\"\r\n            css=\"bg-warning-light\"\r\n            textlink=\"Filtrar não processados\"\r\n            @evt_footerclick=\"dashcardfooterclick($event, null)\"\r\n          />\r\n        </b-col>\r\n        <b-col>\r\n          <Dashcard\r\n            :value=\"nroinfo\"\r\n            details=\"Todos os arquivos\"\r\n            icon=\"check2-all\"\r\n            css=\"bg-info-light\"\r\n            textlink=\"Todos\"\r\n            @evt_footerclick=\"dashcardfooterclick($event, 'all')\"\r\n          />\r\n        </b-col>\r\n      </b-row>\r\n    </div>\r\n\r\n    <div>\r\n      <div class=\"d-flex\">\r\n        <b-input-group prepend=\"Filtro\" size=\"sm\" class=\"mb-2 mr-2\">\r\n          <b-form-input\r\n            id=\"input-filtro\"\r\n            type=\"search\"\r\n            v-model=\"arquivos.filtro\"\r\n            placeholder=\"Informe o nome do arquivo\"\r\n          ></b-form-input>\r\n        </b-input-group>\r\n\r\n        <b-pagination\r\n          id=\"input-pagination\"\r\n          v-model=\"arquivos.currentPage\"\r\n          :total-rows=\"arquivos.data.length\"\r\n          :per-page=\"arquivos.perPage\"\r\n          size=\"sm\"\r\n          class=\"my-0\"\r\n        >\r\n        </b-pagination>\r\n      </div>\r\n\r\n      <b-table\r\n        small\r\n        hover\r\n        responsive\r\n        striped\r\n        bordered\r\n        head-variant=\"dark\"\r\n        :items=\"despesas\"\r\n      >\r\n        <template #cell(DataLeitura)=\"data\">\r\n          {{ data.item.DataLeitura | datetime }}\r\n        </template>\r\n        <template #cell(id)=\"data\">\r\n          <span  v-copy>{{ data.item.id }}</span>\r\n        </template>\r\n        <template #cell(buttons)=\"data\">\r\n          <span class=\"spaced iconbutton\" @click=\"\">\r\n            <b-icon-file-earmark-text />\r\n          </span>\r\n        </template>\r\n      </b-table>\r\n\r\n      <div class=\"d-flex float-right\">\r\n        <b-pagination\r\n          id=\"input-pagination\"\r\n          v-model=\"arquivos.currentPage\"\r\n          :total-rows=\"arquivos.data.length\"\r\n          :per-page=\"arquivos.perPage\"\r\n          size=\"sm\"\r\n          class=\"my-0\"\r\n        >\r\n        </b-pagination>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  </CardCollapsed>\r\n-->\r\n<div>\r\n  <BarcodeScanner v-model=\"barras\"/>\r\n</div>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\n<br/>\r\nbarras: {{barras}}\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapActions } from \"vuex\";\r\n// import \"https://cdn.jsdelivr.net/npm/clappr@latest/dist/clappr.min.js\"\r\n// import ColetaEAN from '../../../components/ColetaEAN'\r\nimport GaiolasItens from \"../../almoxarifado/components/GaiolasItens\";\r\nimport QrCode from \"../../../components/QrCode\";\r\nimport Dashcard from \"../../../components/Dashcard.vue\";\r\n/* import VueQrcode from 'vue-qrcode' */\r\nimport GridLayout from \"../../GridLayout/views/index.vue\";\r\nimport Signature from '../../../components/Signature'\r\n\r\nimport CardCollape from \"../../../components/CardCollapse\";\r\n\r\nimport model from \"../../almoxarifado/stores/model\";\r\nimport state from \"../../../store\";\r\n\r\nimport API from \"../../../services/api.service.js\";\r\nimport BarcodeScanner from \"@/components/BarcodeScanner\";\r\nimport CardCollapsed from \"@/components/CardCollapsed\";\r\n\r\nexport default {\r\n  name: \"pedidos\",\r\n\r\n  data() {\r\n    return {\r\n      despesas: [{id: 1, }],\r\n      sigImage: \"\",\r\n      storeVar: \"\",\r\n      video: process.env.BASE_API || \"nenhum\",\r\n      barcodeValue: \"test\",\r\n      qrcode: \"TESTE DE QRCODE\",\r\n      almox: model.almox,\r\n      apiData: model.apiData,\r\n      prepareApiData: model.prepareApiData({\r\n        Success: \"False\",\r\n        Error: \"deu erro\",\r\n        Data: [{ id: 1, text: \"AAAA\" }],\r\n      }),\r\n      users: [],\r\n      user: {},\r\n      ramais: [],\r\n      barras: []\r\n    };\r\n  },\r\n\r\n  created() {\r\n    //var player = new Clappr.Player({source: \"http://your.video/here.mp4\", parentId: \"#player\"});\r\n  },\r\n\r\n  components: {\r\n    // ColetaEAN\r\n    GaiolasItens,\r\n    QrCode,\r\n    /* VueQrcode */\r\n    Dashcard,\r\n    GridLayout,\r\n    CardCollape,\r\n    BarcodeScanner,\r\n    Signature,\r\n    CardCollapsed,\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\r\n      \"gtxParempBusy\",\r\n      \"gtxParempResponse\",\r\n      \"gtxParempSuccess\",\r\n      \"gtxParempError\",\r\n      \"gtxParempData\",\r\n      \"gtxParempDataEmpresas\",\r\n      \"gtxParempDataDepositos\",\r\n      \"gtxParempDataAlmoxarifados\",\r\n      \"gtxParempDataDivisoes\",\r\n    ]),\r\n\r\n    userid() {\r\n      return { id: this.user.id };\r\n    },\r\n\r\n    getstore() {\r\n      if (this.storeVar !== \"\") {\r\n        return state.getters[this.storeVar];\r\n      }\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    ...mapActions([\"actColeta\"]),\r\n\r\n    activeTab(tab, prevTab, event) {\r\n      console.log(\"Foi para a aba:\", tab);\r\n      console.log(\"Veio da aba:\", prevTab);\r\n      console.log(\"event:\", event);\r\n    },\r\n    download() {\r\n      API.downloadFile({\r\n        url: \"https://intranet.grupok1.com.br/AppDownload/OM_Mecanicos.apk\",\r\n        filename: \"OM.apk\",\r\n      });\r\n    },\r\n\r\n    getUsers() {\r\n      API.localDB\r\n        .collection(\"users\")\r\n        .get()\r\n        .then((users) => {\r\n          this.users = users;\r\n        });\r\n    },\r\n    async setUser(user) {\r\n      await API.localDB.collection(\"users\").add(user);\r\n      this.getUsers();\r\n    },\r\n\r\n    getUser(id) {\r\n      API.localDB\r\n        .collection(\"users\")\r\n        .doc(id)\r\n        .get()\r\n        .then((user) => {\r\n          this.user = user;\r\n        });\r\n    },\r\n\r\n    ramaisget() {\r\n      this.$api.get('Servicos/Ramais').then((ramais) => {if (ramais.Success) this.ramais = ramais});\r\n    }\r\n  },\r\n};\r\n</script>\r\n"]}]}