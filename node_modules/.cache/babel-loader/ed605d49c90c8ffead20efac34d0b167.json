{"remainingRequest":"C:\\k1-services\\node_modules\\babel-loader\\lib\\index.js!C:\\k1-services\\node_modules\\cache-loader\\dist\\cjs.js??ref--1-0!C:\\k1-services\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\k1-services\\src\\pages\\navbar\\components\\MainNav.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\k1-services\\src\\pages\\navbar\\components\\MainNav.vue","mtime":1670977910650},{"path":"C:\\k1-services\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1670977934221},{"path":"C:\\k1-services\\node_modules\\babel-loader\\lib\\index.js","mtime":1670977935138},{"path":"C:\\k1-services\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1670977934221},{"path":"C:\\k1-services\\node_modules\\vue-loader\\lib\\index.js","mtime":1670977935327}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkQA;AACA;AACA;AACA;AACA;AACA;AAEA;EACAA,eADA;EAGAC;IACAC,KADA;IAEAC,QAFA;IAGAC;EAHA,CAHA;;EASAC;IACA;MACAC;IADA;EAGA,CAbA;;EAeAC,YACA,eACA,YADA,EAEA,iBAFA,EAGA,gBAHA,EAIA,aAJA,EAKA,2BALA,EAMA,SANA,EAOA,WAPA,EAQA,cARA,EASA,UATA,EAUA,YAVA,EAWA,oBAXA,EAYA,mBAZA,EAaA,eAbA,EAcA,mBAdA,EAeA,0BAfA,EAgBA,aAhBA,EAiBA,uBAjBA,EAkBA,iBAlBA,EADA;;IAsBAC;MACA;MACA;IACA,CAzBA;;IA2BAC;MACA;IACA;;EA7BA,CAfA;;EA+CAC;IACAC;EACA,CAjDA;;EAmDAC;IACA;IAEA;MACA;MACA;IACA;EACA,CA1DA;;EA4DAC;IACA;IACAF;EACA,CA/DA;;EAiEAG,WACA,eACA,iBADA,EAEA,mBAFA,EAGA,oBAHA,EAIA,mBAJA,EAKA,UALA,EAMA,YANA,EADA;;IASAC;MACA;MACA;IACA,CAZA;;IAaAC;MACA;IACA,CAfA;;IAgBAC;MACA;MACA;IACA,CAnBA;;IAoBAC;MACA,WACAC,WADA,CACA,sCADA,EACA,UADA,EAEAC,IAFA,CAEAC;QACA;MACA,CAJA;IAKA,CA1BA;;IA2BAC;MACA;MACA;IACA,CA9BA;;IA+BAC;MACA;MACA;MACA;IACA,CAnCA;;IAoCAC;MACA;IACA;;EAtCA,CAjEA;EA0GAC;IACAC;MACA;MACA;IACA;;EAJA;AA1GA","names":["name","components","Login","Profiler","VuexInspect","data","inputPesquisa","computed","badgerros","logoK1","beforeMount","ApiService","created","destroyed","methods","menuclick","searchclick","onlogin","logout","showmessage","then","choice","selidioma","selparemp","onLoginDropDown","watch","gtxLoginHits"],"sourceRoot":"src/pages/navbar/components","sources":["MainNav.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <b-navbar\r\n      toggleable=\"sm\"\r\n      type=\"dark\"\r\n      variant=\"danger\"\r\n      class=\"shadow\"\r\n      v-if=\"gtxnavbar\"\r\n    >\r\n      <div style=\"display: flex\">\r\n        <b-navbar-brand href=\"#\" to=\"/home\"\r\n          ><img\r\n            :src=\"logoK1\"\r\n             v-b-tooltip.hover title=\"Home site\"\r\n            style=\"width: 24px;\"\r\n        /></b-navbar-brand>\r\n        <b-navbar-nav v-if=\"gtxloginAuth\">\r\n          <b-nav-item v-b-toggle.sidemenu  v-b-tooltip.hover title=\"Menu lateral\">\r\n            <!-- <b-icon-layout-sidebar-inset/> -->\r\n            <b-icon-menu-app />\r\n          </b-nav-item>\r\n        </b-navbar-nav>\r\n      </div>\r\n\r\n      <b-navbar-nav>\r\n        <b-nav-item align=\"start\" href=\"#\" :to=\"gtxhome\" v-b-tooltip.hover title=\"Home page\">\r\n          <b-icon icon=\"house-fill\" />\r\n        </b-nav-item>\r\n      </b-navbar-nav>\r\n\r\n      <b-navbar-toggle target=\"nav-collapse\"></b-navbar-toggle>\r\n\r\n      <b-collapse id=\"nav-collapse\" is-nav>\r\n        <b-navbar-nav>\r\n          <b-nav-item\r\n            align=\"start\"\r\n            href=\"#\"\r\n            v-b-tooltip.hover :title=\"$t(menu.caption.toLocaleLowerCase()) | proper\"\r\n            v-for=\"(menu, index) of gtxmenus\"\r\n            :key=\"index\"\r\n            :to=\"menu.to\"\r\n            ><b-icon :icon=\"menu.icon\" />\r\n            {{ $t(menu.caption.toLocaleLowerCase()) | capitalize }}</b-nav-item\r\n          >\r\n          <!-- <b-nav-item align=\"start\" href=\"#\" to=\"/testes\"><b-icon icon=\"columns-gap\"/>Testes</b-nav-item>\r\n                <b-nav-item v-if=\"gtxSystemErrorLog\" align=\"start\" href=\"#\" to=\"/error\"><b-icon icon=\"bug\"/>Error</b-nav-item> -->\r\n        </b-navbar-nav>\r\n\r\n        <!-- Right aligned nav items -->\r\n        <b-navbar-nav class=\"ml-auto\">\r\n          <b-nav-item-dropdown right v-if=\"sysdevmode\" v-b-tooltip.hover title=\"Development Mode\">\r\n            <template v-slot:button-content to=\"/admindashboard\">\r\n              <b-icon-gear />\r\n            </template>\r\n            <b-dropdown-item :to=\"'/admindashboard'\">\r\n              <b-icon-tools />\r\n              Administrador\r\n            </b-dropdown-item>\r\n            <b-dropdown-item\r\n              href=\"#\"\r\n              title=\"Development Mode\"\r\n              v-b-modal.modal-nav-vuexinspector\r\n            >\r\n              <b-icon-braces />\r\n              Vuex Inspector\r\n            </b-dropdown-item>\r\n            <b-dropdown-item href=\"#\" title=\"Off Line\" to=\"/offline\">\r\n              <b-icon-wifi-off />\r\n              Off Line\r\n            </b-dropdown-item>\r\n            <b-dropdown-item href=\"#\" title=\"Icones\" to=\"/icons\">\r\n              <b-icon-puzzle />\r\n              √çcones\r\n            </b-dropdown-item>\r\n          </b-nav-item-dropdown>\r\n\r\n          <!--\r\n          <b-nav-item\r\n            href=\"#\"\r\n            :to=\"'/chat'\"\r\n            v-b-tooltip.hover title=\"Chat\"\r\n            v-if=\"gtxchatShow(gtxAuthUser) && sysdevmode\"\r\n          >\r\n            <b-icon-chat-dots />\r\n            <b-badge\r\n              v-if=\"gtxchatMessagesUnread(gtxAuthUser) > 0\"\r\n              variant=\"light\"\r\n              class=\"ml-1 float-sm-right shake_me\"\r\n              style=\"font-size: 10px;\"\r\n            >\r\n              {{ gtxchatMessagesUnread(gtxAuthUser) }}\r\n            </b-badge>\r\n          </b-nav-item>\r\n          -->\r\n\r\n          <b-nav-item-dropdown right v-b-tooltip.hover title=\"Pesquisar\">\r\n            <template v-slot:button-content>\r\n              <b-icon-search />\r\n            </template>\r\n            <b-dropdown-form class=\"searchbar\">\r\n              <b-input-group block>\r\n                <b-form-input\r\n                  size=\"sm\"\r\n                  :placeholder=\"$t('pesquisar') | capitalize\"\r\n                  v-model=\"inputPesquisa\"\r\n                ></b-form-input>\r\n                <b-input-group-append>\r\n                  <b-button\r\n                    size=\"sm\"\r\n                    variant=\"outline-success\"\r\n                    type=\"button\"\r\n                    :disabled=\"inputPesquisa.length == 0\"\r\n                    @click=\"searchclick\"\r\n                    ><b-icon-search\r\n                  /></b-button>\r\n                </b-input-group-append>\r\n              </b-input-group>\r\n            </b-dropdown-form>\r\n          </b-nav-item-dropdown>\r\n\r\n          <b-navbar-nav v-if=\"badgerros > 0\">\r\n            <b-nav-item href=\"#\" :to=\"'/error'\" v-b-tooltip.hover title=\"Erros\">\r\n              <b-icon-bug />\r\n              <b-badge\r\n                variant=\"light\"\r\n                class=\"float-sm-right\"\r\n                style=\"font-size: 10px;\"\r\n              >\r\n                {{ badgerros }}\r\n              </b-badge>\r\n            </b-nav-item>\r\n          </b-navbar-nav>\r\n\r\n          <b-navbar-nav>\r\n            <b-nav-item href=\"#\" :to=\"'/ramais'\" v-b-tooltip.hover title=\"Ramais\">\r\n              <b-icon-telephone />\r\n            </b-nav-item>\r\n          </b-navbar-nav>\r\n\r\n          <b-nav-item-dropdown right no-caret v-b-tooltip.hover title=\"Idiomas\">\r\n            <template v-slot:button-content>\r\n              <b-icon-globe2 />\r\n              <b-badge\r\n                variant=\"outline-light\"\r\n                class=\"float-right\"\r\n                v-if=\"gtxidiomasselected\"\r\n                >{{ gtxidiomasselected.lang }}</b-badge\r\n              >\r\n            </template>\r\n            <b-dropdown-item\r\n              href=\"#\"\r\n              v-for=\"(idioma, index) of gtxidiomas\"\r\n              :key=\"index\"\r\n              @click=\"selidioma(idioma)\"\r\n            >\r\n              <b-icon-toggle-on\r\n                variant=\"success\"\r\n                v-if=\"idioma.lang == gtxidiomasselected.lang\"\r\n              />\r\n              <b-icon-toggle-off variant=\"info\" v-else />\r\n              {{ idioma.caption }} ({{ idioma.lang }})\r\n            </b-dropdown-item>\r\n          </b-nav-item-dropdown>\r\n\r\n          <b-nav-item-dropdown right no-caret v-show=\"gtxloginAuth\" lazy v-b-tooltip.hover title=\"Empresas\">\r\n            <template v-slot:button-content>\r\n              <b-icon-building />\r\n              <b-badge variant=\"outline-light\" class=\"float-right\">{{\r\n                gtxConfigParemp\r\n              }}</b-badge>\r\n            </template>\r\n            <b-dropdown-item\r\n              href=\"#\"\r\n              v-for=\"(paremp, index) of gtxParempData\"\r\n              :key=\"index\"\r\n              @click=\"selparemp(paremp)\"\r\n            >\r\n              <b-icon-toggle-on\r\n                variant=\"success\"\r\n                v-if=\"paremp.value == gtxConfigParemp\"\r\n              />\r\n              <b-icon-toggle-off variant=\"info\" v-else />\r\n              {{ paremp.text | proper }}\r\n            </b-dropdown-item>\r\n          </b-nav-item-dropdown>\r\n\r\n          <b-nav-item-dropdown\r\n            right\r\n            no-caret\r\n            v-b-tooltip.hover title=\"Autentica√ß√£o\"\r\n            v-show=\"!gtxloginAuth\"\r\n            @shown=\"onLoginDropDown()\"\r\n          >\r\n            <!-- Using 'button-content' slot -->\r\n            <template #button-content>\r\n              <b-icon-person-x />\r\n            </template>\r\n            <b-dropdown-item href=\"#\" to=\"/login\">\r\n              Login\r\n            </b-dropdown-item>\r\n            <b-dropdown-divider />\r\n\r\n            <b-dropdown-form style=\"width: 250px;\">\r\n              <Login ref=\"LoginRef\" />\r\n            </b-dropdown-form>\r\n          </b-nav-item-dropdown>\r\n\r\n          <b-nav-item-dropdown right no-caret v-show=\"gtxloginAuth\">\r\n            <!-- Using 'button-content' slot -->\r\n            <template #button-content>\r\n              <b-icon-person-check-fill />\r\n            </template>\r\n\r\n            <b-dropdown-item href=\"#\" v-b-modal.modal-profiler>\r\n              <b-icon-person-badge />\r\n              {{ gtxAuthProfiledisplayName }}\r\n            </b-dropdown-item>\r\n            <b-dropdown-item href=\"#\" v-b-modal.modal-config>\r\n              <b-icon-tools />\r\n              Configura√ß√µes\r\n            </b-dropdown-item>\r\n            <b-dropdown-divider />\r\n            <b-dropdown-item variant=\"danger\" href=\"#\" @click=\"logout\">\r\n              <b-icon-power />\r\n              Sair\r\n            </b-dropdown-item>\r\n          </b-nav-item-dropdown>\r\n        </b-navbar-nav>\r\n      </b-collapse>\r\n    </b-navbar>\r\n\r\n    <!--\r\n        gtxConfigParemp: {{gtxConfigParemp}}<br/>\r\n        gtxConfigAlmox: {{gtxConfigAlmox}}<br/>\r\n        gtxParempSelected: {{gtxParempSelected}}\r\n        gtxParempData: {{gtxParempData}}\r\n        -->\r\n\r\n    <b-modal id=\"modal-profiler\" title=\"Meu cadastro\" hide-footer>\r\n      <Profiler />\r\n    </b-modal>\r\n\r\n    <b-modal id=\"modal-config\" :title=\"$t('configura√ß√µes') | proper\"> </b-modal>\r\n\r\n    <b-modal\r\n      id=\"modal-nav-vuexinspector\"\r\n      title=\"Vuex Inspector\"\r\n      size=\"xl\"\r\n      scrollable\r\n      hide-footer\r\n      @shown=\"$refs.vuexinspector.doInputFocus()\"\r\n    >\r\n      <VuexInspect ref=\"vuexinspector\" />\r\n    </b-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapActions } from \"vuex\";\r\nimport ApiService from \"../../../services/api.service.js\";\r\nimport store from \"../stores\";\r\nimport Login from \"../../login/components/Login\";\r\nimport Profiler from \"../../profiler/components/Profiler\";\r\nimport VuexInspect from \"../../../components/VuexInspect\";\r\n\r\nexport default {\r\n  name: \"MainNav\",\r\n\r\n  components: {\r\n    Login,\r\n    Profiler,\r\n    VuexInspect,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      inputPesquisa: \"\",\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\r\n      \"sysdevmode\",\r\n      \"gtxConfigParemp\",\r\n      \"gtxConfigAlmox\",\r\n      \"gtxAuthUser\",\r\n      \"gtxAuthProfiledisplayName\",\r\n      \"gtxhome\",\r\n      \"gtxnavbar\",\r\n      \"gtxloginAuth\",\r\n      \"gtxmenus\",\r\n      \"gtxidiomas\",\r\n      \"gtxidiomasselected\",\r\n      \"gtxSystemErrorLog\",\r\n      \"gtxParempData\",\r\n      \"gtxParempSelected\",\r\n      \"gtxchatusercountunreaded\",\r\n      \"gtxchatShow\",\r\n      \"gtxchatMessagesUnread\",\r\n      \"geterrorslenght\",\r\n    ]),\r\n\r\n    badgerros() {\r\n      // return this.gtxSystemErrorLog.length\r\n      return this.geterrorslenght;\r\n    },\r\n\r\n    logoK1() {\r\n      return require(\"../../../assets/K1_Logo_BRANCO_sm.png\");\r\n    },\r\n  },\r\n\r\n  beforeMount() {\r\n    ApiService.loginObserver.subscribe(this.onlogin);\r\n  },\r\n\r\n  created() {\r\n    // this.onFireData('chat')\r\n\r\n    if (!this.$store.hasModule(\"mainnav\")) {\r\n      this.$store.registerModule(\"mainnav\", store);\r\n      this.actmenus(\"\");\r\n    }\r\n  },\r\n\r\n  destroyed() {\r\n    // this.onFireData('chat')\r\n    ApiService.loginObserver.unsubscribe(this.onlogin);\r\n  },\r\n\r\n  methods: {\r\n    ...mapActions([\r\n      \"actConfigParemp\",\r\n      \"actParempSelected\",\r\n      \"actidiomasselected\",\r\n      \"setParempSelected\",\r\n      \"actmenus\",\r\n      \"onFireData\",\r\n    ]),\r\n    menuclick(idioma) {\r\n      // console.log(\"MainNav.language.click: \", idioma);\r\n      this.actidiomasselected(idioma);\r\n    },\r\n    searchclick() {\r\n      this.$router.push(`/sitemap/${this.inputPesquisa}`);\r\n    },\r\n    onlogin(auth) {\r\n      // console.log(\"onlogin\", auth);\r\n      this.actmenus();\r\n    },\r\n    logout() {\r\n      this.$root\r\n        .showmessage(\"Deseja realmente encerrar a sess√£o ?\", \"Intranet\")\r\n        .then((choice) => {\r\n          if (choice) this.$emit(\"evt-logout\", true);\r\n        });\r\n    },\r\n    selidioma(idioma) {\r\n      this.actidiomasselected(idioma);\r\n      this.$emit(\"evt-idioma\", idioma);\r\n    },\r\n    selparemp(paremp) {\r\n      this.actConfigParemp(paremp.value);\r\n      this.actParempSelected(paremp.value);\r\n      this.$emit(\"evt-paremp\", paremp);\r\n    },\r\n    onLoginDropDown() {\r\n      this.$refs.LoginRef.doInputFocus();\r\n    },\r\n  },\r\n\r\n  watch: {\r\n    gtxLoginHits() {\r\n      // console.log(\"App.mainnav:\", this.gtxAuthisLoggedin);\r\n      if (this.gtxAuthisLoggedin) this.actmenus();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.nav-item {\r\n  padding-left: 5px;\r\n  padding-right: 5px;\r\n}\r\n.nav-item:hover {\r\n  background-color: brown;\r\n  border-radius: 5px;\r\n}\r\n.searchbar {\r\n  width: 350px;\r\n}\r\n\r\n/* ANIMATIONS */\r\n.blink_me {\r\n  animation: blinker 1s linear 10;\r\n}\r\n.shake_me {\r\n  animation: shake 0.5s linear 10;\r\n}\r\n\r\n@keyframes blinker {\r\n  50% {\r\n    opacity: 0;\r\n    background-color: lime;\r\n    color: white;\r\n  }\r\n}\r\n\r\n@keyframes shake {\r\n  10%,\r\n  90% {\r\n    transform: translate3d(-1px, 0, 0);\r\n  }\r\n\r\n  20%,\r\n  80% {\r\n    transform: translate3d(2px, 0, 0);\r\n  }\r\n\r\n  30%,\r\n  50%,\r\n  70% {\r\n    transform: translate3d(-3px, 0, 0);\r\n  }\r\n\r\n  40%,\r\n  60% {\r\n    transform: translate3d(3px, 0, 0);\r\n  }\r\n}\r\n\r\n@keyframes vibra {\r\n  0% {\r\n    transform: rotate(-10deg);\r\n  }\r\n  15% {\r\n    transform: rotate(10deg);\r\n  }\r\n  30% {\r\n    transform: rotate(-10deg);\r\n  }\r\n  45% {\r\n    transform: rotate(10deg);\r\n  }\r\n  60% {\r\n    transform: rotate(-10deg);\r\n  }\r\n  75% {\r\n    transform: rotate(10deg);\r\n  }\r\n  90% {\r\n    transform: rotate(-10deg);\r\n  }\r\n  100% {\r\n    transform: rotate(10deg);\r\n  }\r\n}\r\n</style>\r\n"]}]}