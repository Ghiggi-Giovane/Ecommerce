" v-b-toggle=\"[`erro-${i}`]\">\r\n\r\n                <div class=\"d-flex w-100 justify-content-between\">\r\n                    <h5 class=\"mb-1 text-light\">{{ e.message }}</h5>\r\n                    <small>{{ e.timestamp | datetime }}</small>\r\n                </div>\r\n\r\n                <div class=\"d-flex justify-content-end\">\r\n                    <b-badge variant=\"danger\" class=\"p-2\">Clique para mais detalhes</b-badge>\r\n                    <b-badge variant=\"danger\" class=\"p-2 ml-2\" @click=\"btnExcluir($event, e.id)\"><b-icon-trash/></b-badge>\r\n                </div>\r\n\r\n                <b-collapse :id=\"`erro-${i}`\" class=\"mt-2\">\r\n                    <b-card class=\"panel\">\r\n                        <b-table-simple small caption-top stacked>\r\n                            <colgroup><col><col></colgroup>\r\n                            <b-tbody>\r\n                            <b-tr>\r\n                                <b-td stacked-heading=\"id:\">{{ e.id }}</b-td>\r\n                                <b-td stacked-heading=\"Time:\">{{ e.timestamp | datetime }}</b-td>\r\n                                <b-td stacked-heading=\"Where:\">{{ e.info }}</b-td>\r\n                                <b-td stacked-heading=\"Message:\">{{ e.message }}</b-td>\r\n                                <b-td stacked-heading=\"Rota:\">{{ e.route }}</b-td>\r\n                                <!--\r\n                                <b-td stacked-heading=\"Status:\">{{ e.error.error.response.status }}</b-td>\r\n                                <b-td stacked-heading=\"Status Text:\">{{ e.error.error.response.statusText }}</b-td>\r\n                                <b-td stacked-heading=\"Request:\">{{ e.error.error.config.method.toUpperCase() }}</b-td>\r\n                                <b-td stacked-heading=\"Accept:\">{{ e.error.error.response.config.headers.Accept }}</b-td>\r\n                                <b-td stacked-heading=\"Route:\">{{ e.error.error.response.config.url }}</b-td>\r\n\r\n                                <b-td stacked-heading=\"Route:\">{{ e.error.error.config.url }}</b-td>\r\n                                <b-td stacked-heading=\"Method:\">{{ e.error.error.config.method.toString().toUpperCase() }}</b-td>\r\n                                <b-td stacked-heading=\"Autor:\">{{ e.error.error.config.headers.Autor }}</b-td>\r\n                                <b-td stacked-heading=\"Nome:\">{{ JSON.parse(e.error.error.config.headers.Autor).nome }}</b-td>\r\n                                -->\r\n\r\n                                <b-td style=\"text-align: right\">\r\n                                    <b-button variant='danger' @click=\"btnExcluir($event, e.id)\">Excluir</b-button>\r\n                                </b-td>\r\n                            </b-tr>\r\n                            </b-tbody>\r\n                        </b-table-simple>\r\n                    </b-card>\r\n                </b-collapse>\r\n\r\n            </b-list-group-item>      \r\n  \r\n        </b-list-group>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport JumboTron from '../../../components/JumboTron'\r\nexport default {\r\n\r\n    name: 'Error',\r\n\r\n    components: {\r\n\r\n        jumbotron: JumboTron\r\n\r\n    },\r\n\r\n    props: {\r\n\r\n        errorlist: {\r\n            type: Array,\r\n            required: true\r\n        }\r\n        \r\n    }, \r\n\r\n    methods: {\r\n        \r\n        btnExcluir( e, id ) {\r\n            this.$emit('evt_errorbtnexcluir', id)\r\n            e.preventDefault()\r\n        }\r\n\r\n    },\r\n\r\n    computed: {\r\n\r\n        errorlistOrdered() {\r\n            return this.errorlist.sort((a, b) => new Date(b.timestamp) - new Date(a.timestamp))\r\n        },\r\n\r\n        temErros() {\r\n            return (this.errorlist.length>0)\r\n        }\r\n\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    h1 {\r\n        font-size: 74px;\r\n    }\r\n    p {\r\n        font-size: 36px;\r\n        text-shadow: 2px 2px silver;\r\n    }\r\n    .panel {\r\n        background-color: rgba(255, 255, 255, .6);\r\n        color: black;\r\n        text-shadow: blanchedalmond;\r\n        border-radius: 5px;\r\n        padding: 20px;\r\n    }\r\n</style>"]}]}