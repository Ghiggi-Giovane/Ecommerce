456', '7895155123456', '789456789123']\r\n        // {7895155123456: ['7895155123456', '7895155123456'], 789456789123: ['789456789123']}\r\n        const eansgroup = state.eans.reduce((groups, ean) => {\r\n            if (!groups[ean]) {\r\n                groups[ean] = [];\r\n            }\r\n            groups[ean].push(ean);\r\n            return groups;\r\n        }, {}\r\n        );\r\n        \r\n        // [{ean: '7895155123456', reads: 2}, {ean: '789456789123', reads: 1}]\r\n        return Object.keys(eansgroup).map(ean => { return { ean: ean, reads: eansgroup[ean].length } })\r\n    },\r\n    \r\n    gtxeans128reads: (state, getters) => getters.gtxeansreads.map(ean => getters.gtxeans128byean(ean.ean) ),\r\n\r\n    gtxeansreadsbyean: (state, getters) => (ean) => getters.gtxeansreads.find(f => f.ean == ean) || {ean: ''},\r\n    \r\n    gtxeans128readsbyean: (state, getters) => (ean) => getters.gtxeans128byean(getters.gtxeansreadsbyean(ean).ean),\r\n\r\n    gtxeanslidos: (state, getters) => getters.gtxeansreads.length,\r\n\r\n    // ean historico ...\r\n    gtxeanshist: state => state.eanshist,\r\n    gtxeanshistlidos: (state, getters) => getters.gtxeanshist.length,\r\n\r\n    // nÃ£o usa dados da store ...\r\n    gtxeantype: (state) => (ean) => {\r\n\r\n        let eantype = {ean: ean, type: 'undefined', unique: true}\r\n\r\n        if (((ean.substr(0, 3) == '010') && (ean.substr(16, 2) == '10')) ||\r\n            ((ean.substr(0, 5) == '80060') && (ean.substr(22, 2) == '10')))\r\n            eantype = { ean: ean, type: 'ean128', unique: (ean.substr(0, 3) == '010') }\r\n        \r\n        return eantype\r\n\r\n    },\r\n\r\n    gtxeans128byean: (state, getters) => (e) => {\r\n\r\n        let ean = { ean: e }\r\n\r\n        if ((e) && getters.gtxeantype(e).type == 'ean128') {\r\n            let i = 0\r\n            let fields = {}\r\n            let ean128 = {}\r\n            \r\n            if (state.ean128.reduce((a, b) => a + b.tamanho, 0) > 0) {\r\n                if (getters.gtxeantype(e).unique) {\r\n                    i = 18\r\n                    //if (e.substr(0, 3) == '010') {\r\n                    fields = { ean13: e.substr(3, 13), vol: '01', vols: '01', caixa: 'A' }\r\n                    state.ean128.map(m => {\r\n                        ean128 = { ...ean128, ...{ [m.field]: e.substr(i, m.tamanho), reads: 1 } }\r\n                        i += m.tamanho\r\n                    })\r\n                } else {\r\n                    // if (e.substr(0, 5) == '80060') {s\r\n                    i = 24\r\n                    fields = { ean13: e.substr(5, 13), vol: e.substr(18, 2), vols: e.substr(20, 2), caixa: getters.gtxvolumeascii(e.substr(18, 2)) }\r\n                    state.ean128.map(m => {\r\n                        ean128 = { ...ean128, ...{ [m.field]: e.substr(i, m.tamanho), reads: 1 } }\r\n                        i += m.tamanho\r\n                    })\r\n                }\r\n    \r\n                ean = { ...ean, ...fields, ...ean128 }\r\n            }\r\n        }\r\n\r\n        return ean\r\n    },\r\n\r\n    gtxvolumeascii: state => vol => String.fromCharCode(parseInt(vol) + 64),\r\n\r\n    gtxnewid: state => '_' + Math.random().toString(36).substr(2, 13)\r\n\r\n}\r\n\r\nconst mutations = {\r\n\r\n    seteans: (state, ean) => state.eans.push(ean),\r\n\r\n    seteansclear: state => state.eans = [],\r\n\r\n    seteansclearean: (state, eans) => state.eans = eans,\r\n\r\n    seteanssave: (state, eanshist) => state.eanshist.push(eanshist),\r\n    \r\n    seteandelete: (state, ean) => {\r\n\r\n        const index = state.eans.findIndex(e => e == ean.ean)\r\n        if (index > -1) state.eans.splice(index, 1)\r\n        \r\n    },\r\n\r\n    seteanhistdelete: (state, id) => {\r\n\r\n        const index = state.eanshist.findIndex(e => e.id == id)\r\n        if (index > -1) state.eanshist.splice(index, 1)\r\n        \r\n    },\r\n\r\n    seteanhisteandelete: (state, ean) => {\r\n\r\n        const list = state.eanshist.find(e => e.id == ean.id)\r\n        let index = list.eans128reads.findIndex(e => e.ean == ean.ean)\r\n        if (index > -1) list.eans128reads.splice(index, 1)\r\n        index = list.eansreads.findIndex(e => e.ean == ean.ean)\r\n        if (index > -1) list.eansreads.splice(index, 1)\r\n        list.eanslidos = list.eansreads.length\r\n\r\n        if (list.eanslidos.length == 0) {\r\n            state.eanshist.find(e => e.id == ean.id).splice(0,1)\r\n        }\r\n        \r\n    }\r\n\r\n}\r\n\r\nconst actions = {\r\n\r\n    // 0107895155354473101008510001208\r\n    // 8006078951558966520104100963830653908\r\n    // 8006078951558966520204100963830654008\r\n    // 8006078951558966520304100963830654108\r\n    // 8006078951558966520404100963830654208\r\n    // 01078951553544731010085100012088006078951558966520104100963830653908800607895155896652020410096383065400880060789515589665203041009638306541088006078951558966520404100963830654208\r\n\r\n    acteans: ({ commit }, eans) => {\r\n\r\n        if (eans.length > 0) {\r\n\r\n            if (eans.includes('\\n')) {\r\n                eans.split('\\n')\r\n                    .filter(e => e !== '') // novo array somente com os eans, sem os enters no final\r\n                    .map(e => commit('seteans', e))\r\n            } else {\r\n                commit('seteans', eans)\r\n            }\r\n        \r\n        }\r\n\r\n    },\r\n\r\n    acteansclear: ({ commit }) => {\r\n\r\n        commit('seteansclear')\r\n        \r\n    },\r\n\r\n    acteansdeletebyean: ({ commit }, ean) => {\r\n\r\n        commit('seteandelete', ean)\r\n\r\n    },\r\n\r\n    acteanshistclear: ({ commit }, id) => {\r\n\r\n        console.log('acteanshistclear', id)\r\n\r\n        commit('seteanhistdelete', id)\r\n        \r\n    },\r\n\r\n    acteanshistdeletebyean: ({ commit }, ean) => {\r\n\r\n        commit('seteanhisteandelete', ean)\r\n\r\n    },\r\n\r\n    acteanssave: ({ commit, getters }) => {\r\n\r\n        const eanshist = {\r\n            id: '_' + Math.random().toString(36).substr(2, 13),\r\n            envio: new Date(),\r\n            eans: getters.eans,\r\n            eanslidos: getters.gtxeanslidos,\r\n            eansreads: getters.gtxeansreads,\r\n            eans128reads: getters.gtxeans128reads\r\n        }\r\n        \r\n        commit('seteanssave', eanshist)\r\n\r\n        commit('seteansclear')\r\n\r\n    }\r\n}\r\n\r\nexport default { state, mutations, getters, actions }\r\n"],"mappings":";AAAA,MAAMA,KAAK,GAAG;EAEVC,IAAI,EAAE,EAFI;EAGVC,QAAQ,EAAE,EAHA;EAIVC,MAAM,EAAE,CACJ;IAACC,KAAK,EAAE,MAAR;IAAgBC,OAAO,EAAE;EAAzB,CADI,EACyB;EAC7B;IAACD,KAAK,EAAE,KAAR;IAAeC,OAAO,EAAE;EAAxB,CAFI,EAEyB;EAC7B;IAACD,KAAK,EAAE,KAAR;IAAeC,OAAO,EAAE;EAAxB,CAHI,CAGyB;EAHzB;AAJE,CAAd;AAYA,MAAMC,OAAO,GAAG;EAEZC,OAAO,EAAEP,KAAK,IAAIA,KAAK,CAACC,IAFZ;EAIZ;EACAO,YAAY,EAAER,KAAK,IAAI;IACnB;IACA;IACA;IACA,MAAMS,SAAS,GAAGT,KAAK,CAACC,IAAN,CAAWS,MAAX,CAAkB,CAACC,MAAD,EAASC,GAAT,KAAiB;MACjD,IAAI,CAACD,MAAM,CAACC,GAAD,CAAX,EAAkB;QACdD,MAAM,CAACC,GAAD,CAAN,GAAc,EAAd;MACH;;MACDD,MAAM,CAACC,GAAD,CAAN,CAAYC,IAAZ,CAAiBD,GAAjB;MACA,OAAOD,MAAP;IACH,CANiB,EAMf,EANe,CAAlB,CAJmB,CAanB;;IACA,OAAOG,MAAM,CAACC,IAAP,CAAYN,SAAZ,EAAuBO,GAAvB,CAA2BJ,GAAG,IAAI;MAAE,OAAO;QAAEA,GAAG,EAAEA,GAAP;QAAYK,KAAK,EAAER,SAAS,CAACG,GAAD,CAAT,CAAeM;MAAlC,CAAP;IAAmD,CAAvF,CAAP;EACH,CApBW;EAsBZC,eAAe,EAAE,CAACnB,KAAD,EAAQM,OAAR,KAAoBA,OAAO,CAACE,YAAR,CAAqBQ,GAArB,CAAyBJ,GAAG,IAAIN,OAAO,CAACc,eAAR,CAAwBR,GAAG,CAACA,GAA5B,CAAhC,CAtBzB;EAwBZS,iBAAiB,EAAE,CAACrB,KAAD,EAAQM,OAAR,KAAqBM,GAAD,IAASN,OAAO,CAACE,YAAR,CAAqBc,IAArB,CAA0BC,CAAC,IAAIA,CAAC,CAACX,GAAF,IAASA,GAAxC,KAAgD;IAACA,GAAG,EAAE;EAAN,CAxBpF;EA0BZY,oBAAoB,EAAE,CAACxB,KAAD,EAAQM,OAAR,KAAqBM,GAAD,IAASN,OAAO,CAACc,eAAR,CAAwBd,OAAO,CAACe,iBAAR,CAA0BT,GAA1B,EAA+BA,GAAvD,CA1BvC;EA4BZa,YAAY,EAAE,CAACzB,KAAD,EAAQM,OAAR,KAAoBA,OAAO,CAACE,YAAR,CAAqBU,MA5B3C;EA8BZ;EACAQ,WAAW,EAAE1B,KAAK,IAAIA,KAAK,CAACE,QA/BhB;EAgCZyB,gBAAgB,EAAE,CAAC3B,KAAD,EAAQM,OAAR,KAAoBA,OAAO,CAACoB,WAAR,CAAoBR,MAhC9C;E