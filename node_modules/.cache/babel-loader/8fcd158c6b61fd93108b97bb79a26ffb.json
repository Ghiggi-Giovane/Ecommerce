","tempTakenPhotos","clickedPhoto","props","idcarregamentoinspecao","type","required","computed","salvarfotos","created","methods","openFilePicker","inputElement","handleFileSelection","filePromises","fileReader","resolve","Promise","base64Array","photoId","photoUrl","idpergunta","idCarregamento","CarregamentoSalvarFiles","API","limpar_lista","toggleCamera","audio","video","facingMode","getUserMedia","then","catch","stopCameraStream","tracks","track","takePhoto","setTimeout","canvas","context","deletePhoto","photo"],"sourceRoot":"src/components","sources":["CameraCapture.vue"],"sourcesContent":["<template>\r\n  <b-row>\r\n    <b-col cols=\"12\" sm=\"2\"> </b-col>\r\n    <b-col cols=\"12\" sm=\"8\" class=\"web-camera-container\">\r\n      <b-col class=\"camera-button\">\r\n        <button\r\n          type=\"button\"\r\n          class=\"button-78\"\r\n          :class=\"{ 'is-primary': !isCameraOpen, 'is-danger': isCameraOpen }\"\r\n          @click=\"toggleCamera\"\r\n        >\r\n          <span v-if=\"!isCameraOpen\">Abrir Camera</span>\r\n          <span v-else @click=\"stopCameraStream()\">Fechar Camera</span>\r\n        </button>\r\n        <b-button\r\n          size=\"md\"\r\n          variant=\"primary\"\r\n          type=\"button\"\r\n          class=\"button-78\"\r\n          @click=\"openFilePicker\"\r\n        >\r\n          <b-icon size=\"sm\" icon=\"images\" aria-label=\"gallery\"></b-icon>\r\n          Selecionar Fotos\r\n        </b-button>\r\n      </b-col>\r\n      <b-col\r\n        v-if=\"isCameraOpen\"\r\n        v-show=\"!isLoading\"\r\n        class=\"camera-box\"\r\n        :class=\"{ flash: isShotPhoto }\"\r\n      >\r\n        <b-col class=\"camera-shutter\" :class=\"{ flash: isShotPhoto }\"></b-col>\r\n\r\n        <video\r\n          v-show=\"!isPhotoTaken\"\r\n          ref=\"camera\"\r\n          webkit-playsinline\r\n          playsinline\r\n          autoplay\r\n          :width=\"350\"\r\n          :height=\"450\"\r\n          class=\"layout-camera-celular\"\r\n        ></video>\r\n\r\n        <canvas\r\n          v-show=\"isPhotoTaken\"\r\n          id=\"photoTaken\"\r\n          ref=\"canvas\"\r\n          :width=\"350\"\r\n          :height=\"450\"\r\n          class=\"layout-camera-celular\"\r\n        ></canvas>\r\n      </b-col>\r\n\r\n      <b-col v-if=\"isCameraOpen && !isLoading\" class=\"camera-shoot\">\r\n        <b-button\r\n          size=\"md\"\r\n          variant=\"primary\"\r\n          type=\"button\"\r\n          class=\"button-78\"\r\n          @click=\"takePhoto\"\r\n        >\r\n          <b-icon\r\n            :icon=\"isPhotoTaken ? 'plus-circle-fill' : 'camera-fill'\"\r\n            aria-label=\"camera\"\r\n            size=\"14px\"\r\n          ></b-icon>\r\n        </b-button>\r\n      </b-col>\r\n    </b-col>\r\n\r\n    <b-col cols=\"12\" sm=\"2\"> </b-col>\r\n\r\n    <b-col cols=\"12\" sm=\"2\"> </b-col>\r\n    <b-col cols=\"12\" sm=\"8\" class=\"fotos-temporarias-style\">\r\n      <b-col v-if=\"tempTakenPhotos.length > 0\" class=\"temp-taken-photos\">\r\n        <h2>Fotos temporárias</h2>\r\n        <b-button\r\n          size=\"md\"\r\n          variant=\"primary\"\r\n          type=\"button\"\r\n          class=\"button-78\"\r\n          @click=\"CarregamentoSalvarFiles()\"\r\n        >\r\n          <b-icon\r\n            size=\"sm\"\r\n            icon=\"cloud-upload-fill\"\r\n            aria-label=\"trash\"\r\n          ></b-icon>\r\n          Upload das fotos\r\n        </b-button>\r\n\r\n        <ul>\r\n          <li v-for=\"t in tempTakenPhotos\" :key=\"t.photoId\">\r\n            <img :src=\"t.photoUrl\" alt=\"Taken Photo\" @click=\"showPhoto(t)\" />\r\n            <b-button size=\"sm\" variant=\"danger\" class=\"ml-2\">\r\n              <b-icon\r\n                size=\"sm\"\r\n                icon=\"trash-fill\"\r\n                @click=\"deletePhoto(t.photoId)\"\r\n                aria-label=\"trash\"\r\n              ></b-icon>\r\n            </b-button>\r\n          </li>\r\n        </ul>\r\n      </b-col>\r\n    </b-col>\r\n    <b-col cols=\"12\" sm=\"2\"> </b-col>\r\n  </b-row>\r\n</template>\r\n\r\n<script>\r\nimport API from \"../services/api.service\";\r\n\r\nexport default {\r\n  name: \"cameracapture\",\r\n  data() {\r\n    return {\r\n      isCameraOpen: false,\r\n      isPhotoTaken: false,\r\n      isShotPhoto: false,\r\n      isLoading: false,\r\n      link: \"#\",\r\n      tempTakenPhotos: [],\r\n      clickedPhoto: null,\r\n    };\r\n  },\r\n\r\n  props: {\r\n    idcarregamentoinspecao: {\r\n      type: {\r\n        type: Number,\r\n        required: true,\r\n      },\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    salvarfotos() {\r\n      return {\r\n        idcarregamentoinspecao: this.idcarregamentoinspecao,\r\n        tempTakenPhotos: this.tempTakenPhotos,\r\n      };\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.createCameraElement();\r\n  },\r\n\r\n  methods: {\r\n    openFilePicker() {\r\n      const inputElement = document.createElement(\"input\");\r\n      inputElement.type = \"file\";\r\n      inputElement.multiple = true;\r\n      inputElement.accept = \"image/*\";\r\n\r\n      inputElement.addEventListener(\"change\", this.handleFileSelection);\r\n\r\n      inputElement.click();\r\n    },\r\n\r\n    handleFileSelection(event, idCarregamento) {\r\n      const selectedFiles = event.target.files;\r\n      const filePromises = [];\r\n\r\n      for (let i = 0; i < selectedFiles.length; i++) {\r\n        const file = selectedFiles[i];\r\n        const fileReader = new FileReader();\r\n\r\n        filePromises.push(\r\n          new Promise((resolve) => {\r\n            fileReader.onload = (event) => {\r\n              const photoUrl = event.target.result;\r\n              resolve(photoUrl);\r\n            };\r\n            fileReader.readAsDataURL(file);\r\n          })\r\n        );\r\n      }\r\n      Promise.all(filePromises).then((base64Array) => {\r\n        const imageUrl = URL.createObjectURL(selectedFiles[0]);\r\n\r\n        const photoId = this.tempTakenPhotos.length + 1;\r\n        const idpergunta = this.idpergunta;\r\n\r\n        base64Array.forEach((photoUrl) => {\r\n          const base64String = photoUrl.replace(\r\n            \"image/jpeg;\",\r\n            \"image/jpeg;quality=0.6;\"\r\n          );\r\n\r\n          const photo = {\r\n            photoId: photoId,\r\n            photoUrl: photoUrl,\r\n            idpergunta: idpergunta,\r\n            idCarregamento: idCarregamento,\r\n          };\r\n\r\n          this.tempTakenPhotos.push(photo);\r\n        });\r\n        this.stopCameraStream();\r\n\r\n        this.CarregamentoSalvarFiles();\r\n      });\r\n    },\r\n    CarregamentoSalvarFiles() {\r\n      API.post(\r\n        \"v2/Servicos/InspecaoCarregamentoFiles/Post\",\r\n        this.salvarfotos\r\n      ).then((api) => {\r\n        if (api.data.Success == \"True\" || api.data.Success) {\r\n          this.$root.toastsuccess(\"Upload da fotos realizado com sucesso!\");\r\n          this.limpar_lista();\r\n        } else {\r\n          this.$root.toastdanger(api.data.Error);\r\n        }\r\n      });\r\n    },\r\n    limpar_lista() {\r\n      this.tempTakenPhotos = [];\r\n    },\r\n\r\n    toggleCamera() {\r\n      if (this.isCameraOpen) {\r\n        this.isCameraOpen = false;\r\n        this.isPhotoTaken = false;\r\n        this.isShotPhoto = false;\r\n        this.stopCameraStream();\r\n      } else {\r\n        this.isCameraOpen = true;\r\n        this.createCameraElement();\r\n      }\r\n    },\r\n\r\n    async createCameraElement() {\r\n      // this.isLoading = true;\r\n\r\n      const constraints = {\r\n        audio: false,\r\n        video: {\r\n          facingMode: \"environment\",\r\n        },\r\n      };\r\n\r\n      await navigator.mediaDevices\r\n        .getUserMedia(constraints)\r\n        .then((stream) => {\r\n          1;\r\n          this.isLoading = false;\r\n          this.$refs.camera.srcObject = stream;\r\n        })\r\n        .catch((error) => {\r\n          this.isLoading = false;\r\n        });\r\n    },\r\n\r\n    stopCameraStream() {\r\n      if (this.$refs.camera.srcObject) {\r\n        let tracks = this.$refs.camera.srcObject.getTracks();\r\n\r\n        tracks.forEach((track) => {\r\n          track.stop();\r\n        });\r\n      }\r\n    },\r\n\r\n    takePhoto() {\r\n      //  let index = 0;\r\n      if (!this.isPhotoTaken) {\r\n        this.isShotPhoto = true;\r\n\r\n        const FLASH_TIMEOUT = 50;\r\n\r\n        setTimeout(() => {\r\n          this.isShotPhoto = false;\r\n        }, FLASH_TIMEOUT);\r\n      }\r\n      this.isPhotoTaken = !this.isPhotoTaken;\r\n\r\n      const video = this.$refs.camera;\r\n      const canvas = this.$refs.canvas;\r\n      const context = canvas.getContext(\"2d\");\r\n\r\n      // Defina manualmente a largura e altura do canvas\r\n      const canvasWidth = 350;\r\n      const canvasHeight = 450;\r\n\r\n      canvas.width = canvasWidth;\r\n      canvas.height = canvasHeight;\r\n\r\n      context.drawImage(video, 0, 0, canvasWidth, canvasHeight);\r\n\r\n      // Adicione a foto à lista temporária\r\n      if (this.isShotPhoto) {\r\n        const photoUrl = canvas.toDataURL(\"image/jpeg\");\r\n        const photoId = this.tempTakenPhotos.length + 1;\r\n\r\n        const photo = {\r\n          photoId: photoId,\r\n          photoUrl: photoUrl,\r\n        };\r\n\r\n        this.tempTakenPhotos.push(photo);\r\n      }\r\n    },\r\n    deletePhoto(photoId) {\r\n      const index = this.tempTakenPhotos.findIndex(\r\n        (photo) => photo.photoId === photoId\r\n      );\r\n      if (index !== -1) {\r\n        this.tempTakenPhotos.splice(index, 1);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.web-camera-container {\r\n  margin-top: 2rem;\r\n  margin-bottom: 2rem;\r\n\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  border: 2px solid black;\r\n  border-radius: 4px;\r\n}\r\n.web-camera-container .camera-button {\r\n  margin-bottom: 2rem;\r\n  text-align: center;\r\n}\r\n\r\n.web-camera-container .camera-box .camera-shutter {\r\n  opacity: 0;\r\n  width: 350px;\r\n  height: 450px;\r\n  background-color: #fff;\r\n  position: absolute;\r\n}\r\n.web-camera-container .camera-box .camera-shutter.flash {\r\n  opacity: 1;\r\n}\r\n.web-camera-container .camera-shoot {\r\n  margin: 1rem 0;\r\n  text-align: -webkit-center;\r\n}\r\n.web-camera-container .camera-shoot button {\r\n  height: 60px;\r\n  width: 60px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  border-radius: 100%;\r\n}\r\n.web-camera-container .camera-shoot button img {\r\n  height: 35px;\r\n  object-fit: cover;\r\n}\r\n.web-camera-container .camera-loading {\r\n  overflow: hidden;\r\n  height: 100%;\r\n  position: absolute;\r\n  width: 100%;\r\n\r\n  margin: 3rem 0 0 -1.2rem;\r\n}\r\n.web-camera-container .camera-loading ul {\r\n  height: 100%;\r\n  position: absolute;\r\n  width: 100%;\r\n  z-index: 999999;\r\n  margin: 0;\r\n}\r\n.web-camera-container .camera-loading .loader-circle {\r\n  display: block;\r\n  height: 14px;\r\n  margin: 0 auto;\r\n  top: 50%;\r\n  left: 100%;\r\n  transform: translateY(-50%);\r\n  transform: translateX(-50%);\r\n  position: absolute;\r\n  width: 100%;\r\n  padding: 0;\r\n}\r\n.web-camera-container .camera-loading .loader-circle li {\r\n  display: block;\r\n  float: left;\r\n  width: 10px;\r\n  height: 10px;\r\n  line-height: 10px;\r\n  padding: 0;\r\n  position: relative;\r\n  margin: 0 0 0 4px;\r\n  background: #999;\r\n  animation: preload 1s infinite;\r\n  top: -50%;\r\n  border-radius: 100%;\r\n}\r\n.web-camera-container .camera-loading .loader-circle li:nth-child(2) {\r\n  animation-delay: 0.2s;\r\n}\r\n.web-camera-container .camera-loading .loader-circle li:nth-child(3) {\r\n  animation-delay: 0.4s;\r\n}\r\n\r\n.taken-photos {\r\n  margin-top: 2rem;\r\n}\r\n.temp-taken-photos {\r\n  margin-top: 2rem;\r\n}\r\n\r\n.temp-taken-photos h2 {\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.temp-taken-photos ul {\r\n  list-style: none;\r\n  padding: 0;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: space-around;\r\n}\r\n\r\n.temp-taken-photos li {\r\n  margin: 0.5rem;\r\n}\r\n\r\n.temp-taken-photos img {\r\n  width: 202px;\r\n  height: 266px;\r\n  object-fit: cover;\r\n}\r\n.fotos-temporarias-style {\r\n  text-align: center;\r\n}\r\n/* CSS */\r\n.button-78 {\r\n  align-items: center;\r\n  appearance: none;\r\n  background-clip: padding-box;\r\n  background-color: initial;\r\n  background-image: none;\r\n  border-style: none;\r\n  box-sizing: border-box;\r\n  color: #fff;\r\n  cursor: pointer;\r\n  display: inline-block;\r\n  flex-direction: row;\r\n  flex-shrink: 0;\r\n  font-family: Eina01, sans-serif;\r\n  font-size: 15px;\r\n  font-weight: 800;\r\n  justify-content: center;\r\n  line-height: 24px;\r\n  margin: 0;\r\n  outline: none;\r\n  overflow: visible;\r\n  padding: 13px 24px;\r\n  pointer-events: auto;\r\n  position: relative;\r\n  text-align: center;\r\n  text-decoration: none;\r\n  text-transform: none;\r\n  user-select: none;\r\n  -webkit-user-select: none;\r\n  touch-action: manipulation;\r\n  vertical-align: middle;\r\n  width: auto;\r\n  word-break: keep-all;\r\n  z-index: 0;\r\n}\r\n\r\n.button-78:before,\r\n.button-78:after {\r\n  border-radius: 80px;\r\n}\r\n\r\n.button-78:before {\r\n  background-image: linear-gradient(\r\n    92.83deg,\r\n    rgba(156, 16, 6) 0,\r\n    rgba(156, 16, 6) 100%\r\n  );\r\n  content: \"\";\r\n  display: block;\r\n  height: 100%;\r\n  left: 0;\r\n  overflow: hidden;\r\n  position: absolute;\r\n  top: 0;\r\n  width: 100%;\r\n  z-index: -2;\r\n}\r\n\r\n.button-78:after {\r\n  background-color: initial;\r\n  background-image: linear-gradient(#541a0f 0, #0c0d0d 100%);\r\n  bottom: 4px;\r\n  content: \"\";\r\n  display: block;\r\n  left: 4px;\r\n  overflow: hidden;\r\n  position: absolute;\r\n  right: 4px;\r\n  top: 4px;\r\n  transition: all 100ms ease-out;\r\n  z-index: -1;\r\n}\r\n\r\n.button-78:hover:not(:disabled):before {\r\n  background: linear-gradient(\r\n    92.83deg,\r\n    rgba(156, 16, 6) 0%,\r\n    rgba(156, 16, 6) 100%\r\n  );\r\n}\r\n\r\n.button-78:hover:not(:disabled):after {\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  top: 0;\r\n  transition-timing-function: ease-in;\r\n  opacity: 0;\r\n}\r\n\r\n.button-78:active:not(:disabled) {\r\n  color: #ccc;\r\n}\r\n\r\n.button-78:active:not(:disabled):before {\r\n  background-image: linear-gradient(\r\n      0deg,\r\n      rgba(0, 0, 0, 0.2),\r\n      rgba(0, 0, 0, 0.2)\r\n    ),\r\n    linear-gradient(92.83deg, rgba(156, 16, 6) 0, rgba(156, 16, 6) 100%);\r\n}\r\n\r\n.button-78:active:not(:disabled):after {\r\n  background-image: linear-gradient(#541a0f 0, #0c0d0d 100%);\r\n  bottom: 4px;\r\n  left: 4px;\r\n  right: 4px;\r\n  top: 4px;\r\n}\r\n\r\n.button-78:disabled {\r\n  cursor: default;\r\n  opacity: 0.24;\r\n}\r\n\r\n@media (max-width: 320px) {\r\n  .web-camera-container {\r\n    margin-top: 2rem;\r\n    margin-bottom: 2rem;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    border: 2px solid #000;\r\n    border-radius: 4px;\r\n    max-width: 294px;\r\n    margin-left: -9px;\r\n  }\r\n  .web-camera-container .camera-box .camera-shutter {\r\n    opacity: 0;\r\n    width: 252px;\r\n    height: 448.5px;\r\n    background-color: #fff;\r\n    position: absolute;\r\n    margin-left: -18px;\r\n  }\r\n  h2 {\r\n    font-size: 25px;\r\n    text-align: center;\r\n    color: black;\r\n    font-family: \"Din Pro Regular\";\r\n  }\r\n  .button-78 {\r\n    align-items: center;\r\n    appearance: none;\r\n    background-clip: padding-box;\r\n    background-color: initial;\r\n    background-image: none;\r\n    border-style: none;\r\n    box-sizing: border-box;\r\n    color: #fff;\r\n    cursor: pointer;\r\n    display: inline-block;\r\n    flex-direction: row;\r\n    flex-shrink: 0;\r\n    font-family: Eina01, sans-serif;\r\n    font-size: 12px;\r\n    font-weight: 800;\r\n    justify-content: center;\r\n    line-height: 24px;\r\n    margin: 0;\r\n    outline: none;\r\n    overflow: visible;\r\n    padding: 5px 13px;\r\n    pointer-events: auto;\r\n    position: relative;\r\n    text-align: center;\r\n    text-decoration: none;\r\n    text-transform: none;\r\n    user-select: none;\r\n    -webkit-user-select: none;\r\n    touch-action: manipulation;\r\n    vertical-align: middle;\r\n    width: auto;\r\n    word-break: keep-all;\r\n    z-index: 0;\r\n  }\r\n  .video-class {\r\n    border: 2px solid black;\r\n    height: 296px;\r\n    width: 291px;\r\n    margin-left: -31px;\r\n  }\r\n  .layout-camera-celular {\r\n    margin-left: -21px !important;\r\n    width: 262px;\r\n  }\r\n}\r\n\r\n@keyframes preload {\r\n  0% {\r\n    opacity: 1;\r\n  }\r\n  50% {\r\n    opacity: 0.4;\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n  }\r\n}\r\n</style>\r\n"]}]}