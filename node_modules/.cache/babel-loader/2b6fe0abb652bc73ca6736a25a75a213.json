"isBusy","img","created","computed","produtosTotaisCount","methods","carregaImagem","abrirImagem","props","src","center","fluid","buttonSize","centered","size","buscarProdutos","ApiService","result","onRowSelected","ImagensSave","Imagens","then","catch","e","ReenviarImagens","params","Referencia","save","btSalvarPrecos","btSalvarMedidas","err","activeTab","referencia"],"sourceRoot":"src/pages/ecommerce/views","sources":["Produtos.vue"],"sourcesContent":["<template>\r\n  <div id=\"divPrincipal\">\r\n    <b-card size=\"sm\">\r\n      <b-row>\r\n        <b-col cols=\"5\">\r\n          <div class=\"mb-2\" style=\"display: flex\">\r\n            <!-- <b-dropdown right text=\"Visíveis\" size=\"sm\" class=\"mr-2\">\r\n              <b-dropdown-header id=\"dropdown-header-label\">Desmarque todos para não filtrar</b-dropdown-header>\r\n              <b-dropdown-form>\r\n                <b-form-checkbox\r\n                  style=\"width: 200px\"\r\n                  v-model=\"visivelMarketplace\"\r\n                  name=\"marketplace\"\r\n                  switch\r\n                >Visível Marketplace</b-form-checkbox>\r\n                <b-form-checkbox\r\n                  style=\"width: 200px\"\r\n                  v-model=\"visivelEcommerce\"\r\n                  name=\"ecommerce\"\r\n                  switch\r\n                >Visível E-commerce</b-form-checkbox>\r\n                <b-dropdown-item-button @click=\"filtroVisivel\">Filtro</b-dropdown-item-button>\r\n              </b-dropdown-form>\r\n            </b-dropdown>-->\r\n\r\n            <b-input-group prepend=\"Filtro\" size=\"sm\">\r\n              <b-form-input\r\n                id=\"input-filtro\"\r\n                type=\"search\"\r\n                v-model=\"filtro\"\r\n                placeholder=\"Filtre por Marca, Referencia, Linha de produto, Categoria, Descrição...\"\r\n              ></b-form-input>\r\n            </b-input-group>\r\n            <b-button class=\"ml-2\" size=\"sm\" @click=\"buscarProdutos\">Atualizar</b-button>\r\n          </div>\r\n          <!-- <div>\r\n            <span>\r\n              <strong>Total:</strong>\r\n            </span>\r\n            <span>{{produtosTotaisCount}}</span>\r\n          </div>-->\r\n          <b-table\r\n            responsive=\"sm\"\r\n            striped\r\n            bordered\r\n            hover\r\n            selectable\r\n            small\r\n            sticky-header=\"800px\"\r\n            select-mode=\"single\"\r\n            @row-selected=\"onRowSelected\"\r\n            :items=\"items\"\r\n            :fields=\"fields\"\r\n            :busy=\"isBusy\"\r\n            :filter=\"filtro\"\r\n            :filter-included-fields=\"filterOn\"\r\n          >\r\n            <template #cell(Nome)=\"data\">{{ data.item.Nome.substr(0,50).toUpperCase() }}</template>\r\n            <template\r\n              #cell(PrecosB2C)=\"data\"\r\n            >{{ data.item.Precos.filter(x => x.TabelaPreco == \"B2C\")[0].Valor | R$ }}</template>\r\n            <template\r\n              #cell(PrecosSite)=\"data\"\r\n            >{{ data.item.Precos.filter(x => x.TabelaPreco == \"SITE\")[0].Valor | R$ }}</template>\r\n          </b-table>\r\n        </b-col>\r\n        <b-col>\r\n          <b-card no-body>\r\n            <b-row>\r\n              <!-- <b-col class=\"m-1\">\r\n                Visibilidade:\r\n                <br />\r\n                <strong>Ecommerce: {{selected.VisivelEcommerce}}</strong>\r\n                <br />\r\n                <strong>Marketplace: {{selected.VisivelMarketplace}}</strong>\r\n              </b-col>-->\r\n              <b-col class=\"m-1\">\r\n                Referência:\r\n                <strong>{{selected.Referencia}}</strong>\r\n              </b-col>\r\n              <b-col class=\"m-1\">\r\n                <span>\r\n                  Marca:\r\n                  <strong>{{selected.MarcaFantasia}}</strong>\r\n                </span>\r\n              </b-col>\r\n              <b-col class=\"m-1\">\r\n                <span>\r\n                  Linha de Produto:\r\n                  <strong>{{selected.LinhaProduto}}</strong>\r\n                </span>\r\n              </b-col>\r\n            </b-row>\r\n\r\n            <hr />\r\n            <b-tabs content-class=\"mt-2\" fill v-model=\"tabAtivo\" @activate-tab=\"activeTab\">\r\n              <!-- //TODO E-COMMERCE -->\r\n              <b-tab title=\"E-commerce\" active class=\"m-2\">\r\n                <b-row>\r\n                  <b-col cols=\"2\">\r\n                    <label style=\"margin-top: 0px\">Referência</label>\r\n                    <b-input v-model=\"selected.Referencia\" disabled></b-input>\r\n                  </b-col>\r\n                  <b-col cols=\"8\">\r\n                    <div\r\n                      class=\"form-inline\"\r\n                      style=\"align-items: baseline;justify-content: space-between;\"\r\n                    >\r\n                      <label>Título</label>\r\n                      <label\r\n                        v-if=\"selected.NomeEcommerce\"\r\n                      >{{selected.NomeEcommerce.toString().length}}</label>\r\n                    </div>\r\n                    <b-input v-model=\"selected.NomeEcommerce\"></b-input>\r\n                  </b-col>\r\n                  <b-col cols=\"2\">\r\n                    <label style=\"margin-top: 0px\">Cor</label>\r\n                    <b-form-select v-model=\"selected.CorId\" :options=\"Cores\"></b-form-select>\r\n                  </b-col>\r\n                </b-row>\r\n                <b-row>\r\n                  <b-col>\r\n                    <div\r\n                      class=\"form-inline\"\r\n                      style=\"align-items: baseline;justify-content: space-between;\"\r\n                    >\r\n                      <label>Descrição</label>\r\n                      <label\r\n                        v-if=\"selected.DescricaoEcommerce\"\r\n                      >{{selected.DescricaoEcommerce.toString().length}}</label>\r\n                    </div>\r\n                    <b-form-textarea rows=\"4\" v-model=\"selected.DescricaoEcommerce\"></b-form-textarea>\r\n                  </b-col>\r\n                </b-row>\r\n                <b-row>\r\n                  <b-col cols=\"1\">\r\n                    <label>Visibilidade</label>\r\n                    <b-form-checkbox switch v-model=\"selected.VisivelEcommerce\" disabled size=\"sm\"></b-form-checkbox>\r\n                  </b-col>\r\n                  <b-col cols=\"5\">\r\n                    <label>Categoria</label>\r\n                    <b-form-select v-model=\"selected.CategoriaId\" :options=\"categorias\" size=\"sm\"></b-form-select>\r\n                  </b-col>\r\n                  <b-col cols=\"6\">\r\n                    <label>Vídeo URL</label>\r\n                    <b-link :href=\"selected.VideoUrl\" target=\"_blank\">\r\n                      <b-input v-model=\"selected.VideoUrl\"></b-input>\r\n                    </b-link>\r\n                  </b-col>\r\n                </b-row>\r\n                <b-row>\r\n                  <b-col>\r\n                    <div\r\n                      class=\"form-inline\"\r\n                      style=\"align-items: baseline;justify-content: space-between;\"\r\n                    >\r\n                      <label>Palavras Substitutas:</label>\r\n                      <label\r\n                        v-if=\"selected.PalavrasSubstitutas\"\r\n                      >{{selected.PalavrasSubstitutas.toString().length}}</label>\r\n                    </div>\r\n                    <b-form-textarea rows=\"2\" v-model=\"selected.PalavrasSubstitutas\"></b-form-textarea>\r\n                  </b-col>\r\n                </b-row>\r\n                <b-row>\r\n                  <b-col>\r\n                    <div\r\n                      class=\"form-inline\"\r\n                      style=\"align-items: baseline;justify-content: space-between;\"\r\n                    >\r\n                      <label>Meta Tag:</label>\r\n                      <label v-if=\"selected.MetaTag\">{{selected.MetaTag.toString().length}}</label>\r\n                    </div>\r\n                    <b-form-textarea rows=\"2\" v-model=\"selected.MetaTag\"></b-form-textarea>\r\n                  </b-col>\r\n                </b-row>\r\n                <hr />\r\n                <b-button v-b-modal.modal-2 id=\"but_salvar\" variant=\"success\" @click=\"save\">Salvar</b-button>\r\n              </b-tab>\r\n\r\n              <!-- //TODO MARKETPLACE -->\r\n              <b-tab title=\"Marketplace\" class=\"m-2\">\r\n                <b-row>\r\n                  <b-col cols=\"2\">\r\n                    <label style=\"margin-top: 0px\">Referência</label>\r\n                    <b-input v-model=\"selected.Referencia\" disabled></b-input>\r\n                  </b-col>\r\n                  <b-col cols=\"8\">\r\n                    <div\r\n                      class=\"form-inline\"\r\n                      style=\"align-items: baseline;justify-content: space-between;\"\r\n                    >\r\n                      <label>Título</label>\r\n                      <label\r\n                        v-if=\"selected.NomeMarketplace\"\r\n                      >{{selected.NomeMarketplace.toString().length}}</label>\r\n                    </div>\r\n                    <b-input v-model=\"selected.NomeMarketplace\"></b-input>\r\n                  </b-col>\r\n                  <b-col cols=\"2\">\r\n                    <label style=\"margin-top: 0px\">Cor</label>\r\n                    <b-form-select v-model=\"selected.CorId\" :options=\"Cores\"></b-form-select>\r\n                  </b-col>\r\n                </b-row>\r\n                <b-row>\r\n                  <b-col>\r\n                    <div\r\n                      class=\"form-inline\"\r\n                      style=\"align-items: baseline;justify-content: space-between;\"\r\n                    >\r\n                      <label>Descrição</label>\r\n                      <label\r\n                        v-if=\"selected.DescricaoMarketplace\"\r\n                      >{{selected.DescricaoMarketplace.toString().length}}</label>\r\n                    </div>\r\n                    <b-form-textarea rows=\"4\" v-model=\"selected.DescricaoMarketplace\"></b-form-textarea>\r\n                  </b-col>\r\n                </b-row>\r\n                <b-row>\r\n                  <b-col cols=\"1\">\r\n                    <label>Visibilidade</label>\r\n                    <b-form-checkbox\r\n                      switch\r\n                      v-model=\"selected.VisivelMarketplace\"\r\n                      disabled\r\n                      size=\"sm\"\r\n                    ></b-form-checkbox>\r\n                  </b-col>\r\n                  <b-col cols=\"5\">\r\n                    <label>Categoria</label>\r\n                    <b-form-select v-model=\"selected.CategoriaId\" :options=\"categorias\" size=\"sm\"></b-form-select>\r\n                  </b-col>\r\n                  <b-col cols=\"6\">\r\n                    <label>Vídeo URL</label>\r\n                    <b-link :href=\"selected.VideoUrl\" target=\"_blank\">\r\n                      <b-input v-model=\"selected.VideoUrl\"></b-input>\r\n                    </b-link>\r\n                  </b-col>\r\n                </b-row>\r\n                <hr />\r\n                <b-button v-b-modal.modal-2 id=\"but_salvar\" variant=\"success\" @click=\"save\">Salvar</b-button>\r\n              </b-tab>\r\n\r\n              <!-- //TODO PREÇO -->\r\n              <b-tab title=\"Preços\" class=\"m-2\">\r\n                <b-table striped bordered hovers responsive small :items=\"selected.Precos\">\r\n                  <template #cell(Valor)=\"data\">\r\n                    <b-form-input type=\"number\" v-model=\"data.item.Valor\" />\r\n                  </template>\r\n                </b-table>\r\n                <b-button\r\n                  v-b-modal.modal-2\r\n                  id=\"btSalvarPrecos\"\r\n                  variant=\"success\"\r\n                  @click=\"btSalvarPrecos\"\r\n                >Salvar</b-button>\r\n              </b-tab>\r\n\r\n              <!-- //TODO LOGISTICA -->\r\n              <b-tab title=\"Logística\" class=\"m-2\">\r\n                <div class=\"mb-2\">\r\n                  <b-row>\r\n                    <b-col cols=\"4\">\r\n                      <label style=\"margin-top: 0px\">Modelo:</label>\r\n                      <b-input v-model=\"logistica.Modelo\" disabled></b-input>\r\n                    </b-col>\r\n                    <b-col cols=\"4\">\r\n                      <label style=\"margin-top: 0px\">Quantidade de volumes:</label>\r\n                      <b-input v-model=\"logistica.QtdVolumes\" disabled></b-input>\r\n                    </b-col>\r\n                    <b-col cols=\"4\">\r\n                      <label style=\"margin-top: 0px\">Peso dos volumes:</label>\r\n                      <b-input v-model=\"logistica.SomaBPesos\" disabled></b-input>\r\n                    </b-col>\r\n                  </b-row>\r\n                </div>\r\n                <div class=\"mb-2\">\r\n                  <b-row>\r\n                    <b-col cols=\"4\">\r\n                      <label style=\"margin-top: 0px\">Comprimento:</label>\r\n                      <b-input type=\"number\" v-model=\"logistica.CaixasAgrupadasComprimento\"></b-input>\r\n                    </b-col>\r\n                    <b-col cols=\"4\">\r\n                      <label style=\"margin-top: 0px\">Largura:</label>\r\n                      <b-input type=\"number\" v-model=\"logistica.CaixasAgrupadasLargura\"></b-input>\r\n                    </b-col>\r\n                    <b-col cols=\"4\">\r\n                      <label style=\"margin-top: 0px\">Altura:</label>\r\n                      <b-input type=\"number\" v-model=\"logistica.CaixasAgrupadasAltura\"></b-input>\r\n                    </b-col>\r\n                  </b-row>\r\n                </div>\r\n                <div class=\"mb-2\">\r\n                  <b-row>\r\n                    <b-col cols=\"4\">\r\n                      <label style=\"margin-top: 0px\">Comprimento Sugerido:</label>\r\n                      <b-input v-model=\"logistica.ComprimentoSugerido\" disabled></b-input>\r\n                    </b-col>\r\n                    <b-col cols=\"4\">\r\n                      <label style=\"margin-top: 0px\">Largura Sugerida:</label>\r\n                      <b-input v-model=\"logistica.LarguraSugerido\" disabled></b-input>\r\n                    </b-col>\r\n                    <b-col cols=\"4\">\r\n                      <label style=\"margin-top: 0px\">Altura Sugerida:</label>\r\n                      <b-input v-model=\"logistica.AlturaSugerido\" disabled></b-input>\r\n                    </b-col>\r\n                  </b-row>\r\n                </div>\r\n                <b-button\r\n                  class=\"mb-2 flex-inline\"\r\n                  style=\"justify-content: end;\"\r\n                  v-b-modal.modal-2\r\n                  id=\"btSalvarPrecos\"\r\n                  variant=\"success\"\r\n                  @click=\"btSalvarMedidas\"\r\n                >Salvar</b-button>\r\n                <br />\r\n                <div class=\"text-center\">\r\n                  <b-table\r\n                    head-variant=\"dark\"\r\n                    striped\r\n                    bordered\r\n                    hovers\r\n                    responsive\r\n                    small\r\n                    :items=\"logistica.Caixas\"\r\n                    :fields=\"fieldsLogistica\"\r\n                  ></b-table>\r\n                </div>\r\n                <div class=\"mb-2\">\r\n                  <span style=\"color: red\">*Todos as medidas são apresentadas em centímetros (cm).</span>\r\n                </div>\r\n              </b-tab>\r\n\r\n              <!-- //TODO ESTOQUES -->\r\n              <b-tab title=\"Estoques\" class=\"m-2\">\r\n                <div class=\"text-center\">\r\n                  <b-table\r\n                    head-variant=\"dark\"\r\n                    striped\r\n                    bordered\r\n                    hovers\r\n                    responsive\r\n                    small\r\n                    :items=\"Estoques\"\r\n                    :fields=\"fieldsEstoques\"\r\n                  >\r\n                    <template #head(TEstoque)=\"data\">\r\n                      <span v-b-tooltip.hover title=\"Estoque - PreColeta\">{{ data.label }}</span>\r\n                    </template>\r\n                  </b-table>\r\n                </div>\r\n                <!-- <b-button\r\n                  class=\"mb-2 flex-inline\"\r\n                  style=\"justify-content: end;\"\r\n                  v-b-modal.modal-2\r\n                  id=\"btSalvarPrecos\"\r\n                  variant=\"success\"\r\n                  @click=\"btSalvarMedidas\"\r\n                >Salvar</b-button>-->\r\n              </b-tab>\r\n\r\n              <!-- //TODO IMAGENS -->\r\n              <b-tab title=\"Imagens\" class=\"m-2\">\r\n                <b-button\r\n                  class=\"mb-2\"\r\n                  v-b-modal.modal-2\r\n                  id=\"btSalvarImagens\"\r\n                  variant=\"success\"\r\n                  @click=\"ImagensSave\"\r\n                >Salvar</b-button>\r\n                <b-button\r\n                  class=\"mb-2 ml-2\"\r\n                  v-b-modal.modal-2\r\n                  id=\"btSalvarImagens\"\r\n                  @click=\"ReenviarImagens\"\r\n                  v-b-tooltip.hover title=\"Reenviar imagens para Vtex\"\r\n                >Reenviar Vtex</b-button>\r\n                <div>\r\n                  <b-card class=\"mb-2\" v-for=\"imagem in selected.Imagens\" :key=\"imagem.id\">\r\n                    <b-row>\r\n                      <b-col>\r\n                        <!--@click=\"carregaImagem(imagem)\"-->\r\n                        <b-img\r\n                          @click=\"abrirImagem(imagem.Url, imagem.NomeSEO)\"\r\n                          width=\"250\"\r\n                          height=\"150\"\r\n                          :src=\"imagem.Url\"\r\n                          rounded\r\n                        ></b-img>\r\n                      </b-col>\r\n                      <b-col>\r\n                        <b-row>\r\n                          <b-col>\r\n                            <label style=\"margin-top: 0px\">Nome SEO:</label>\r\n                            <b-input v-model=\"imagem.NomeSEO\"></b-input>\r\n                          </b-col>\r\n                          <b-col class=\"col-3\">\r\n                            <label style=\"margin-top: 0px\">Indice</label>\r\n                            <b-input type=\"number\" v-model=\"imagem.Indice\"></b-input>\r\n                          </b-col>\r\n                        </b-row>\r\n                        <b-row>\r\n                          <label class=\"ml-3 mt-2\" style=\"margin-top: 0px\">Principal:</label>\r\n                          <b-form-checkbox\r\n                            class=\"mt-1 ml-2\"\r\n                            v-model=\"imagem.Principal\"\r\n                            name=\"Principal\"\r\n                            switch\r\n                          ></b-form-checkbox>\r\n                        </b-row>\r\n                        <b-row>\r\n                          <label class=\"ml-3 mt-2\" style=\"margin-top: 0px\">Vitrine:</label>\r\n                          <b-form-checkbox\r\n                            class=\"mt-1 ml-2\"\r\n                            v-model=\"imagem.Vitrine\"\r\n                            name=\"Vitrine\"\r\n                            switch\r\n                          ></b-form-checkbox>\r\n                        </b-row>\r\n                        <b-row>\r\n                          <label class=\"ml-3 mt-2\" style=\"margin-top: 0px\">Medidas:</label>\r\n                          <b-form-checkbox\r\n                            class=\"mt-1 ml-2\"\r\n                            v-model=\"imagem.Medidas\"\r\n                            name=\"Medidas\"\r\n                            switch\r\n                          ></b-form-checkbox>\r\n                        </b-row>\r\n                      </b-col>\r\n                    </b-row>\r\n                  </b-card>\r\n                </div>\r\n              </b-tab>\r\n            </b-tabs>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions, mapGetters } from \"vuex\";\r\n//import GridGallery from '../../../components/GridGallery'\r\nimport ApiService from \"../../../services/api.service\";\r\nexport default {\r\n  name: \"EcommerceProdutos\",\r\n  data() {\r\n    return {\r\n      filtro: \"\",\r\n      filterOn: [\r\n        \"Referencia\",\r\n        \"Nome\",\r\n        \"LinhaProduto\",\r\n        \"MarcaFantasia\",\r\n        \"CategoriaDescricaoCompleta\",\r\n        \"VisivelMarketplace\"\r\n        // \"VisivelEcommerce\"\r\n      ],\r\n      fieldsLogistica: [\r\n        { key: \"Caixa\", sortable: false, tdClass: \"text-nowrap\" },\r\n        { key: \"Comprimento\", sortable: false, tdClass: \"text-nowrap\" },\r\n        { key: \"Largura\", sortable: false, tdClass: \"text-nowrap\" },\r\n        { key: \"Altura\", sortable: false, tdClass: \"text-nowrap\" },\r\n        {\r\n          key: \"PesoBVolume\",\r\n          label: \"Peso do Volume\",\r\n          sortable: false,\r\n          tdClass: \"text-nowrap\"\r\n        }\r\n      ],\r\n      fieldsEstoques: [\r\n        { key: \"Composicao\", sortable: false, tdClass: \"text-nowrap\" },\r\n        { key: \"Estoque\", sortable: false, tdClass: \"text-nowrap\" },\r\n        { key: \"PreColeta\", sortable: false, tdClass: \"text-nowrap\" },\r\n        { key: \"Producao\", sortable: false, tdClass: \"text-nowrap\" },\r\n        { key: \"TEstoque\", sortable: false, tdClass: \"text-nowrap\" },\r\n        { key: \"Empenho\", sortable: false, tdClass: \"text-nowrap\" }\r\n      ],\r\n      fields: [\r\n        { key: \"Referencia\", sortable: true, tdClass: \"text-nowrap\" },\r\n        { key: \"Nome\", sortable: true, thStyle: \"width: 400px\" },\r\n        {\r\n          key: \"EstoqueTotal\",\r\n          label: \"Estoque\",\r\n          sortable: true,\r\n          tdClass: \"text-nowrap\"\r\n        },\r\n        {\r\n          key: \"PrecosB2C\",\r\n          label: \"B2C\",\r\n          sortable: true,\r\n          tdClass: \"text-nowrap\"\r\n        },\r\n        {\r\n          key: \"PrecosSite\",\r\n          label: \"Site\",\r\n          sortable: true,\r\n          tdClass: \"text-nowrap\"\r\n        }\r\n      ],\r\n      items: [],\r\n      selected: {},\r\n      categorias: [],\r\n      Cores: [],\r\n      UsaTituloModelo: false,\r\n      UsaDescricaoModelo: false,\r\n      UsaCategoriaModelo: false,\r\n      UsaVideoModelo: false,\r\n      logistica: [],\r\n      Estoques: [],\r\n      tabAtivo: 0,\r\n      isBusy: false,\r\n      img: {},\r\n    };\r\n  },\r\n  //   components: {\r\n  //     GridGallery,\r\n  //   },\r\n\r\n  created() {\r\n    this.acthome(\"/ecommerce\");\r\n    this.buscarProdutos();\r\n  },\r\n\r\n  //   computed: {\r\n  //     ...mapGetters([\"gtxMenusGallery\"]),\r\n  //   },\r\n  computed: {\r\n    produtosTotaisCount() {\r\n      if (this.items == undefined) {\r\n        return 0;\r\n      }\r\n      return this.items.length;\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    carregaImagem(img) {\r\n      this.img = img;\r\n      this.$bvModal.show(\"modal-imagens\");\r\n    },\r\n    abrirImagem(url, nome) {\r\n      // cria o elemento modal em estrutura HTML\r\n      const h = this.$createElement;\r\n      // More complex structure\r\n\r\n      const messageVNode = h(\"b-img\", {\r\n        props: {\r\n          src: url,\r\n          center: true,\r\n          fluid: true\r\n        }\r\n      });\r\n\r\n      // tem que passar em array se não não funciona\r\n      this.$bvModal.msgBoxOk([messageVNode], {\r\n        buttonSize: \"sm\",\r\n        centered: true,\r\n        size: \"xl\"\r\n      });\r\n    },\r\n\r\n    ...mapActions([\"acthome\"]),\r\n    buscarProdutos() {\r\n      this.isBusy = true;\r\n      ApiService.get(\"v2/IntranetEcommerceB2C/Produtos/Get\").then(result => {\r\n        this.items = result.data.Data;\r\n        // console.log(this.items);\r\n        this.isBusy = false;\r\n      });\r\n      ApiService.get(\"v2/IntranetEcommerceB2C/Produtos/Categorias\").then(\r\n        result => {\r\n          this.categorias = result.data.Data;\r\n          // console.log(\"Categorias\", result.data.Data);\r\n        }\r\n      );\r\n      ApiService.get(\"v2/IntranetEcommerceB2C/Produtos/Cores\").then(result => {\r\n        // console.log(\"Cores\", result.data.Data);\r\n        this.Cores = result.data.Data;\r\n      });\r\n    },\r\n    onRowSelected(items) {\r\n      if (items[0]) {\r\n        this.selected = items[0];\r\n        if (this.tabAtivo == 3) {\r\n          //LOGISTICA\r\n          this.activeTab(3);\r\n        } else if (this.tabAtivo == 4) {\r\n          //Estoque Componentes\r\n          this.activeTab(4);\r\n        }\r\n      }\r\n    },\r\n\r\n    ImagensSave() {\r\n      ApiService.post(\"v2/IntranetEcommerceB2C/Produtos/ImagensSave\", {\r\n        Imagens: this.selected.Imagens\r\n      })\r\n        .then(data => {\r\n          this.$root.toastsuccess(\"Registro salvo com sucesso!\");\r\n        })\r\n        .catch(e => {\r\n          this.$root.toastdanger(\"Erro ao salvar o registro!\");\r\n        });\r\n    },\r\n\r\n    ReenviarImagens() {\r\n      ApiService.get(\"https://api.grupok1.com.br/Integracoes/Marketplaces/Vtex/ImagensDelete\", {\r\n        params: { Referencia: this.selected.Referencia }\r\n      })\r\n        .then(data => {\r\n          ApiService.get(\"https://api.grupok1.com.br/Integracoes/Marketplaces/Vtex/ImagensPost\", {\r\n            params: { Referencia: this.selected.Referencia }\r\n          })\r\n            .then(data => {\r\n              ApiService.get(\"https://api.grupok1.com.br/Integracoes/Marketplaces/Vtex/ImagensConsulta\", {\r\n                params: { Referencia: this.selected.Referencia }\r\n              })\r\n                .then(data => {\r\n                  this.$root.toastsuccess(\"Reenviado com sucesso!\");\r\n                })\r\n                .catch(e => {\r\n                  this.$root.toastdanger(\"Erro ao salvar o registro!\");\r\n                });\r\n            })\r\n            .catch(e => {\r\n              this.$root.toastdanger(\"Erro ao salvar o registro!\");\r\n            });\r\n        })\r\n        .catch(e => {\r\n          this.$root.toastdanger(\"Erro ao salvar o registro!\");\r\n        });\r\n    },\r\n\r\n    save() {\r\n      ApiService.post(\"v2/IntranetEcommerceB2C/Produtos/Save\", this.selected)\r\n        .then(data => {\r\n          this.$root.toastsuccess(\"Registro salvo com sucesso!\");\r\n        })\r\n        .catch(e => {});\r\n    },\r\n    btSalvarPrecos() {\r\n      ApiService.post(\r\n        \"v2/IntranetEcommerceB2C/Produtos/SalvarPrecos\",\r\n        this.selected.Precos\r\n      )\r\n        .then(data => {\r\n          this.$root.toastsuccess(\"Preços salvos com sucesso!\");\r\n        })\r\n        .catch(e => {});\r\n    },\r\n\r\n    btSalvarMedidas() {\r\n      ApiService.post(\"v2/IntranetEcommerceB2C/Produtos/LogisticaSave\", {\r\n        logistica: this.logistica\r\n      })\r\n        .then(result => {\r\n          if (result.data.Success == true) {\r\n            this.$root.toastsuccess(\"Registro salvo com sucesso!\");\r\n          } else {\r\n            this.$root.toastdanger(\"Erro ao salvar o registro!\");\r\n          }\r\n        })\r\n        .catch(err => {\r\n          this.$root.toastdanger(\"Erro ao salvar o registro!\");\r\n        });\r\n    },\r\n\r\n    activeTab(tab) {\r\n      if (tab === 3 && this.selected) {\r\n        //LOGISTICA\r\n        ApiService.post(\"v2/IntranetEcommerceB2C/Produtos/LogisticaGet\", {\r\n          referencia: this.selected.Referencia\r\n        }).then(result => {\r\n          this.logistica = result.data.Data;\r\n        });\r\n      } else if (tab === 4 && this.selected) {\r\n        // Estoques\r\n        ApiService.post(\"v2/IntranetEcommerceB2C/Produtos/EstoqueGet\", {\r\n          referencia: this.selected.Referencia\r\n        }).then(result => {\r\n          this.Estoques = result.data.Data.EstoqueComponentes;\r\n          this.selected.EstoqueTotal = result.data.data.Estoque;\r\n        });\r\n      } else {\r\n        return;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n#divPrincipal {\r\n  font-size: 12px;\r\n}\r\nlabel {\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"]}]}