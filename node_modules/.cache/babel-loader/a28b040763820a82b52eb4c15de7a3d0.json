  itensped: [],\r\n        notasped: []\r\n      },\r\n    }\r\n  },\r\n  computed: {\r\n    params() {\r\n      return {\r\n        idpedido: this.form.idpedido,\r\n        Numero: this.form.Numero\r\n      }\r\n    },\r\n    titlepedido() {\r\n      return this.form.idpedido > 0 ? this.form.Numero : 'NOVO';\r\n    }\r\n  },\r\n  beforeMount() {\r\n    // console.log('beforeMount.$route.params', this.$route.params);\r\n    // console.log('beforeMount.$route.query', this.$route.query);\r\n\r\n    if (this.idpedido == 0) {\r\n      this.form.idpedido = this.$route.params ? this.$route.params.idpedido || 0 : 0;\r\n    } else {\r\n      this.form.idpedido = this.idpedido;\r\n    }\r\n\r\n    this.getPedido();\r\n\r\n    // console.log('beforeMount.form', this.form);\r\n  },\r\n  methods: {\r\n    evtRespostaUpload(anexos) {\r\n      console.log(\"evt-respostaUpload\", anexos);\r\n      this.anexos = anexos;\r\n    },\r\n    getPedido() {\r\n      if (this.form.idpedido > 0) {\r\n        API.get(\"v2/Servicos/Pedidos/Get\", this.params)\r\n        .then((apidata)=>{\r\n          const res = API.prepareData(apidata.data);\r\n          if (res.success) {\r\n            this.form = res.data[0];\r\n          } else {\r\n            this.form = this.formempty;\r\n          }\r\n          // console.log('getPedido.form:', this.form);\r\n        })\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>"]}]}