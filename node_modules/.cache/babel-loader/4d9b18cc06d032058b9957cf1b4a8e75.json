e).data || []).find(\r\n      (k) => k[(Ok(object), 1)] == object[(Ok(object), 1)]\r\n    ),\r\n\r\n  gtxAuthStoreDataByObjectNew: (state, getters) => (object) =>\r\n    (getters.gtxAuthStoreData(Ok(object, 0)).data || []).find(\r\n      (k) => k[Ok(object, 1)] == object[Ok(object, 1)]\r\n    ),\r\n};\r\n\r\nconst mutations = {\r\n  [SET_AUTH_HITS](state) {\r\n    state.loginhits++;\r\n  },\r\n  [SET_AUTH](state, token) {\r\n    state.loginAuth.token = token;\r\n    state.loginAuth.auth = true;\r\n    state.loginAuth.lastLogin = new Date();\r\n    state.loginAuth.lastLogout = null;\r\n    state.loginAuth.loading = false;\r\n  },\r\n  \r\n  [SET_AUTH_NAME]: (state, user) => (state.loginAuth.user = user),\r\n  \r\n  [SET_AUTH_LOGOFF]: (state) => {\r\n    state.loginAuth.token = null;\r\n    state.loginAuth.auth = false;\r\n    state.loginAuth.loading = false;\r\n    state.loginAuth.lastLogout = new Date();\r\n  },\r\n\r\n  [SET_AUTH_LOGIN](state, loginauth) {\r\n    state.loginAuth = loginauth;\r\n  },\r\n\r\n  [SET_AUTH_LOADING](state, loading) {\r\n    state.loginAuth.loading = loading || false;\r\n  },\r\n\r\n  [SET_STORE_NAME](state, storename) {\r\n    state.store.name = storename;\r\n  },\r\n\r\n  [SET_AUTH_SET](state, auth) {\r\n    state.loginAuth.auth = auth;\r\n  },\r\n\r\n  [SET_PROFILE](state, profiler) {\r\n    state.profiler = profiler;\r\n  },\r\n\r\n  [SET_LASTROUTE](state, route) {\r\n    state.lastroute = route;\r\n  },\r\n};\r\n\r\nconst actions = {\r\n  actLoginAuth: (state, user) => {\r\n    state.commit(\"setAlert\", { visible: false });\r\n    // state.commit(\"SET_AUTH\", \"\");\r\n\r\n    // console.log('actLoginAuth: ', state.state.loginAuth)\r\n\r\n    //        if (!jwtService.isTokenValid()) {\r\n\r\n    state.commit(\"SET_AUTH_LOADING\", true);\r\n    //console.log(\"actLoginAuth.user:\", user);\r\n\r\n    ApiService.setBasicHeader();\r\n    ApiService.post(\"Authentication\", { ...user })\r\n      .then((res) => {\r\n        // console.log(\"auth.actLoginAuth.then:\", res);\r\n        state.commit(\"SET_AUTH_LOADING\", false);\r\n\r\n        // localStorage.setItem(\"sessionToken\", res.data.TokenAccess);\r\n\r\n        state.commit(\"SET_AUTH_HITS\");\r\n        state.commit(\"SET_AUTH\", res.data.TokenAccess);\r\n\r\n        // ApiService.loginObserver.notify(true);\r\n\r\n        // state.commit(\"SET_AUTH_NAME\", user.User.toLowerCase());\r\n        // state.commit('setAlert', { message: 'Autenticado com sucesso!', variant: 'success' })\r\n\r\n        // salva o nome do login que se autenticou ...\r\n\t\t\t\tApiService.saveStoreUser(user.User, state.state.loginAuth);\r\n\r\n\t\t\t\tApiService.get(\"v2/Servicos/Logins/Get\").then((apidata) => {\r\n          const res = ApiService.prepareData(apidata.data);\r\n          console.log(\"v2/Servicos/Logins/Get\", res);\r\n\t\t\t\t});\r\n\r\n        // reload\r\n        state.dispatch(\"actLoginReload\");\r\n\r\n        // exibe o menu ...\r\n        state.dispatch(\"actmenus\");\r\n        // state.dispatch(\"actnavbar\", true);\r\n      })\r\n      .catch((error) => {\r\n        // console.log(\"auth.actLoginAuth.catch.error:\", error);\r\n        // console.log(\"auth.actLoginAuth.catch.error.response:\", error.response);\r\n        state.commit(\"SET_AUTH_LOADING\", false);\r\n\r\n        let mensagem = \"\";\r\n        if (error.response) {\r\n          mensagem = error.response.data;\r\n        } else {\r\n          mensagem = error.message;\r\n        }\r\n\r\n        state.commit(\"setAlert\", { message: mensagem, variant: \"danger\" });\r\n\r\n        // console.log(\"error: \", error);\r\n\r\n        // throw new Error(`[VUEX] ${error}`);\r\n      });\r\n    //        } else {\r\n    //            state.commit('setAlert', { message: 'Usuário já autenticado', variant: 'info' })\r\n    //        }\r\n  },\r\n\r\n  actMudaSenha: async ({ commit }, passwords) => {\r\n    await ApiService.post(\"Servicos/Muda/Senha\", passwords)\r\n      .then((res) => {\r\n        console.log(\"actMudaSenha:\", passwords, \"res:\", res);\r\n\r\n        if (res.data.Success == \"False\") {\r\n          commit(\"setAlert\", { message: res.data.Error, variant: \"danger\" });\r\n        } else {\r\n          commit(\"setAlert\", {\r\n            message: res.data.Mensagem,\r\n            variant: \"success\",\r\n          });\r\n          return false;\r\n        }\r\n      })\r\n      .catch((error) => {\r\n        let mensagem = \"\";\r\n        if (error.response && error.response.status == 400) {\r\n          mensagem = error.response.data;\r\n        }\r\n        if (error.Error) {\r\n          mensagem = error.Error;\r\n        }\r\n        if (error.Mensagem) {\r\n          mensagem = error.Mensagem;\r\n        }\r\n\r\n        commit(\"setAlert\", { message: mensagem, variant: \"danger\" });\r\n\r\n        console.log(\"error: \", error);\r\n\r\n        throw new Error(`[VUEX] ${error}`);\r\n      });\r\n  },\r\n\r\n  actLoginReload: (state) => {\r\n    // busca os dados do login...\r\n    state.dispatch(\"actAuthProfile\");\r\n\r\n    // carrega as rotas...\r\n    // state.dispatch('actAuthRoutes')\r\n\r\n    // carrega as stores...\r\n    // state.dispatch('actStores')\r\n  },\r\n\r\n  actLoginCheck: (state) => {\r\n    if (state.loginAuth.token) {\r\n      return Promise.resolve(state.loginAuth.token);\r\n    }\r\n  },\r\n\r\n  actLoginAuthOff: ({ commit, dispatch }, ) => {\r\n    // console.log(\"actLoginAuthOff\");\r\n    // ApiService.loginObserver.notify(false);\r\n\r\n    VueCookies.remove(\"authToken\");\r\n    VueCookies.remove(\"token\");\r\n\r\n    const user = jwtService.getLoginAuth();\r\n    const login = jwtService.getUserName();\r\n\r\n    if (user) {\r\n      user.auth = false;\r\n      user.token = \"\";\r\n      user.lastLogout = new Date();\r\n\r\n      localStorage.setItem(login, JSON.stringify(user));\r\n    }\r\n    localStorage.setItem(\"loginAuth\", \"\");\r\n\r\n    const notify = {\r\n      