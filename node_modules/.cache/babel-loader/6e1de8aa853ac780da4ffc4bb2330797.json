456', '7895155123456', '789456789123']\r\n        // {7895155123456: ['7895155123456', '7895155123456'], 789456789123: ['789456789123']}\r\n        const eansgroup = state.eans.reduce((groups, ean) => {\r\n            if (!groups[ean]) {\r\n                groups[ean] = [];\r\n            }\r\n            groups[ean].push(ean);\r\n            return groups;\r\n        }, {}\r\n        );\r\n        \r\n        // [{ean: '7895155123456', reads: 2}, {ean: '789456789123', reads: 1}]\r\n        return Object.keys(eansgroup).map(ean => { return { ean: ean, reads: eansgroup[ean].length } })\r\n    },\r\n    \r\n    gtxeans128reads: (state, getters) => getters.gtxeansreads.map(ean => getters.gtxeans128byean(ean.ean) ),\r\n\r\n    gtxeansreadsbyean: (state, getters) => (ean) => getters.gtxeansreads.find(f => f.ean == ean) || {ean: ''},\r\n    \r\n    gtxeans128readsbyean: (state, getters) => (ean) => getters.gtxeans128byean(getters.gtxeansreadsbyean(ean).ean),\r\n\r\n    gtxeanslidos: (state, getters) => getters.gtxeansreads.length,\r\n\r\n    // ean historico ...\r\n    gtxeanshist: state => state.eanshist,\r\n    gtxeanshistlidos: (state, getters) => getters.gtxeanshist.length,\r\n\r\n    // não usa dados da store ...\r\n    gtxeantype: (state) => (ean) => {\r\n\r\n        let eantype = {ean: ean, type: 'undefined', unique: true}\r\n\r\n        if (((ean.substr(0, 3) == '010') && (ean.substr(16, 2) == '10')) ||\r\n            ((ean.substr(0, 5) == '80060') && (ean.substr(22, 2) == '10')))\r\n            eantype = { ean: ean, type: 'ean128', unique: (ean.substr(0, 3) == '010') }\r\n        \r\n        return eantype\r\n\r\n    },\r\n\r\n    gtxeans128byean: (state, getters) => (e) => {\r\n\r\n        let ean = { ean: e }\r\n\r\n        if ((e) && getters.gtxeantype(e).type == 'ean128') {\r\n            let i = 0\r\n            let fields = {}\r\n            let ean128 = {}\r\n            \r\n            if (state.ean128.reduce((a, b) => a + b.tamanho, 0) > 0) {\r\n                if (getters.gtxeantype(e).unique) {\r\n                    i = 18\r\n                    //if (e.substr(0, 3) == '010') {\r\n                    fields = { ean13: e.substr(3, 13), vol: '01', vols: '01', caixa: 'A' }\r\n                    state.ean128.map(m => {\r\n                        ean128 = { ...ean128, ...{ [m.field]: e.substr(i, m.tamanho), reads: 1 } }\r\n                        i += m.tamanho\r\n                    })\r\n                } else {\r\n                    // if (e.substr(0, 5) == '80060') {s\r\n                    i = 24\r\n                    fields = { ean13: e.substr(5, 13), vol: e.substr(18, 2), vols: e.substr(20, 2), caixa: getters.gtxvolumeascii(e.substr(18, 2)) }\r\n                    state.ean128.map(m => {\r\n                        ean128 = { ...ean128, ...{ [m.field]: e.substr(i, m.tamanho), reads: 1 } }\r\n                        i += m.tamanho\r\n                    })\r\n                }\r\n    \r\n                ean = { ...ean, ...fields, ...ean128 }\r\n            }\r\n        }\r\n\r\n        return ean\r\n    },\r\n\r\n    gtxvolumeascii: state => vol => String.fromCharCode(parseInt(vol) + 64),\r\n\r\n    gtxnewid: state => '_' + Math.random().toString(36).substr(2, 13)\r\n\r\n}\r\n\r\nconst mutations = {\r\n\r\n    seteans: (state, ean) => state.eans.push(ean),\r\n\r\n    seteansclear: state => state.eans = [],\r\n\r\n    seteansclearean: (state, eans) => state.eans = eans,\r\n\r\n    seteanssave: (state, eanshist) => state.eanshist.push(eanshist),\r\n    \r\n    seteandelete: (state, ean) => {\r\n\r\n        const index = state.eans.findIndex(e => e == ean.ean)\r\n        if (index > -1) state.eans.splice(index, 1)\r\n        \r\n    },\r\n\r\n    seteanhistdelete: (state, id) => {\r\n\r\n        const index = state.eanshist.findIndex(e => e.id == id)\r\n        if (index > -1) state.eanshist.splice(index, 1)\r\n        \r\n    },\r\n\r\n    seteanhisteandelete: (state, ean) => {\r\n\r\n        const list = state.eanshist.find(e => e.id == ean.id)\r\n        let index = list.eans128reads.findIndex(e => e.ean == ean.ean)\r\n        if (index > -1) list.eans128reads.splice(index, 1)\r\n        index = list.eansreads.findIndex(e => e.ean == ean.ean)\r\n        if (index > -1) list.eansreads.splice(index, 1)\r\n        list.eanslidos = list.eansreads.length\r\n\r\n        if (list.eanslidos.length == 0) {\r\n            state.eanshist.find(e => e.id == ean.id).splice(0,1)\r\n        }\r\n        \r\n    }\r\n\r\n}\r\n\r\nconst actions = {\r\n\r\n    // 0107895155354473101008510001208\r\n    // 8006078951558966520104100963830653908\r\n    // 8006078951558966520204100963830654008\r\n    // 8006078951558966520304100963830654108\r\n    // 8006078951558966520404100963830654208\r\n    // 01078951553544731010085100012088006078951558966520104100963830653908800607895155896652020410096383065400880060789515589665203041009638306541088006078951558966520404100963830654208\r\n\r\n    acteans: ({ commit }, eans) => {\r\n\r\n        if (eans.length > 0) {\r\n\r\n            if (eans.includes('\\n')) {\r\n                eans.split('\\n')\r\n                    .filter(e => e !== '') // novo array somente com os eans, sem os enters no final\r\n                    .map(e => commit('seteans', e))\r\n            } else {\r\n                commit('seteans', eans)\r\n            }\r\n        \r\n        }\r\n\r\n    },\r\n\r\n    acteansclear: ({ commit }) => {\r\n\r\n        commit('seteansclear')\r\n        \r\n    },\r\n\r\n    acteansdeletebyean: ({ commit }, ean) => {\r\n\r\n        commit('seteandelete', ean)\r\n\r\n    },\r\n\r\n    acteanshistclear: ({ commit }, id) => {\r\n\r\n        console.log('acteanshistclear', id)\r\n\r\n        commit('seteanhistdelete', id)\r\n        \r\n    },\r\n\r\n    acteanshistdeletebyean: ({ commit }, ean) => {\r\n\r\n        commit('seteanhisteandelete', ean)\r\n\r\n    },\r\n\r\n    acteanssave: ({ commit, getters }) => {\r\n\r\n        const eanshist = {\r\n            id: '_' + Math.random().toString(36).substr(2, 13),\r\n            envio: new Date(),\r\n            eans: getters.eans,\r\n            eanslidos: getters.gtxeanslidos,\r\n            eansreads: getters.gtxeansreads,\r\n            eans128reads: getters.gtxeans128reads\r\n        }\r\n        \r\n        commit('seteanssave', eanshist)\r\n\r\n        commit('seteansclear')\r\n\r\n    }\r\n}\r\n\r\nexport default { state, mutations, getters, actions }\r\n"],"mappings":";AAAA,MAAMA,KAAK,GAAG;EAEVC,IAAI,EAAE,EAAE;EACRC,QAAQ,EAAE,EAAE;EACZC,MAAM,EAAE,CACJ;IAACC,KAAK,EAAE,MAAM;IAAEC,OAAO,EAAE;EAAC,CAAC;EAAE;EAC7B;IAACD,KAAK,EAAE,KAAK;IAAEC,OAAO,EAAE;EAAC,CAAC;EAAG;EAC7B;IAACD,KAAK,EAAE,KAAK;IAAEC,OAAO,EAAE;EAAC,CAAC,CAAG;EAAA;AAGrC,CAAC;;AAED,MAAMC,OAAO,GAAG;EAEZC,OAAO,EAAEP,KAAK,IAAIA,KAAK,CAACC,IAAI;EAE5B;EACAO,YAAY,EAAER,KAAK,IAAI;IACnB;IACA;IACA;IACA,MAAMS,SAAS,GAAGT,KAAK,CAACC,IAAI,CAACS,MAAM,CAAC,CAACC,MAAM,EAAEC,GAAG,KAAK;MACjD,IAAI,CAACD,MAAM,CAACC,GAAG,CAAC,EAAE;QACdD,MAAM,CAACC,GAAG,CAAC,GAAG,EAAE;MACpB;MACAD,MAAM,CAACC,GAAG,CAAC,CAACC,IAAI,CAACD,GAAG,CAAC;MACrB,OAAOD,MAAM;IACjB,CAAC,EAAE,CAAC,CAAC,CACJ;;IAED;IACA,OAAOG,MAAM,CAACC,IAAI,CAACN,SAAS,CAAC,CAACO,GAAG,CAACJ,GAAG,IAAI;MAAE,OAAO;QAAEA,GAAG,EAAEA,GAAG;QAAEK,KAAK,EAAER,SAAS,CAACG,GAAG,CAAC,CAACM;MAAO,CAAC;IAAC,CAAC,CAAC;EACnG,CAAC;EAEDC,eAAe,EAAE,CAACnB,KAAK,EAAEM,OAAO,KAAKA,OAAO,CAACE,YAAY,CAACQ,GAAG,CAACJ,GAAG,IAAIN,OAAO,CAACc,eAAe,CAACR,GAAG,CAACA,GAAG,CAAC,CAAE;EAEvGS,iBAAiB,EAAE,CAACrB,KAAK,EAAEM,OAAO,KAAMM,GAAG,IAAKN,OAAO,CAACE,YAAY,CAACc,IAAI,CAACC,CAAC,IAAIA,CAAC,CAACX,GAAG,IAAIA,GAAG,CAAC,IAAI;IAACA,GAAG,EAAE;EAAE,CAAC;EAEzGY,oBAAoB,EAAE,CAACxB,KAAK,EAAEM,OAAO,KAAMM,GAAG,IAAKN,OAAO,CAACc,eAAe,CAACd,OAAO,CAACe,iBAAiB,CAACT,GAAG,CAAC,CAACA,GAAG,CAAC;EAE9Ga,YAAY,EAAE,CAACzB,KAAK,EAAEM,OAAO,KAAKA,OAAO,CAACE,YAAY,CAACU,MAAM;EAE7D;EACAQ,WAAW,EAAE1B,KAAK,IAAIA,KAAK,CAACE,QAAQ;EACpCyB,gBAAgB,EAAE,CAAC3B,KAAK,EAAEM,OAAO,KAAKA,OAAO,CAACoB,WAAW,CAACR,MAAM;EAEhE;EACAU,UAAU,EAAG5B,KAAK,IAAMY,