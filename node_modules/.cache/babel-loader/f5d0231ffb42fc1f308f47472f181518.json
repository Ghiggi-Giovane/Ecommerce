AoC;MAEAC;IAEA;;EAlEA;AA/DA","names":["name","data","chavenfe","nfefields","key","label","sortable","nfebuttons","icon","variant","size","tooltip","header","nfelist","itens","components","GaiolasItens","TableEditApi","computed","itnfelist","n","idnfe","iditnfe","item","descricao","qtde","haveitens","methods","buscaNFE","pesquisa","newSearch","addNota","addItem","un","remItem","addAllItens","params","rowInsert","console"],"sourceRoot":"src/pages/almoxarifado/views","sources":["nfeitens.vue"],"sourcesContent":["<template>\r\n    <div class=\"m-2\">\r\n\r\n        <b-card header=\"Notas fiscais de Entrada\" header-bg-variant=\"dark\" header-text-variant=\"white\">\r\n\r\n            <b-input-group id=\"input-group-notas\" label=\"Data Final\" label-for=\"input-chavenfe\" class=\"mb-2\">\r\n                <b-form-input\r\n                    id=\"input-chavenfe\"\r\n                    type=\"number\"\r\n                    placeholder=\"Chave DANFE\"\r\n                    v-model=\"chavenfe\">\r\n                </b-form-input>\r\n                <b-input-group-append>\r\n                    <b-button variant=\"success\" @click=\"buscaNFE\">Buscar</b-button>\r\n                </b-input-group-append>\r\n            </b-input-group>\r\n\r\n            <!-- Pesquisa nota de entrada -->\r\n            <div class=\"bg-dark text-warning rounded p-2 mb-2\" v-if=\"gtxAlmoxnfeData.length>0\">\r\n                <div>\r\n                    <strong>Numero Nota:</strong>\r\n                    <span class=\"ml-2 text-light\">{{ gtxAlmoxnfeData[0].numero }}</span>\r\n                    <strong class=\"ml-3\">Emiss√£o:</strong>\r\n                    <span class=\"ml-2 text-light\">{{ gtxAlmoxnfeData[0].data_emissao | dateBR }}</span>\r\n                </div>\r\n                <div>\r\n                    <strong>Fornecedor:</strong>\r\n                    <span class=\"ml-2 text-light\">{{ gtxAlmoxnfeData[0].fornecedor.nome }}</span>\r\n                </div>\r\n                <div class=\"mt-3\">\r\n                    <b-card header-bg-variant=\"secondary\" header-text-variant=\"white\" header=\"Itens da nota\">\r\n                        <!-- <TableEditApi :fields=\"nfefields\" :items=\"gtxAlmoxnfeDataItens\"/> -->   \r\n                        <b-table striped hover :items=\"gtxAlmoxnfeDataItens\" :fields=\"nfefields\">\r\n                            <template #cell(actions)=\"data\">\r\n                                <b-button-group>\r\n                                    <b-button size=\"sm\" variant=\"outline-primary\" v-b-tooltip.hover.topleft=\"'Adicionar'\" @click=\"addItem(data.item)\"><b-icon-plus/></b-button>\r\n                                </b-button-group>\r\n                            </template>\r\n                        </b-table>         \r\n                    </b-card>\r\n                </div>\r\n                <div class=\"d-flex justify-content-between mt-2\">\r\n                    <div>\r\n                        <strong>Total de itens:</strong>\r\n                        <span class=\"ml-2 text-light\">{{ gtxAlmoxnfeDataItens.length }}</span>\r\n                    </div>\r\n                    <div>\r\n                        <b-button variant=\"light\" @click=\"newSearch\">Cancelar</b-button>\r\n                        <b-button variant=\"primary\" @click=\"addNota\" class=\"ml-2\">Adicionar todos os itens</b-button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <!-- Itens selecionados -->\r\n            <!-- <b-card header-bg-variant=\"secondary\" header-text-variant=\"white\" header=\"Itens selecionados\"> -->\r\n            <b-card header-bg-variant=\"secondary\" header-text-variant=\"white\" header=\"Itens selecionados\" header-tag=\"div\">\r\n                <!-- <template #header>\r\n                    <div class=\"d-flex justify-content-between\">\r\n                        <p>Itens selecionados</p>\r\n                        <b-button size=\"sm\" variant=\"primary\">Movimenta√ß√£o Estoque</b-button>\r\n                    </div>\r\n                </template> -->\r\n\r\n                <!-- <TableEditApi\r\n                    v-if=\"nfefields.length>0\"\r\n                    :fields=\"nfefields\"\r\n                    :items=\"itnfelist\"\r\n                    :buttons=\"nfebuttons\"\r\n                /> -->\r\n\r\n                <b-table striped hover :items=\"itens\" :fields=\"nfefields\">\r\n                    <template #cell(actions)=\"data\">\r\n                        <b-button-group>\r\n                            <!-- <b-button size=\"sm\" variant=\"outline-primary\" v-b-tooltip.hover.left=\"'Estoque'\" @click=\"rowInsert(data.item)\"><b-icon-cart-plus/></b-button>\r\n                            <b-button size=\"sm\" variant=\"outline-danger\" v-b-tooltip.hover.left=\"'Remover'\" @click=\"remItem(data.item)\"><b-icon-cart-dash/></b-button> -->\r\n                            <b-button size=\"sm\" variant=\"outline-primary\" hint=\"Teste hint\" alt=\"Teste alt\" @click=\"rowInsert(data.item)\"><b-icon-cart-plus/></b-button>\r\n                            <b-button size=\"sm\" variant=\"outline-danger\" @click=\"remItem(data.item)\"><b-icon-cart-dash/></b-button>\r\n                        </b-button-group>\r\n                    </template>\r\n\r\n                    <template #cell(qtde)=\"data\">\r\n                        <b-form-spinbutton v-model=\"data.item.qtde\" min=\"1\"></b-form-spinbutton>\r\n                    </template>\r\n                </b-table>\r\n\r\n                <div class=\"d-flex justify-content-between mt-2\">\r\n                    <div>\r\n                        <strong>Total de itens:</strong>\r\n                        <span class=\"ml-2\">{{ itens.length }}</span>\r\n                    </div>\r\n                    <div>\r\n                        <b-button variant=\"primary\" @click=\"addAllItens\" :disabled=\"!haveitens\">Adicionar todos os itens</b-button>\r\n                    </div>\r\n                </div>\r\n\r\n\r\n        </b-card>\r\n\r\n        <!-- TESTES\r\n\r\n        gtxAlmoxnfeData: <pre>{{gtxAlmoxnfeData}}</pre>\r\n\r\n        nfelist: <pre>{{nfelist}}</pre>\r\n\r\n        itnfelist: <pre>{{itnfelist}}</pre>\r\n\r\n        itens: <pre>{{itens}}</pre>\r\n\r\n        <b-modal\r\n            id=\"modal-almoxestoque\"\r\n            title=\"Movimenta√ß√£o de estoque\">\r\n            <GaiolasItens/>\r\n        </b-modal>\r\n\r\n        -->\r\n\r\n        </b-card>\r\n        \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions, mapGetters } from 'vuex'\r\nimport GaiolasItens from '../components/GaiolasItens.vue'\r\nimport TableEditApi from '../../../components/TableEditApi.vue'\r\nexport default {\r\n\r\n    name: 'NFeItens',\r\n\r\n    data() {\r\n        return {\r\n\r\n            chavenfe: '',\r\n\r\n            nfefields: [\r\n                {key: 'item', label: 'Item', sortable: true},\r\n                {key: 'descricao', label: 'Descri√ß√£o', sortable: true},\r\n                {key: 'un', label: 'UND', sortable: true},\r\n                {key: 'qtde', label: 'Quant.', sortable: true},\r\n                {key: 'actions', label: 'A√ß√µes'},\r\n            ],\r\n\r\n            nfebuttons: [\r\n                {key: 'mov', icon: 'box-seam', variant: 'success', size: 'sm', tooltip: 'Movimenta Estoque'},\r\n                {key: 'add', icon: 'plus', variant: 'primary', size: 'sm', tooltip: 'Adicionar manual', header: true}\r\n            ],\r\n\r\n            nfelist: [],\r\n            itens: []\r\n        }\r\n    },\r\n\r\n    components: {\r\n\r\n        GaiolasItens,\r\n        TableEditApi\r\n        \r\n    },\r\n\r\n    computed: {\r\n\r\n        ...mapGetters([\r\n            'gtxAlmoxnfeData',\r\n            'gtxAlmoxnfeError',\r\n            'gtxAlmoxnfeDataItens'\r\n        ]),\r\n\r\n        itnfelist() {\r\n\r\n            let itens = []\r\n            this.nfelist.map(n => {\r\n                n.itens.map(i => {\r\n                    itens.push(\r\n                        { idnfe: n.idnfe, iditnfe: i.iditnfe, item: i.item, descricao: i.descricao, qtde: i.qtde}\r\n                    )\r\n                })\r\n            })\r\n\r\n            return itens\r\n\r\n        },\r\n\r\n        haveitens() {\r\n            return (this.itens.length>0)\r\n        }\r\n\r\n    },\r\n\r\n    methods: {\r\n\r\n        ...mapActions([\r\n            'actAlmoxnfe',\r\n            'actAlmoxnfeClear'\r\n        ]),\r\n\r\n        buscaNFE() {\r\n\r\n            const params = { pesquisa: this.chavenfe }\r\n\r\n            this.actAlmoxnfe(params)\r\n                //.then((res) => { this.$root.toastsuccess('certo'); console.log('actAlmoxnfe.catch:', res) })\r\n                // .catch((err) => { this.$root.toastdanger(err.Error) })\r\n\r\n        },\r\n\r\n        newSearch() {\r\n                this.actAlmoxnfeClear()\r\n                this.chavenfe = ''\r\n        },\r\n\r\n        addNota() {\r\n\r\n            if (!this.nfelist.find(f => f.idnfe == this.gtxAlmoxnfeData[0].idnfe)) {\r\n                this.nfelist.push(this.gtxAlmoxnfeData[0])\r\n                this.addItem(this.gtxAlmoxnfeData[0].itens)\r\n                this.newSearch()\r\n            } else {\r\n                // this.$root.toastdanger('Nota j√° adicionada!')\r\n            }\r\n\r\n        },\r\n\r\n        addItem(item) {\r\n\r\n            if (!Array.isArray(item)) {\r\n                item = [item]\r\n            }\r\n\r\n            item.map(m => {\r\n                if (!this.itens.find(i => i.item == m.item)) {\r\n                    this.itens.push({item: m.item, descricao: m.descricao, un: m.unidadeutilizada, qtde: parseFloat(m.qtde)})\r\n                } else {\r\n                    this.itens.find(i => i.item == m.item).qtde += parseFloat(m.qtde)\r\n                }\r\n            })\r\n\r\n        },\r\n\r\n        remItem(item) {\r\n            let i = this.itens.findIndex(i => i.item == item.item)\r\n            if (i > -1) this.itens.splice(i, 1)\r\n        },\r\n\r\n        addAllItens() {\r\n\r\n            //this.$router.push(`/gaiolasitens?itens=${itens}`)\r\n            this.$router.push({ name: 'gaiolasitens', params: { data: this.itens }})\r\n\r\n        },\r\n\r\n        rowInsert(item) {\r\n\r\n            console.log('Abrir entrada de estoque:', item)\r\n\r\n        }\r\n\r\n    }\r\n\r\n    \r\n}\r\n</script>"]}]}