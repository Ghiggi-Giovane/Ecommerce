<span>{{p.Medidas}}</span>\r\n                            </b-col>\r\n                        </b-row>\r\n                     </b-card>\r\n                    <b-table striped hover small :items=\"p.Volumes\" :fields=\"tabFields\">\r\n\r\n                        <template #cell(Actions)=\"data\">\r\n                            <b-form-checkbox v-model=\"data.item.checked\"></b-form-checkbox>\r\n                        </template>\r\n\r\n                    </b-table>\r\n                    <b-card-footer class=\"font-weight-bolder\" v-if=\"tabFieldsChecked.length==0\">{{ p.Volumes.length }} Registros</b-card-footer>\r\n                    <b-card-footer class=\"font-weight-bolder\" v-else>{{ tabFieldsChecked.length }} Selecionados</b-card-footer>\r\n                </b-tab>\r\n            </b-tabs>\r\n        </b-card>\r\n\r\n        <!--\r\n        tabIndex: {{tabIndex}}<br>\r\n        tabIndex: {{tabIndex+1}}<br>\r\n        gtxPedidosAcessoriosVolumesChecked: {{gtxPedidosAcessoriosVolumesChecked(tabIndex+1)}}<br>\r\n        gtxPedidosAcessoriosVolumes: {{gtxPedidosAcessoriosVolumes(tabIndex+1)}}<br>\r\n        gtxPedidosAcessoriosData: {{gtxPedidosAcessoriosData}}<br>\r\n        -->\r\n    </div>\r\n</template>\r\n<script>\r\nimport { mapActions, mapGetters } from 'vuex'\r\nexport default {\r\n\r\n    name: 'PedidosAcessorios',\r\n\r\n    data() {\r\n        return {\r\n            tabIndex: 0,\r\n            tabFields: [\r\n                {key: 'Item', sortable: true},\r\n                {key: 'Descricao', label: 'Descrição', sortable: true},\r\n                {key: 'Qtde', sortable: true},\r\n                {key: 'Actions', sortable: false},\r\n            ],\r\n            pedido: 2952208\r\n        }\r\n    },\r\n\r\n    computed: {\r\n        ...mapGetters([\r\n            'gtxPedidosAcessoriosData',\r\n            'gtxPedidosAcessoriosVolumes',\r\n            'gtxPedidosAcessoriosVolumesChecked'\r\n        ]),\r\n\r\n        tabFieldsChecked() {\r\n            return this.gtxPedidosAcessoriosVolumesChecked(this.tabIndex+1)\r\n        }\r\n    },\r\n\r\n    beforeMount() {\r\n        this.buscar()\r\n    },\r\n\r\n    methods: {\r\n        ...mapActions([\r\n            'actLoadingVisible',\r\n            'actPedidosAcessorios',\r\n            'actPedidosAcessoriosLimpar'\r\n        ]),\r\n\r\n        buscar() {\r\n            this.actLoadingVisible(true)\r\n            this.actPedidosAcessorios({numero: this.pedido})\r\n        },\r\n\r\n        limpar() {\r\n            this.actPedidosAcessoriosLimpar()\r\n        }\r\n    },\r\n\r\n    watch: {\r\n        gtxPedidosAcessoriosData() {\r\n            this.actLoadingVisible(false)\r\n        }\r\n    }\r\n    \r\n}\r\n</script>"]}]}