el=\"form.Data\"\r\n                class=\"form-datepicker\"\r\n              ></b-form-datepicker>\r\n            </b-col>\r\n\r\n            <b-col cols=\"12\" lg=\"4\">\r\n              <h6>Empresa</h6>\r\n              <b-form-select\r\n                name=\"Empresa\"\r\n                class=\"form-select\"\r\n                v-model=\"form.Empresa\"\r\n                :options=\"Certificacao_Empresa\"\r\n                label=\"Empresa\"\r\n              ></b-form-select>\r\n            </b-col>\r\n            <b-col cols=\"12\" lg=\"4\">\r\n              <h6>Marca</h6>\r\n              <b-form-select\r\n                class=\"form-select\"\r\n                v-model=\"form.Marca\"\r\n                :options=\"Certificacao_Marca\"\r\n                placeholder=\"Marca\"\r\n              ></b-form-select>\r\n            </b-col>\r\n          </b-row>\r\n        </div>\r\n      </form>\r\n\r\n      <div class=\"form-container\">\r\n        <form ref=\"forRadio\" @submit.prevent=\"onSave\">\r\n          <b-card\r\n            class=\"card m-2\"\r\n            border-variant=\"dark\"\r\n            :header=\"s.Descricao\"\r\n            header-class=\"header-color\"\r\n            v-for=\"s of Certificacao_GET\"\r\n            :key=\"s.Id\"\r\n          >\r\n            <b-row v-for=\"p of s.Perguntas\" :key=\"p.Id\">\r\n              <b-col cols=\"12\" lg=\"8\">\r\n                <span>{{ p.Descricao }}</span>\r\n                <hr />\r\n              </b-col>\r\n\r\n              <b-col cols=\"12\" lg=\"4\" class=\"radio\">\r\n                <b-form-group v-slot=\"{ ariaDescribedby }\">\r\n                  <b-form-radio-group\r\n                    :id=\"`Resposta${p.Id}`\"\r\n                    v-model=\"p.Resposta\"\r\n                    :options=\"options1\"\r\n                    :aria-describedby=\"ariaDescribedby\"\r\n                    :name=\"`Resposta${p.Id}`\"\r\n                    class=\"d-flex justify-content-between\"\r\n                  ></b-form-radio-group>\r\n                </b-form-group>\r\n                <hr />\r\n              </b-col>\r\n            </b-row>\r\n            <b-row>\r\n              <b-col class=\"col-sm-12\">\r\n                <b-form-textarea\r\n                  id=\"textarea\"\r\n                  v-model=\"s.Consideracao\"\r\n                  placeholder=\"Considerações\"\r\n                  rows=\"3\"\r\n                  max-rows=\"6\"\r\n                ></b-form-textarea>\r\n              </b-col>\r\n            </b-row>\r\n          </b-card>\r\n        </form>\r\n      </div>\r\n      <b-button\r\n        variant=\"primary\"\r\n        :disabled=\"!vSalvar\"\r\n        @click=\"CertificacaoSalvar\"\r\n        >Salvar</b-button\r\n      >\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport API from \"../../../services/api.service\";\r\nimport SearchApi from \"@/components/SearchApi.vue\";\r\n\r\nexport default {\r\n  name: \"CertificacaoOEA\",\r\n  data() {\r\n    return {\r\n      Img: require(\"@/assets/1.jpg\"),\r\n      Certificacao_Marca: [],\r\n      Certificacao_Empresa: [],\r\n      Certificacao_GET: [],\r\n      options1: [\r\n        { text: \"OK\", value: \"1\" },\r\n        { text: \"Não OK\", value: \"2\" },\r\n        { text: \"Não Aplica\", value: \"3\" },\r\n      ],\r\n      form: {\r\n        Data: new Date(),\r\n        Empresa: \"\",\r\n        Marca: \"\",\r\n      },\r\n    };\r\n  },\r\n  beforeMount() {\r\n    this.CertificacaoGET();\r\n    this.CertificacaoMarca();\r\n    this.CertificacaoEmpresa();\r\n  },\r\n  computed: {\r\n    Certificacao_POST() {\r\n      return { form: this.form, secao: this.Certificacao_GET };\r\n    },\r\n    PercFormCompleted() {\r\n      let totais = this.Certificacao_POST.secao\r\n        .map((s) =>\r\n          s.Perguntas.map((p) => (p.Resposta && p.Resposta > 0 ? 1 : 0))\r\n        )\r\n        .map((r, i, a) => (r.reduce((a, b) => a + b, 0) / a[i].length) * 100.0)\r\n        .map((a) => (a == 100 ? 1 : 0));\r\n      return (totais.reduce((a, b) => a + b, 0) / totais.length) * 100.0;\r\n    },\r\n    vSalvar() {\r\n      return this.PercFormCompleted == 100 ? true : false;\r\n    },\r\n  },\r\n  methods: {\r\n    Limpar() {\r\n      this.Certificacao_GET.map((s) =>\r\n        s.Perguntas.map((p) => (p.Resposta = 0))\r\n      );\r\n      this.form.Empresa = \"\";\r\n      this.form.Marca = \"\";\r\n      this.form.Descricao = \"\";\r\n    },\r\n    CertificacaoSalvar() {\r\n      API.post(\"v2/Servicos/CertificacaoOEA/Post\", this.Certificacao_POST).then(\r\n        (api) => {\r\n          if (api.data.Success == \"True\" || api.data.Success) {\r\n            this.$root.toastsuccess(\"Certificação salva com sucesso!\");\r\n          } else {\r\n            this.$root.toastdanger(api.data.Error);\r\n          }\r\n          this.Limpar();\r\n        }\r\n      );\r\n    },\r\n    CertificacaoGET() {\r\n      API.get(\"v2/Servicos/CertificacaoOEA/GET\").then((api) => {\r\n        if (api.data.Success == \"True\" || api.data.Success) {\r\n          this.Certificacao_GET = api.data.Data;\r\n        } else {\r\n          this.Certificacao_GET = [];\r\n        }\r\n      });\r\n    },\r\n    CertificacaoMarca() {\r\n      API.get(\"v2/Servicos/DespesasMarcas/Get\", this.form).then((api) => {\r\n        if (api.data.Success == \"True\" || api.data.Success) {\r\n          this.Certificacao_Marca = api.data.Data;\r\n        } else {\r\n          this.Certificacao_Marca = [];\r\n        }\r\n      });\r\n    },\r\n    CertificacaoEmpresa() {\r\n      API.get(\"v2/Servicos/CertificacaoEmpresa/Get\", this.form).then((api) => {\r\n        if (api.data.Success == \"True\" || api.data.Success) {\r\n          this.Certificacao_Empresa = api.data.Data;\r\n        } else {\r\n          this.Certificacao_Empresa = [];\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.form {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-family: sans-serif;\r\n}\r\n\r\n.header-color {\r\n  background-color: #b3b3b3;\r\n}\r\n\r\n.div-principal {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-family: sans-serif;\r\n}\r\n.card-1-principal {\r\n  width: 950px;\r\n  height: auto;\r\n}\r\n\r\n.radio {\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n}\r\n\r\n#legenda {\r\n  background-color: grey !important;\r\n}\r\n\r\n.card-header {\r\n  border-width: 3px;\r\n  border-color: black;\r\n}\r\n.form-select {\r\n  border-color: black;\r\n  border-width: 1px;\r\n}\r\n.form-datepicker {\r\n  border-color: black;\r\n  border-width: 1px;\r\n}\r\n</style>\r\n"]}]}