port default {\r\n    \r\n    name: 'ListSearch',\r\n\r\n    props: {\r\n\r\n        id: { type: String, required: true },\r\n        list: { type: Array, required: true }\r\n\r\n    },\r\n\r\n    data() {\r\n        return {\r\n            currentPage: 1,\r\n            perPage: 1,\r\n            selectedid: -1,\r\n            selected: null\r\n        }\r\n    },\r\n\r\n    methods: {\r\n\r\n        listkey(keycode, index) {\r\n            if ((keycode == 'ArrowUp') || (keycode == 'ArrowDown')) {\r\n                let i = index\r\n                console.log('list: ', i, ' of ', this.list.length-1)\r\n\r\n                console.log('valor encontrado: ', this.list[i])\r\n\r\n                if (keycode == 'ArrowUp') i = (i==0) ? i : (i - 1)\r\n                if (keycode == 'ArrowDown') i = (i==(this.list.length)) ? i : (i + 1)\r\n                console.log('valor incrementado: ', this.list[i])\r\n\r\n                this.onSelect(this.list[i])\r\n\r\n            }\r\n        },\r\n\r\n        onSelect(selection) {\r\n            this.$emit('input', selection)\r\n            this.$emit('onListSearchSelected', selection)\r\n            this.selectedid = selection.value\r\n            this.selected = selection\r\n        },\r\n\r\n        badgeVariant(select) {\r\n            return (select.value == this.selectedid) ? 'light' : 'primary'\r\n        }\r\n    },\r\n    \r\n    computed: {\r\n\r\n        rows() {\r\n            return this.list.length\r\n        }\r\n\r\n    }\r\n    \r\n}\r\n</script>"]}]}