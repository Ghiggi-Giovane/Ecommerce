\r\n      <b-card-group deck>\r\n        <div>\r\n          <b-card id=\"MaqName\" border-variant=\"dark\">\r\n            <b-card-text>\r\n              Máquina - {{ nomeMaq }}\r\n              <br />\r\n              Prioridade: {{ listItens[0].Ordenacao }}\r\n            </b-card-text>\r\n          </b-card>\r\n          <b-card\r\n            :class=\"MoldeManutencao(itens)\"\r\n            v-for=\"itens of listItens\"\r\n            :key=\"itens.id\"\r\n            border-variant=\"dark\"\r\n          >\r\n            {{ itens.OpMes }}\r\n            <br />\r\n            Peç.: {{ itens.ReferenciaFilha }}\r\n            <br />\r\n            <div id=\"div\" class=\"mb-1\">\r\n              <span v-if=\"corMolde(itens)\" class=\"border-layout\">\r\n                Molde: {{ itens.Molde }}\r\n              </span>\r\n              <span v-else> Molde: {{ itens.Molde }} </span>\r\n              <div>Box: {{ itens.Box }}</div>\r\n            </div>\r\n\r\n            <div\r\n              style=\"border-style: solid\"\r\n              v-for=\"item in itens.Itens\"\r\n              :key=\"item.Item\"\r\n            >\r\n              <div class=\"text-center\" style=\"white-space: break-spaces\">\r\n                <span v-if=\"corhoras(itens)\" class=\"sublinhado-azul\">\r\n                  {{ item.Descricao }}\r\n                </span>\r\n                <span v-else>\r\n                  {{ item.Descricao }}\r\n                </span>\r\n              </div>\r\n              <div id=\"div\">\r\n                <div>Item: {{ item.Item }}</div>\r\n                <div>KG: {{ item.Qtde }}</div>\r\n                <!-- <b-form-checkbox\r\n                  v-model=\"item.Marcacao\"\r\n                  name=\"check-button\"\r\n                  switch\r\n                  v-if=\"liberaMarcacao\"\r\n                  @change=\"\r\n                    MarcacaoComponente({\r\n                      SeqProdId: itens.SeqProdId,\r\n                      Item: item.Item.toString(),\r\n                      Marcacao: item.Marcacao,\r\n                      Referencia: item.itens.Referencia,\r\n                    })\r\n                  \"\r\n                >\r\n                </b-form-checkbox> -->\r\n              </div>\r\n              <div id=\"div\" v-if=\"item.CX_Papelao\">\r\n                <div>Papelão: {{ item.CX_Papelao }}</div>\r\n                <div>Qtde: {{ item.QtdePapelao * itens.Quantidade }}</div>\r\n                <b-form-checkbox\r\n                  v-model=\"item.Marcacao\"\r\n                  name=\"check-button\"\r\n                  switch\r\n                  v-if=\"liberaMarcacao\"\r\n                  @change=\"\r\n                    MarcacaoComponente({\r\n                      SeqProdId: itens.SeqProdId,\r\n                      Item: item.CX_Papelao.toString(),\r\n                      Marcacao: item.Marcacao,\r\n                      Referencia: itens.Referencia,\r\n                    })\r\n                  \"\r\n                >\r\n                </b-form-checkbox>\r\n              </div>\r\n              <div\r\n                v-for=\"componente in item.Componentes\"\r\n                :key=\"componente.Item\"\r\n              >\r\n                <div id=\"div\">\r\n                  <div>Comp: {{ componente.Item }}</div>\r\n                  <div>Qtde: {{ componente.Qtde * itens.Quantidade }}</div>\r\n                  <b-form-checkbox\r\n                    v-model=\"componente.Marcacao\"\r\n                    name=\"check-button\"\r\n                    switch\r\n                    v-if=\"liberaMarcacao\"\r\n                    @change=\"\r\n                      MarcacaoComponente({\r\n                        SeqProdId: itens.SeqProdId,\r\n                        Item: componente.Item.toString(),\r\n                        Marcacao: componente.Marcacao,\r\n                        Referencia: itens.Referencia,\r\n                      })\r\n                    \"\r\n                  >\r\n                  </b-form-checkbox>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <br />\r\n            Produzido:\r\n            {{ itens.Produzido }} / {{ itens.Quantidade }}\r\n            <b-progress :value=\"itens.Produzido\" :max=\"itens.Quantidade\">\r\n              <b-progress-bar :value=\"itens.Produzido\">\r\n                <span>\r\n                  <strong\r\n                    >{{ itens.Produzido }} / {{ itens.Quantidade }}</strong\r\n                  >\r\n                </span>\r\n              </b-progress-bar>\r\n            </b-progress>\r\n            <br />\r\n            Faltam: {{ itens.Extenso }}\r\n            <br />\r\n            <div id=\"div\">\r\n              <div>Tm: {{ itens.TipoMaterial }}</div>\r\n              <div :style=\"itens.CodigoCorHex\">COR: {{ itens.Cor }}</div>\r\n            </div>\r\n            <hr />\r\n            <div>\r\n              <!--\r\n            <hr v-show=\"itens.PrevisaoConclusao != null\" />\r\n            <div v-show=\"itens.PrevisaoConclusao != null\">\r\n              -->\r\n              Previsão:\r\n              <span v-if=\"itens.PrevisaoConclusao != null\">{{\r\n                new Date(itens.PrevisaoConclusao).toLocaleDateString()\r\n              }}</span>\r\n            </div>\r\n          </b-card>\r\n        </div>\r\n      </b-card-group>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport API from \"../../../services/api.service\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      maquina: \"Máquina \",\r\n    };\r\n  },\r\n  props: {\r\n    nomeMaq: { type: String },\r\n    listItens: { type: Array },\r\n  },\r\n\r\n  computed: {\r\n    liberaMarcacao() {\r\n      return localStorage.getItem(\"lastlogin\") == \"uzalmoxarifado\";\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    MoldeManutencao(itemMolde) {\r\n      if (itemMolde.MoldeEmManutencao == \"1\") {\r\n        if (itemMolde.PossuiReservaDisponivel == \"1\") {\r\n          return \"cardReserva\";\r\n        }\r\n        return \"cardManut\";\r\n      } else {\r\n        return \"card\";\r\n      }\r\n    },\r\n    MarcacaoComponente(componente) {\r\n      API.post(\r\n        \"v2/Servicos/MarcaComponenteSeparadaUzDashboard/Post\",\r\n        componente\r\n      ).then((api) => {\r\n        if (api.data.Success == \"True\" || api.data.Success) {\r\n        } else {\r\n          this.$root.toastdanger(api.data.Error);\r\n        }\r\n      });\r\n    },\r\n    corhoras(item) {\r\n      if (item.Horastime <= 480 && item.Produzido > 0) {\r\n        return item;\r\n      }\r\n    },\r\n    corMolde(item) {\r\n      return item.Fechamento == \"Sim\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card {\r\n  height: auto;\r\n  width: 300px;\r\n  margin-top: 5px;\r\n  background-color: aliceblue;\r\n  color: black;\r\n  white-space: nowrap;\r\n}\r\n.cardManut {\r\n  height: auto;\r\n  width: 300px;\r\n  margin-top: 5px;\r\n  background-color: red;\r\n  color: white;\r\n  white-space: nowrap;\r\n}\r\n.cardReserva {\r\n  height: auto;\r\n  width: 260px;\r\n  margin-top: 5px;\r\n  background-color: rgba(255, 136, 0, 0.795);\r\n  color: white;\r\n  white-space: nowrap;\r\n}\r\n#MaqName {\r\n  text-align: center;\r\n  size: 15px;\r\n  margin-top: 1%;\r\n  background-color: aliceblue;\r\n  color: black;\r\n}\r\n#div {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n@-webkit-keyframes glowing {\r\n  0% {\r\n    background-color: #ffffff;\r\n    -webkit-box-shadow: 0 0 3px #ffffff;\r\n    color: black;\r\n  }\r\n  50% {\r\n    background-color: rgba(16, 177, 164, 0.72);\r\n    -webkit-box-shadow: 0 0 3px rgba(16, 177, 164, 0.72);\r\n  }\r\n  100% {\r\n    background-color: #ffffff;\r\n    -webkit-box-shadow: 0 0 3px #ffffff;\r\n    color: black;\r\n  }\r\n}\r\n\r\n@-moz-keyframes glowing {\r\n  0% {\r\n    background-color: #ffffff;\r\n    -moz-box-shadow: 0 0 3px #ffffff;\r\n    color: black;\r\n  }\r\n  50% {\r\n    background-color: rgba(16, 177, 164, 0.72);\r\n    -moz-box-shadow: 0 0 3px rgba(16, 177, 164, 0.72);\r\n  }\r\n  100% {\r\n    background-color: #ffffff;\r\n    -moz-box-shadow: 0 0 3px #ffffff;\r\n    color: black;\r\n  }\r\n}\r\n\r\n@-o-keyframes glowing {\r\n  0% {\r\n    background-color: #ffffff;\r\n    box-shadow: 0 0 3px #ffffff;\r\n    color: black;\r\n  }\r\n  50% {\r\n    background-color: rgba(16, 177, 164, 0.72);\r\n    box-shadow: 0 0 3px rgba(16, 177, 164, 0.72);\r\n  }\r\n  100% {\r\n    background-color: #ffffff;\r\n    box-shadow: 0 0 3px #ffffff;\r\n    color: black;\r\n  }\r\n}\r\n@keyframes glowing {\r\n  0% {\r\n    background-color: #ffffff;\r\n    box-shadow: 0 0 3px #ffffff;\r\n    color: black;\r\n  }\r\n  50% {\r\n    background-color: rgba(16, 177, 164, 0.72);\r\n    box-shadow: 0 0 3px rgba(16, 177, 164, 0.72);\r\n  }\r\n  100% {\r\n    background-color: #ffffff;\r\n    box-shadow: 0 0 3px #ffffff;\r\n    color: black;\r\n  }\r\n}\r\n\r\n.sublinhado-azul {\r\n  -webkit-animation: glowing 1600ms infinite;\r\n  -moz-animation: glowing 1600ms infinite;\r\n  -o-animation: glowing 1600ms infinite;\r\n  animation: glowing 1600ms infinite;\r\n}\r\n\r\n.border-layout {\r\n  position: relative;\r\n}\r\n\r\n.border-layout::before,\r\n.border-layout::after {\r\n  content: \"\";\r\n  position: absolute;\r\n  top: -1px;\r\n  left: -6px;\r\n  right: -6px;\r\n  bottom: -1px;\r\n  border: 2px solid transparent;\r\n  border-radius: 10px;\r\n  animation: piscar 1s ease infinite;\r\n}\r\n\r\n.border-layout::before {\r\n  animation-delay: 0.2s;\r\n}\r\n\r\n@keyframes piscar {\r\n  0% {\r\n    border-color: #da2128;\r\n  }\r\n  25% {\r\n    border-color: #ffc0cb;\r\n  }\r\n\r\n  50% {\r\n    border-color: #da2128;\r\n  }\r\n\r\n  75% {\r\n    border-color: #ffc0cb;\r\n  }\r\n  100% {\r\n    border-color: #da2128;\r\n  }\r\n}\r\n</style>\r\n"]}]}