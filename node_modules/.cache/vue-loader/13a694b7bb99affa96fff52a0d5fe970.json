r(\"focus\", () => {\r\n      let token = \"\";\r\n      const cookies = document.cookie;\r\n      if (cookies) {\r\n        const cook = cookies\r\n          .split(\"; \")\r\n          .map((a) => a.split(\"=\"))\r\n          .map((r) => {\r\n            return { key: r[0], value: r[1] };\r\n          });\r\n        token = cook.find((f) => f.key === \"token\") || \"\";\r\n        if (token == \"\") {\r\n          this.disconnected++;\r\n        } else {\r\n          this.disconnected = 0;\r\n        }\r\n      }\r\n     });*/\r\n\r\n    // abre as stores ...\r\n    // this.onFireData(\"chat\");\r\n\r\n    // this.$cookies.set(\"anontoken\", \"\");\r\n  },\r\n\r\n  beforeMount() {\r\n    //console.log('App:beforeMount');\r\n    ApiService.loadObserver.subscribe(this.onloading);\r\n    ApiService.errorObserver.subscribe(this.onerror);\r\n    ApiService.notifyObserver.subscribe(this.onnotify);\r\n    ApiService.loginObserver.subscribe(this.onlogin);\r\n    //ApiService.chatObserver.subscribe(this.onchat);\r\n    //ApiService.apiReqObserver.subscribe(this.onapireq);\r\n    ApiService.apiRenewToken.subscribe(this.onapirenewtoken);\r\n\r\n    // Atualiza a store de configuração do sistema\r\n    this.actConfigGetFromLocalBase();\r\n    this.actEmpresasGetFromLocalBase();\r\n\r\n    // pede permissão para exibir notificações\r\n    Notification.requestPermission().then((choice) => {\r\n      console.log(\"Notification.requestPermission:\", choice);\r\n      if (choice !== \"granted\") {\r\n        // this.actToast({ message: \"Voce precisa permitir receber notificações !\", variant: \"warning\" });\r\n      }\r\n    });\r\n\r\n    this.actSidemenus();\r\n  },\r\n\r\n  destroyed() {\r\n    // this.onFireData(\"chat\");\r\n    ApiService.loadObserver.unsubscribe(this.onloading);\r\n    ApiService.errorObserver.unsubscribe(this.onerror);\r\n    ApiService.notifyObserver.unsubscribe(this.onnotify);\r\n    ApiService.loginObserver.unsubscribe(this.onlogin);\r\n    //ApiService.chatObserver.unsubscribe(this.onchat);\r\n    //ApiService.apiReqObserver.unsubscribe(this.onapireq);\r\n    ApiService.apiRenewToken.unsubscribe(this.onapirenewtoken);\r\n  },\r\n\r\n  computed: {\r\n    // \"isAuthenticated\",\r\n    ...mapGetters([\r\n      \"gtxSystemErrorLogLastMessage\",\r\n      \"gtxSystemErrorLogLastError\",\r\n      \"gtxSystemErrorLog\",\r\n      \"gtxalert\",\r\n      \"gtxguardroute\",\r\n      \"gtxAuthisLoggedin\",\r\n      \"gtxLoading\",\r\n      \"gtxLoadingVisible\",\r\n      \"gtxLastRoute\",\r\n    ]),\r\n\r\n    SrcY() {\r\n      return window.scrollY;\r\n    },\r\n\r\n    // JWTtoken: JWT.token,\r\n    //this.tokenPayload = this.parsePayload(this.token);\r\n    //this.tokenIsValid = this.isTokenValid(this.token);\r\n    //this.tokenObject = this.getParsedToken(this.token);\r\n  },\r\n\r\n  watch: {\r\n    focused(focus) {\r\n      if (focus) {\r\n        // this.$refs.MainNav.buscalinks();\r\n        // this.actSidemenus();\r\n      }\r\n    },\r\n    gtxalert(alert) {\r\n      this.$root.toast(alert.message, alert);\r\n    },\r\n    //  isAuthenticated(auth) {\r\n    //    // this.onFireData(\"chat\");\r\n    //  },\r\n  },\r\n\r\n  methods: {\r\n    // \"userLogin\",\r\n    ...mapActions([\r\n      \"actLoginAuthOff\",\r\n      \"actLoginAnon\",\r\n      \"actSystemErrorLog\",\r\n      \"onFireData\",\r\n      \"actParemp\",\r\n      \"actLoadingVisible\",\r\n      \"setloading\",\r\n      \"seterrors\",\r\n      \"actToast\",\r\n      // \"actLoginAuthSet\",//não existe essa action\r\n      \"actSidemenus\",\r\n      \"actConfigGetFromLocalBase\",\r\n      \"actEmpresasGetFromLocalBase\",\r\n    ]),\r\n\r\n    handleScroll() {\r\n      this.scrollY = window.scrollY;\r\n    },\r\n\r\n    scrollTop() {\r\n      window.scrollTo({ top: 0, behavior: \"smooth\" });\r\n    },\r\n\r\n    navlogout(evt) {\r\n      // console.log('navlogout.evt:', evt);\r\n      this.actLoginAuthOff();\r\n      ApiService.setAnonHeader();\r\n      //this.actSidemenus();\r\n      if (this.$router.currentRoute.path !== \"/\")\r\n        this.$router.push({ path: \"/\" });\r\n    },\r\n\r\n    navidioma(evt) {\r\n      // console.log('App.navidioma: ', evt)\r\n      this.$i18n.locale = evt.lang;\r\n      // this.$cookies.set('language', evt.lang);\r\n      // this.$moment.locale(evt.lang);\r\n    },\r\n\r\n    onLoadHide() {\r\n      this.actLoadingVisible(false);\r\n    },\r\n\r\n    onloading(value) {\r\n      // console.log(\"App.onloading: [\", value, \"]\");\r\n      if (this.showloading !== value) {\r\n        this.showloading = value;\r\n      }\r\n      // this.showloading = true\r\n    },\r\n\r\n    onnotify(payload) {\r\n      // console.log(\"App.onnotify:\", payload);\r\n\r\n      let alert = {\r\n        title: \"Intranet\",\r\n        icon: \"info-square\",\r\n        variant: \"info\",\r\n        appendToast: true,\r\n        title: \"Atenção\",\r\n        message: \"\",\r\n      };\r\n\r\n      if (typeof payload == \"string\") {\r\n        alert.message = payload || \"Ocorreu um erro neste processo\";\r\n      }\r\n\r\n      if (typeof payload == \"object\") {\r\n        alert.title = payload.title || \"Intranet\";\r\n        alert.icon = payload.icon || \"info-square\";\r\n        alert.variant = payload.variant || \"info\";\r\n        alert.appendToast = payload.appendToast || true;\r\n        alert.message = payload.message || \"Ocorreu um erro neste processo\";\r\n      }\r\n\r\n      // this.actToast(alert);\r\n      this.$root.toast(alert.message, alert);\r\n    },\r\n\r\n    onchat(value) {\r\n      console.log(\"onchat:\", value);\r\n      // this.actToast(value);\r\n    },\r\n\r\n    onapireq(value) {\r\n      console.log(\"onapireq:\", value);\r\n      //this.ApiService.setLocalDBDocument(value.params, value.url.split('/').join(''));\r\n    },\r\n\r\n    onapirenewtoken(value) {\r\n      console.log(\"onapirenewtoken:\", value);\r\n      // this.ApiService.setLocalDBDocument(value.params, value.url.split('/').join(''));\r\n      // this.actLoginAnon();\r\n    },\r\n\r\n    onlogin(auth) {\r\n      // TODO: não tá caindo aqui\r\n      console.log(\"onlogin:\", auth);\r\n      // this.actLoginAuthSet(auth);//não existe essa action\r\n      if (auth) {\r\n        this.actSidemenus();\r\n        this.actParemp();\r\n        this.disconnected = 2;\r\n      } else {\r\n        this.disconnected = 0;\r\n      }\r\n    },\r\n\r\n    onerror(error) {\r\n      // console.log(\"App.onerror:\", error);\r\n\r\n      /*\r\n      if (error.message)\r\n      this.$root.toastdanger(error.message);\r\n      if (error.response)\r\n      this.$root.toastdanger(error.response.data);\r\n\r\n      if (error.variant == \"danger\") {\r\n        this.seterrors(error);\r\n      } else {\r\n        // console.log(\"App.actToast:\", error);\r\n        this.actToast(error);\r\n      }\r\n      if (\r\n        error.message == \"Usuário não autorizado a utilizar esse método!\" ||\r\n        error.message == \"Erro na autenticação, verifique usuário e senha\"\r\n      ) {\r\n        // console.log(\"App.onerror.error.route:\", error.route);\r\n        // console.log(\"App.$router.currentRoute.path:\", this.$router.currentRoute.path);\r\n        // console.log(\"App.gtxLastRoute:\", this.gtxLastRoute);\r\n\r\n        if (this.gtxLastRoute !== \"\" && this.gtxLastRoute !== \"/\") {\r\n          this.$router.push({\r\n            name: \"Login\",\r\n            query: { redir: this.gtxLastRoute },\r\n          });\r\n        } else {\r\n          this.$router.push({ name: \"Login\" });\r\n        }\r\n      }\r\n      if (error.status == 401) {\r\n        const rota = this.$router.currentRoute.path.replace(\"/\", \"\");\r\n        this.$router.push({\r\n          name: \"Login\",\r\n          query: { redir: rota },\r\n        });\r\n      }\r\n      */\r\n      if ([0, 401, 404].includes(error.status)) {\r\n        this.$root.toastdanger(error.message);\r\n      }\r\n      if (error.status == 400) {\r\n        this.$root.toastdanger(error.data);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n@font-face {\r\n  font-family: \"Din Pro Regular\";\r\n  src: url(\"assets/fonts/fontsfree-net-dinpro-1.woff2\") format(\"woff2\"),\r\n    url(\"assets/fonts/fontsfree-net-dinpro-1.woff\") format(\"woff\");\r\n  font-weight: normal;\r\n  font-style: normal;\r\n}\r\n\r\nhtml {\r\n  margin: 0 auto;\r\n  height: 100%;\r\n  display: flex;\r\n  justify-content: stretch;\r\n  align-items: stretch;\r\n  font-family: \"Din Pro Regular\";\r\n}\r\n\r\nbody {\r\n  height: 100vh;\r\n  width: 100%;\r\n  min-height: 100%;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n/* label {\r\n  margin-bottom: 0 !important;\r\n  margin-top: 0.5rem;\r\n} */\r\n\r\n.fade-enter-active,\r\n.fade-leave-active {\r\n  transition-duration: 0.3s;\r\n  transition-property: opacity;\r\n  transition-timing-function: ease;\r\n}\r\n\r\n.fade-enter,\r\n.fade-leave-active {\r\n  opacity: 0;\r\n}\r\n\r\n#app {\r\n  font-family: \"Din Pro Regular\", Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  color: rgb(72, 81, 86);\r\n}\r\n\r\n.form-group > label {\r\n  margin: 0; /* o label estava com uma margem alta sobre o input */\r\n}\r\n\r\n.router {\r\n  background-repeat: no-repeat;\r\n  background-attachment: fixed;\r\n  background-position: center;\r\n  background-size: 40%;\r\n  min-height: 100vh;\r\n}\r\n\r\n.btnTop {\r\n  position: fixed;\r\n  z-index: 9999;\r\n  bottom: 20px;\r\n  right: 30px;\r\n  opacity: 0.4;\r\n}\r\n\r\n.btnTop:hover {\r\n  opacity: 1;\r\n}\r\n\r\n#nav a {\r\n  /* padding: 30px; */\r\n\r\n  font-weight: bold;\r\n  color: #2c3e50;\r\n}\r\n\r\n.fullpage {\r\n  height: 100%;\r\n  width: 100%;\r\n}\r\n\r\n.spaced > * {\r\n  margin: 2px;\r\n}\r\n.spaced-4 > * {\r\n  margin: 4px;\r\n}\r\n.hspaced > * {\r\n  margin-left: 2px;\r\n  margin-right: 2px;\r\n}\r\n.hspaced-4 > * {\r\n  margin-