      </canvas>\r\n      <b-img ref=\"signatureIMG\" v-show=\"!sigImageEmpty\" :src=\"sigImg\" fluid />\r\n      <div id=\"signature-footer\">\r\n        <span\r\n          class=\"iconbutton\"\r\n          v-show=\"sigImageEmpty\"\r\n          @click=\"sendSignature\"\r\n          v-if=\"buttons.includes('commit')\"\r\n          ><b-icon-file-earmark-check\r\n            variant=\"success\"\r\n            :disabled=\"!signatureValid\"\r\n        /></span>\r\n        <span\r\n          class=\"iconbutton\"\r\n          @click=\"clearCanvas\"\r\n          v-if=\"buttons.includes('clear')\"\r\n          ><b-icon-trash variant=\"danger\" :disabled=\"!signatureValid\"\r\n        /></span>\r\n        <span class=\"iconbutton\" @click=\"showcanva\"\r\n          ><b-icon-person variant=\"success\"\r\n        /></span>\r\n      </div>\r\n    </div>\r\n    <!-- <pre>\r\n      width/height: {{width}}/{{height}}\r\n    </pre> -->\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"Signature\",\r\n  props: {\r\n    value: {\r\n      type: String,\r\n      default: \"\",\r\n    },\r\n    autocommit: {\r\n      type: Boolean,\r\n      default: true,\r\n    },\r\n    buttons: {\r\n      type: Array,\r\n      default() {\r\n        return [\"commit\", \"clear\"];\r\n      },\r\n    },\r\n    sigImage: {\r\n      type: String,\r\n      default: null,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      width: 0,\r\n      height: 0,\r\n      canvas: null,\r\n      ctx: null,\r\n      painting: false,\r\n      sigText: null,\r\n      sigImg: this.sigImage || \"\",\r\n      x: 0,\r\n      y: 0,\r\n      url: \"https://codesource.io/build-a-drawing-app-with-and-vuejs-html5-canvas/\",\r\n    };\r\n  },\r\n  computed: {\r\n    signatureValid() {\r\n      return this.sigText && this.sigText !== \"\";\r\n    },\r\n    sigImageEmpty() {\r\n      return this.sigImg == \"\";\r\n    },\r\n    signatureIMG() {\r\n      return !this.sigImageEmpty ? \"signatureIMG\" : \"\";\r\n    },\r\n  },\r\n  mounted() {\r\n    this.canvas = this.$refs[\"signature\"];\r\n    if (this.canvas) {\r\n      let { width } = this.canvas.parentNode.getBoundingClientRect();\r\n      if (width > 0) {\r\n        this.canvas.width = width;\r\n        this.canvas.height = 150;\r\n        this.ctx = this.canvas.getContext(\"2d\");\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    showcanva() {\r\n      console.log(\"canva:\", this.canvas.toDataURL());\r\n    },\r\n    resize(w, h) {\r\n      let { width, height } = this.canvas.parentNode.getBoundingClientRect();\r\n      if (width == 0 && w > 0) width = w;\r\n\r\n      console.log(\"resize:\", width);\r\n      if (width > 0) {\r\n        this.canvas.width = width;\r\n        this.canvas.height = 150;\r\n        this.ctx = this.canvas.getContext(\"2d\");\r\n      }\r\n    },\r\n    startPainting(e) {\r\n      if (this.ctx) {\r\n        document.documentElement.style.overflow = \"hidden\";\r\n        this.painting = true;\r\n        this.x = e.offsetX;\r\n        this.y = e.offsetY;\r\n        this.$emit(\"evtSignatureChange\", true);\r\n      }\r\n    },\r\n    draw(e) {\r\n      if (!this.painting) return;\r\n\r\n      const { x, y, width, height } = e.target.getBoundingClientRect();\r\n      const offsetX =\r\n        e.offsetX ||\r\n        (e.touches\r\n          ? ((e.touches[0].clientX - x) / width) * e.target.offsetWidth\r\n          : 0);\r\n      const offsetY =\r\n        e.offsetY ||\r\n        (e.touches\r\n          ? ((e.touches[0].clientY - y) / height) * e.target.offsetHeight\r\n          : 0);\r\n\r\n      this.drawLine(this.x, this.y, offsetX, offsetY);\r\n      this.x = offsetX;\r\n      this.y = offsetY;\r\n    },\r\n    drawLine(x1, y1, x2, y2) {\r\n      // let ctx = this.ctx;\r\n      this.ctx.beginPath();\r\n      this.ctx.strokeStyle = \"dodgerblue\";\r\n      this.ctx.lineWidth = 3;\r\n      this.ctx.moveTo(x1, y1);\r\n      this.ctx.lineTo(x2, y2);\r\n      this.ctx.stroke();\r\n      this.ctx.closePath();\r\n    },\r\n    finishedPainting(e) {\r\n      if (this.painting) {\r\n        this.drawLine(this.x, this.y, e.offsetX, e.offsetY);\r\n        this.x = 0;\r\n        this.y = 0;\r\n        this.painting = false;\r\n        document.documentElement.style.overflow = \"auto\";\r\n        if (this.autocommit) {\r\n          this.sendSignature();\r\n        }\r\n      }\r\n    },\r\n    clearCanvas() {\r\n      let { width } = this.canvas.parentNode.getBoundingClientRect();\r\n      this.canvas.width = width;\r\n      this.canvas.height = 150;\r\n      this.ctx = this.canvas.getContext(\"2d\");\r\n\r\n      this.ctx.clearRect(0, 0, this.canvas.width, this.canvas.height);\r\n      this.sigText = null;\r\n      this.sigImg = \"\";\r\n      this.$emit(\"input\", \"\");\r\n    },\r\n    sendSignature() {\r\n      let dataUrl = this.canvas.toDataURL();\r\n      this.sigText = dataUrl;\r\n      this.$emit(\"input\", dataUrl);\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n#signature {\r\n  background-color: floralwhite;\r\n  border: 1px dashed #cccccc;\r\n  border-radius: 2px;\r\n  cursor: pointer;\r\n  margin-top: 0;\r\n}\r\n.signatureIMG {\r\n  background-color: lightgray !important;\r\n}\r\n#signature-footer {\r\n  text-align: right;\r\n  margin-top: -8px;\r\n  border-top: 1px solid #cccccc;\r\n  background-image: linear-gradient(antiquewhite, lightgrey);\r\n  padding-top: 7px;\r\n  padding-bottom: 5px;\r\n  padding-right: 2px;\r\n}\r\n</style>\r\n"]}]}