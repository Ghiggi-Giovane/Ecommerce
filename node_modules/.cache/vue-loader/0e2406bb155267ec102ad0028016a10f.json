   // this.onFireData(\"chat\");\r\n\r\n    // abre a autenticação anônima\r\n    this.actLoginAnon();\r\n  },\r\n\r\n  beforeMount() {\r\n    ApiService.loadObserver.subscribe(this.onloading);\r\n    ApiService.errorObserver.subscribe(this.onerror);\r\n    ApiService.notifyObserver.subscribe(this.onnotify);\r\n    ApiService.loginObserver.subscribe(this.onlogin);\r\n    ApiService.chatObserver.subscribe(this.onchat);\r\n    ApiService.apiReqObserver.subscribe(this.onapireq);\r\n    ApiService.apiRenewToken.subscribe(this.onapirenewtoken);\r\n\r\n    // pede permissão para exibir notificações\r\n    Notification.requestPermission().then((choice) => {\r\n      console.log(\"Notification.requestPermission:\", choice);\r\n      if (choice !== \"granted\") {\r\n        // this.actToast({ message: \"Voce precisa permitir receber notificações !\", variant: \"warning\" });\r\n      }\r\n    });\r\n  },\r\n\r\n  destroyed() {\r\n    // this.onFireData(\"chat\");\r\n    ApiService.loadObserver.unsubscribe(this.onloading);\r\n    ApiService.errorObserver.unsubscribe(this.onerror);\r\n    ApiService.notifyObserver.unsubscribe(this.onnotify);\r\n    ApiService.loginObserver.unsubscribe(this.onlogin);\r\n    ApiService.chatObserver.unsubscribe(this.onchat);\r\n    ApiService.apiReqObserver.unsubscribe(this.onapireq);\r\n    ApiService.apiRenewToken.unsubscribe(this.onapirenewtoken);\r\n  },\r\n\r\n  computed: {\r\n    // \"isAuthenticated\",\r\n    ...mapGetters([\r\n      \"gtxSystemErrorLogLastMessage\",\r\n      \"gtxSystemErrorLogLastError\",\r\n      \"gtxSystemErrorLog\",\r\n      \"gtxalert\",\r\n      \"gtxguardroute\",\r\n      \"gtxAuthisLoggedin\",\r\n      \"gtxLoading\",\r\n      \"gtxLoadingVisible\",\r\n      \"gtxLastRoute\",\r\n    ]),\r\n\r\n    SrcY() {\r\n      return window.scrollY;\r\n    },\r\n\r\n    // JWTtoken: JWT.token,\r\n    //this.tokenPayload = this.parsePayload(this.token);\r\n    //this.tokenIsValid = this.isTokenValid(this.token);\r\n    //this.tokenObject = this.getParsedToken(this.token);\r\n  },\r\n\r\n  //watch: {\r\n  //  isAuthenticated(auth) {\r\n  //    // this.onFireData(\"chat\");\r\n  //  },\r\n  //},\r\n\r\n  methods: {\r\n    // \"userLogin\",\r\n    ...mapActions([\r\n      \"actLoginAuthOff\",\r\n      \"actLoginAnon\",\r\n      \"actSystemErrorLog\",\r\n      \"onFireData\",\r\n      \"actParemp\",\r\n      \"actLoadingVisible\",\r\n      \"setloading\",\r\n      \"seterrors\",\r\n      \"actToast\",\r\n      // \"actLoginAuthSet\",//não existe essa action\r\n    ]),\r\n\r\n    handleScroll() {\r\n      this.scrollY = window.scrollY;\r\n    },\r\n\r\n    scrollTop() {\r\n      window.scrollTo({ top: 0, behavior: \"smooth\" });\r\n    },\r\n\r\n    navlogout(evt) {\r\n      this.actLoginAuthOff();\r\n      if (this.$router.currentRoute.path !== \"/\")\r\n        this.$router.push({ path: \"/\" });\r\n    },\r\n\r\n    navidioma(evt) {\r\n      // console.log('App.navidioma: ', evt)\r\n      this.$i18n.locale = evt.lang;\r\n      // this.$cookies.set('language', evt.lang);\r\n      // this.$moment.locale(evt.lang);\r\n    },\r\n\r\n    onLoadHide() {\r\n      this.actLoadingVisible(false);\r\n    },\r\n\r\n    onloading(value) {\r\n      // console.log(\"App.onloading: [\", value, \"]\");\r\n      if (this.showloading !== value) {\r\n        this.showloading = value;\r\n      }\r\n    },\r\n\r\n    onnotify(value) {\r\n      console.log(\"App.onnotify:\", value);\r\n      this.actToast(value);\r\n    },\r\n\r\n    onchat(value) {\r\n      // console.log(\"App.onnotify:\", value);\r\n      // console.log(\"onchat:\", value);\r\n      // this.actToast(value);\r\n    },\r\n\r\n    onapireq(value) {\r\n      console.log(\"onapireq:\", value);\r\n      //this.ApiService.setLocalDBDocument(value.params, value.url.split('/').join(''));\r\n    },\r\n\r\n    onapirenewtoken(value) {\r\n      console.log(\"onapirenewtoken:\", value);\r\n      //this.ApiService.setLocalDBDocument(value.params, value.url.split('/').join(''));\r\n    },\r\n\r\n    onlogin(auth) {\r\n      // console.log(\"onlogin:\", auth);\r\n      // this.actLoginAuthSet(auth);//não existe essa action\r\n      if (auth) {\r\n        this.actParemp();\r\n        this.disconnected = 2;\r\n      } else {\r\n        this.disconnected = 0;\r\n      }\r\n    },\r\n\r\n    onerror(error) {\r\n      console.log(\"App.onerror:\", error);\r\n      /*\r\n      if (error.variant == \"danger\") {\r\n        this.seterrors(error);\r\n      } else {\r\n        // console.log(\"App.actToast:\", error);\r\n        this.actToast(error);\r\n      }\r\n      if (\r\n        error.message == \"Usuário não autorizado a utilizar esse método!\" ||\r\n        error.message == \"Erro na autenticação, verifique usuário e senha\"\r\n      ) {\r\n        // console.log(\"App.onerror.error.route:\", error.route);\r\n        // console.log(\"App.$router.currentRoute.path:\", this.$router.currentRoute.path);\r\n        // console.log(\"App.gtxLastRoute:\", this.gtxLastRoute);\r\n\r\n        if (this.gtxLastRoute !== \"\" && this.gtxLastRoute !== \"/\") {\r\n          this.$router.push({\r\n            name: \"Login\",\r\n            query: { redir: this.gtxLastRoute },\r\n          });\r\n        } else {\r\n          this.$router.push({ name: \"Login\" });\r\n        }\r\n      }\r\n      */\r\n      if (error.status == 401) {\r\n        const rota = this.$router.currentRoute.path.replace(\"/\", \"\");\r\n        this.$router.push({\r\n          name: \"Login\",\r\n          query: { redir: rota },\r\n        });\r\n      }\r\n      if (error.status == 404) {\r\n        this.$root.toastdanger(error.message);\r\n      }\r\n      if (error.status == 401) {\r\n        this.$root.toastdanger(error.message);\r\n      }\r\n      if (error.status == 400) {\r\n        this.$root.toastdanger(error.data);\r\n      }\r\n      if (error.status == 0) {\r\n        this.$root.toastdanger(error.message);\r\n      }\r\n    },\r\n  },\r\n};\r\n