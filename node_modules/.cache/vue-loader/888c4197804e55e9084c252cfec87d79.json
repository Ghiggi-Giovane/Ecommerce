 || this.user.Password === null\r\n        ? true\r\n        : this.user.User === \"\" || this.user.Password === \"\";\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    ...mapActions([\"actLoginAuth\", \"actLoginAuthOff\", \"actAlert\"]),\r\n\r\n    login() {\r\n      this.actLoginAuth(this.user);\r\n    },\r\n\r\n    showpass(pass) {\r\n      const input = this.$refs.passwordInput.$refs.input;\r\n      if (pass) {\r\n        input.setAttribute(\"type\", \"text\");\r\n      } else {\r\n        input.setAttribute(\"type\", \"password\");\r\n      }\r\n    },\r\n\r\n    onForgotPassword(msg) {\r\n      // this.$bvToast.toast(msg, { title: 'Psiu!', variant: 'info', solid: true })\r\n      // this.$store.appToast (msg, 'Psiu', 'info')\r\n      this.actAlert({ title: \"Psiu!\", variant: \"info\", message: msg });\r\n    },\r\n\r\n    isCaps(e) {\r\n      if (e && e.type && e.type == \"keyup\") {\r\n        // removido pois tem uns taipa que não aceitam essa função\r\n        // this.caps = e.getModifierState(\"CapsLock\");\r\n      }\r\n    },\r\n\r\n    /*\r\n    onFormFocus() {\r\n      console.log(\"onFormFocus\");\r\n      window.addEventListener(\"keyup\", this.isCaps);\r\n    },\r\n\r\n    onFormBlur() {\r\n      console.log(\"onFormBlur\");\r\n      window.removeEventListener(\"keyup\", this.isCaps);\r\n    },\r\n    */\r\n\r\n    doInputFocus() {\r\n      // console.log(\"doInputFocus:\", this.user);\r\n      if (this.user.User) {\r\n        this.$refs.passwordInput.focus();\r\n      } else {\r\n        this.$refs.usernameInput.focus();\r\n      }\r\n    },\r\n  },\r\n\r\n  watch: {\r\n    gtxLoginHits(hits) {\r\n      // console.log(\"Login.watch.gtxLoginHits\", hits);\r\n      this.user.Password = null;\r\n      if (this.gtxloginAuth) this.user.User = null;\r\n      this.$emit(\"evt-Login\", { auth: this.gtxloginAuth });\r\n    },\r\n    pass(pass) {\r\n      // console.log(\"pass:\", pass);\r\n      this.showpass(pass);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<