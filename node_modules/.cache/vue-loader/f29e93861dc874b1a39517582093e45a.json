s=\"flex-column align-items-start\"\r\n        >\r\n          <PanelEmail\r\n            :key=\"k\"\r\n            :de=\"a.de\"\r\n            :data=\"a.data\"\r\n            :assunto=\"a.assunto\"\r\n            :mensagem=\"a.mensagem\"\r\n            variant=\"danger\"\r\n          >\r\n            <template #actions>\r\n              <div>\r\n                <b-button variant=\"info\" size=\"sm\" @click=\"onreply(a)\"\r\n                  ><b-icon-chat-left-dots\r\n                /></b-button>\r\n                <b-button\r\n                  class=\"mx-1\"\r\n                  variant=\"danger\"\r\n                  size=\"sm\"\r\n                  @click=\"ondelete(a.id)\"\r\n                  ><b-icon-trash\r\n                /></b-button>\r\n              </div>\r\n            </template>\r\n          </PanelEmail>\r\n        </b-list-group-item>\r\n      </b-list-group>\r\n    </b-card>\r\n\r\n    <b-modal\r\n      id=\"modal-caixasaida\"\r\n      ref=\"modal-caixasaida\"\r\n      title=\"Enviar notificação\"\r\n      size=\"lg\"\r\n      variant=\"dark\"\r\n      hide-footer\r\n    >\r\n      <b-form @submit=\"onSubmit\" @reset=\"onReset\">\r\n        <b-form-group\r\n          id=\"input-group-1\"\r\n          label=\"Para\"\r\n          label-for=\"input-1\"\r\n          label-cols=\"12\"\r\n          label-cols-lg=\"2\"\r\n          label-align-lg=\"right\"\r\n        >\r\n          <b-form-input\r\n            id=\"input-1\"\r\n            v-model=\"form.para\"\r\n            type=\"text\"\r\n            placeholder=\"Informe o destinatário\"\r\n            required\r\n          ></b-form-input>\r\n        </b-form-group>\r\n\r\n        <b-form-group\r\n          id=\"input-group-2\"\r\n          label=\"Assunto\"\r\n          label-for=\"input-2\"\r\n          label-cols=\"12\"\r\n          label-cols-lg=\"2\"\r\n          label-align-lg=\"right\"\r\n        >\r\n          <b-form-input\r\n            id=\"input-2\"\r\n            v-model=\"form.assunto\"\r\n            placeholder=\"Informe o assunto\"\r\n            required\r\n          ></b-form-input>\r\n        </b-form-group>\r\n\r\n        <b-form-group\r\n          id=\"input-group-3\"\r\n          label=\"Mensagem\"\r\n          label-for=\"input-3\"\r\n          label-cols=\"12\"\r\n          label-cols-lg=\"2\"\r\n          label-align-lg=\"right\"\r\n        >\r\n          <b-form-textarea\r\n            id=\"input-3\"\r\n            v-model=\"form.mensagem\"\r\n            rows=\"6\"\r\n            max-rows=\"12\"\r\n            required\r\n          ></b-form-textarea>\r\n        </b-form-group>\r\n\r\n        <div class=\"float-right\">\r\n          <b-button type=\"submit\" variant=\"primary\" class=\"mx-1\"\r\n            >Enviar</b-button\r\n          >\r\n          <b-button type=\"reset\" variant=\"danger\" class=\"mx-1\">Reset</b-button>\r\n        </div>\r\n      </b-form>\r\n    </b-modal>\r\n  </div>\r\n</template>\r\n<script>\r\nimport PanelEmail from \"../../../components/PanelEmail\";\r\nimport { mapActions, mapGetters } from \"vuex\";\r\nexport default {\r\n  name: \"Caixasaida\",\r\n\r\n  components: {\r\n    PanelEmail,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      form: {\r\n        para: \"\",\r\n        assunto: \"\",\r\n        mensagem: \"\",\r\n      },\r\n      caixasaida: [\r\n        {\r\n          id: 1,\r\n          de: \"Paulo\",\r\n          data: \"23/10/2021 18:30\",\r\n          assunto: \"Verificar sua caixa de e-mail\",\r\n          mensagem:\r\n            \"Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nunc venenatis ultricies erat, sit amet fermentum metus ullamcorper vel.\",\r\n          lida: false,\r\n        },\r\n        {\r\n          id: 2,\r\n          de: \"Lucas\",\r\n          data: \"21/10/2021 15:32\",\r\n          assunto: \"Ligar para o ramal 8842\",\r\n          mensagem:\r\n            \"Etiam ante est, porttitor eu maximus eu, consequat et mi. Nam id elementum risus.\",\r\n          lida: false,\r\n        },\r\n        {\r\n          id: 3,\r\n          de: \"Anderson\",\r\n          data: \"22/10/2021 10:45\",\r\n          assunto: \"Entrar em contato com Rafael\",\r\n          mensagem:\r\n            \"Phasellus sagittis nisi in metus aliquet malesuada. Nam ullamcorper eleifend nulla et convallis.\",\r\n          lida: false,\r\n        },\r\n      ],\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\"gtxcaixasaida\"]),\r\n  },\r\n\r\n  beforeMount() {\r\n    this.actcaixasaida(this.caixasaida);\r\n  },\r\n\r\n  methods: {\r\n    ...mapActions([\"actcaixasaida\", \"actcaixasaidadelete\"]),\r\n    onSubmit(event) {\r\n      event.preventDefault();\r\n      console.log(\"form:\", JSON.stringify(this.form));\r\n    },\r\n    onReset(event) {\r\n      event.preventDefault();\r\n      // Reset our form values\r\n      this.form.para = \"\";\r\n      this.form.assunto = \"\";\r\n      this.form.mensagem = null;\r\n    },\r\n    onnewmessage(item) {\r\n      this.form.para = \"\";\r\n      this.form.assunto = \"\";\r\n      this.form.mensagem = \"\";\r\n      this.$bvModal.show(\"modal-caixasaida\");\r\n    },\r\n    onreply(item) {\r\n      this.form.para = item.de;\r\n      this.form.assunto = `Re: ${item.assunto}`;\r\n      this.form.mensagem = `\\n»»-- Mensagem original -->\\n${item.mensagem}`;\r\n      this.$bvModal.show(\"modal-caixasaida\");\r\n    },\r\n    ondelete(id) {\r\n      this.$root\r\n        .showmessage(\"Deseja realmente excluir essa notificação ?\", \"Intranet\")\r\n        .then((choice) => {\r\n          if (choice) this.actcaixasaidadelete(id);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}