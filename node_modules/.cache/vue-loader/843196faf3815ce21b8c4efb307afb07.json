\r\n            :options=\"variants\"\r\n          ></b-form-select>\r\n        </b-input-group>\r\n      </b-col>\r\n    </b-row>\r\n    <b-row>\r\n      <b-col v-for=\"(i, idx) of iconslist\" :key=\"idx\">\r\n        <b-card\r\n          :class=\"cardbg\"\r\n          class=\"my-1 text-center hover-1\"\r\n          style=\"width: 150px; height: 110px;\"\r\n        >\r\n          <b-icon class=\"h3 mt-2\" :variant=\"variant\" :icon=\"i.icon\"></b-icon>\r\n          <b-card-text :class=\"cardtext\" style=\"font-size: 12px;\" v-copy>\r\n            {{ i.icon }}\r\n          </b-card-text>\r\n        </b-card>\r\n      </b-col>\r\n    </b-row>\r\n  </div>\r\n</template>\r\n<script>\r\nimport json from \"../../../assets/json/bootstrapicons.json\";\r\nexport default {\r\n  name: \"Icons\",\r\n  data() {\r\n    return {\r\n      filtro: \"\",\r\n      filtrodelay: 1000,\r\n      variant: \"light\",\r\n      variants: [\r\n        { value: \"primary\", text: \"Primary\" },\r\n        { value: \"secondary\", text: \"Secondary\" },\r\n        { value: \"success\", text: \"Success\" },\r\n        { value: \"info\", text: \"Info\" },\r\n        { value: \"warning\", text: \"Warning\" },\r\n        { value: \"danger\", text: \"Danger\" },\r\n        { value: \"light\", text: \"Light\" },\r\n      ],\r\n      icons: json,\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    cardbg() {\r\n      return [\"light\", \"warning\"].includes(this.variant)\r\n        ? \"bg-secondary\"\r\n        : \"bg-light\";\r\n    },\r\n    cardtext() {\r\n      return ![\"light\", \"warning\"].includes(this.variant)\r\n        ? \"text-secondary\"\r\n        : \"text-light\";\r\n    },\r\n    lowfilter() {\r\n      return this.filtro.toLocaleLowerCase();\r\n    },\r\n    iconslist() {\r\n      return this.lowfilter !== \"\"\r\n        ? this.icons.filter((i) => i.icon.includes(this.lowfilter))\r\n        : this.icons;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.hover-1:hover svg {\r\n 