.data.Data;\r\n        } else {\r\n          this.paramEmbalagem = [];\r\n        }\r\n      });\r\n    },\r\n    buscarCaixas() {\r\n      API.get(\"v2/Servicos/ImpressaoPadraoEmbalagemCaixas/Get\", {\r\n        Modelo: this.params.Modelo,\r\n      }).then((api) => {\r\n        if (api.data.Success == \"True\" || api.data.Success) {\r\n          this.caixas = api.data.Data;\r\n        } else {\r\n          this.caixas = [];\r\n        }\r\n      });\r\n    },\r\n    buscarMedidasPlastico() {\r\n      API.get(\r\n        \"v2/Servicos/ImpressaoPadraoEmbalagemPlasticos/Get\",\r\n        this.params\r\n      ).then((api) => {\r\n        if (api.data.Success == \"True\" || api.data.Success) {\r\n          this.plasticos = api.data.Data;\r\n        } else {\r\n          this.plasticos = [];\r\n        }\r\n      });\r\n    },\r\n    enableButton: function () {\r\n      this.disabledButton = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.overflow-hidden {\r\n  margin-top: 3%;\r\n  margin-bottom: 2%;\r\n  border-radius: 10px;\r\n  border-width: 3px;\r\n  border-color: #8c201e;\r\n  min-width: 1000px;\r\n}\r\n.form-input {\r\n  border: 1px solid black;\r\n}\r\n.form-group {\r\n  font-size: 14px;\r\n  color: black;\r\n  font-weight: bold;\r\n}\r\n.card-layout {\r\n  border-width: 2px;\r\n  border-color: black;\r\n}\r\nH6 {\r\n  font-family: \"Monaco\", monospace;\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n  color: black;\r\n  text-align: center;\r\n}\r\n.tablecolor {\r\n  border-color: black;\r\n  border-width: 2px;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n}\r\n.tamanho-plastico {\r\n  text-align: center;\r\n}\r\n.v-for-plasticos {\r\n  margin-top: -10px;\r\n  margin-bottom: 2px;\r\n  font-family: \"Monaco\", monospace;\r\n  font-size: 22px;\r\n  font-weight: bold;\r\n  color: black;\r\n}\r\n</style>\r\n"]}]}