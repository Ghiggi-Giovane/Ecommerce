         id=\"tags-with-dropdown\"\r\n          v-model=\"value\"\r\n          no-outer-focus\r\n          class=\"mb-2\"\r\n        >\r\n          <template v-slot=\"{ tags, disabled, addTag, removeTag }\">\r\n            <ul v-if=\"tags.length > 0\" class=\"list-inline d-inline-block mb-2\">\r\n              <li v-for=\"tag in tags\" :key=\"tag\" class=\"list-inline-item\">\r\n                <b-form-tag\r\n                  @remove=\"removeTag(tag)\"\r\n                  :title=\"tag\"\r\n                  :disabled=\"disabled\"\r\n                  variant=\"info\"\r\n                  >{{ tag }}</b-form-tag\r\n                >\r\n              </li>\r\n            </ul>\r\n\r\n            <b-dropdown\r\n              size=\"sm\"\r\n              variant=\"outline-secondary\"\r\n              block\r\n              menu-class=\"w-100\"\r\n            >\r\n              <template #button-content>\r\n                <b-icon icon=\"tag-fill\"></b-icon> Escolha as máquinas\r\n              </template>\r\n              <b-dropdown-form @submit.stop.prevent=\"() => {}\">\r\n                <b-form-group\r\n                  label=\"Procurar Máquinas\"\r\n                  label-for=\"tag-search-input\"\r\n                  label-cols-md=\"auto\"\r\n                  class=\"mb-0\"\r\n                  label-size=\"sm\"\r\n                  :description=\"searchDesc\"\r\n                  :disabled=\"disabled\"\r\n                >\r\n                  <b-form-input\r\n                    v-model=\"search\"\r\n                    id=\"tag-search-input\"\r\n                    type=\"search\"\r\n                    size=\"sm\"\r\n                    autocomplete=\"off\"\r\n                  ></b-form-input>\r\n                </b-form-group>\r\n              </b-dropdown-form>\r\n              <b-dropdown-divider></b-dropdown-divider>\r\n              <b-dropdown-item-button\r\n                v-for=\"option in availableOptions\"\r\n                :key=\"option\"\r\n                @click=\"onOptionClick({ option, addTag })\"\r\n              >\r\n                {{ option }}\r\n              </b-dropdown-item-button>\r\n              <b-dropdown-text v-if=\"availableOptions.length === 0\">\r\n                Não há máquinas disponíveis para selecionar\r\n              </b-dropdown-text>\r\n            </b-dropdown>\r\n          </template>\r\n        </b-form-tags>\r\n        <div>\r\n          <b-button id=\"btn\" size=\"sm\" v-b-modal.modal-1>\r\n            Tempo: {{ numero }}\r\n          </b-button>\r\n        </div>\r\n        <b-button id=\"btn2\" size=\"sm\" v-b-modal.modal-2>Misturas</b-button>\r\n      </b-navbar-nav>\r\n    </b-navbar>\r\n\r\n    <!-- Modal 1 ---------------------------------------------------------------------------------------------------------------------------------------------------------->\r\n    <b-modal id=\"modal-1\" title=\"Opções!\" @ok=\"btnOkTimer\">\r\n      <div id=\"Modal\">\r\n        Atualizar tela a cada (minutos):\r\n        <b-form-input type=\"number\"></b-form-input>\r\n      </div>\r\n    </b-modal>\r\n\r\n    <!-- Modal 2 ---------------------------------------------------------------------------------------------------------------------------------------------------------->\r\n    <b-modal\r\n      id=\"modal-2\"\r\n      ref=\"modal\"\r\n      title=\"Cadastro da Quantidade de Misturas\"\r\n      size=\"xl\"\r\n      ok-only\r\n      no-enforce-focus\r\n      @ok=\"handleOk\"\r\n      ok-title=\"Salvar\"\r\n    >\r\n      <b-form-input\r\n        v-model=\"filtro\"\r\n        style=\"border: 2px solid black\"\r\n        placeholder=\"Filtro\"\r\n      >\r\n      </b-form-input>\r\n      <p></p>\r\n      <b-row>\r\n        <b-col class=\"modalVfor\" v-for=\"Cor in filtroCores\" :key=\"Cor.Id\">\r\n          <b-card\r\n            v-bind:class=\"corBranca(Cor.CodigoCorHex)\"\r\n            :style=\"Cor.CodigoCorHex\"\r\n            no-body\r\n            class=\"card-style\"\r\n          >\r\n            <b-row>\r\n              <b-col class=\"mb-2\" cols=\"12\" sm=\"8\">\r\n                <span>{{ Cor.CodigoCor }} </span>\r\n              </b-col>\r\n              <b-col cols=\"12\" sm=\"4\">\r\n                <b-form-checkbox v-model=\"Cor.Checked\" name=\"checkbox-1\">\r\n                </b-form-checkbox>\r\n              </b-col>\r\n            </b-row>\r\n\r\n            <b-row>\r\n              <b-col sm=\"6\">\r\n                <h6 class=\"tamanhofonth6\">Data</h6>\r\n                <h6 class=\"tamanhofonth6\">Hora</h6>\r\n                <h6 class=\"tamanhofonth6\">Saldo(Kg)</h6>\r\n              </b-col>\r\n              <b-col sm=\"6\">\r\n                <h6 class=\"tamanhofonth6\">{{ Cor.Data | dateBR }}</h6>\r\n                <h6 class=\"tamanhofonth6\">{{ Cor.Data | Time }}</h6>\r\n                <h6 class=\"tamanhofonth6\">{{ Cor.QuantidadeSaldo }}</h6>\r\n              </b-col>\r\n              <b-col>\r\n                <b-form-input\r\n                  type=\"number\"\r\n                  class=\"mt-2\"\r\n                  placeholder=\"Kg\"\r\n                  v-model=\"Cor.Quantidade\"\r\n                ></b-form-input>\r\n              </b-col>\r\n            </b-row>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n      <!-- <template #modal-footer>\r\n        <b-button variant=\"primary\" @click=\"handleOk\">Salvar</b-button>\r\n      </template> -->\r\n    </b-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport API from \"../../../services/api.service\";\r\n\r\nexport default {\r\n  props: {\r\n    listMaq: {\r\n      type: Array,\r\n      default() {\r\n        return [];\r\n      },\r\n      onSave: Function,\r\n      onClearFilter: Function,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      filtro: \"\",\r\n      search: \"\",\r\n      value: [],\r\n      Cores: [],\r\n      timer: null,\r\n      timeReload: 5,\r\n\r\n      relogio: null,\r\n      numero: \"00:00\",\r\n      ss: 0,\r\n      mm: 0,\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    filtroCores() {\r\n      return this.Cores.filter((f) =>\r\n        f.CodigoCor.includes(this.filtro.toUpperCase())\r\n      );\r\n    },\r\n    minutos() {\r\n      return this.timeReload * 1000 * 60;\r\n    },\r\n\r\n    criteria() {\r\n      // Compute the search criteria\r\n      return this.search.trim().toLowerCase();\r\n    },\r\n    availableOptions() {\r\n      const criteria = this.criteria;\r\n      // Filter out already selected options\r\n      const options = this.listMaq.filter(\r\n        (opt) => this.value.indexOf(opt) === -1\r\n      );\r\n      if (criteria) {\r\n        // Show only options that match criteria\r\n        return options.filter(\r\n          (opt) => opt.toLowerCase().indexOf(criteria) > -1\r\n        );\r\n      }\r\n      // Show all options available\r\n      return options.sort();\r\n    },\r\n    searchDesc() {\r\n      if (this.criteria && this.availableOptions.length === 0) {\r\n        return \"Não há máquinas dispoíveis\";\r\n      }\r\n      return \"\";\r\n    },\r\n  },\r\n  beforeMount() {\r\n    this.BuscaQuantidadeCores();\r\n  },\r\n\r\n  methods: {\r\n    corBranca(Cor) {\r\n      return Cor.includes(\"000000\") || Cor.includes(\"2c2120\")\r\n        ? \"fonteBranca\"\r\n        : \"\";\r\n    },\r\n    handleOk() {\r\n      this.AtualizaQuantidade();\r\n      this.BuscaQuantidadeCores();\r\n    },\r\n\r\n    BuscaQuantidadeCores() {\r\n      API.get(\"v2/Servicos/quantidadeCoresBaias/Get\").then((api) => {\r\n        if (api.data.Success == \"True\" || api.data.Success) {\r\n          this.Cores = api.data.Data;\r\n          this.formcollapsed = false;\r\n        } else {\r\n          this.$root.toastdanger(api.data.Error);\r\n          this.Cores = [];\r\n          this.formcollapsed = true;\r\n        }\r\n      });\r\n    },\r\n    AtualizaQuantidade() {\r\n      API.post(\"v2/Servicos/quantidadeCoresBaias/Save\", this.filtroCores).then(\r\n        (api) => {\r\n          if (api.data.Success == \"True\" || api.data.Success) {\r\n            this.$root.toastsuccess(\"Registro atualizado com sucesso!\");\r\n            // window.location.reload();\r\n          } else {\r\n            this.$root.toastdanger(api.data.Error);\r\n          }\r\n        }\r\n      );\r\n    },\r\n\r\n    btnMaq() {\r\n      this.$emit(\"evtFiltrar\", this.value);\r\n    },\r\n\r\n    onOptionClick({ option, addTag }) {\r\n      addTag(option);\r\n      this.search = \"\";\r\n    },\r\n    btnOkTimer() {\r\n      clearInterval(this.timer);\r\n      clearInterval(this.relogio);\r\n      this.ss = 0;\r\n      this.mm = 0;\r\n\r\n      this.relogio = setInterval(() => {\r\n        this.rodaRelogio();\r\n      }, 1000); //1 segundo\r\n\r\n      if (this.timeReload > 0.9) {\r\n        console.log(\"Inciando timer!\", this.timeReload);\r\n        this.timer = setInterval(this.btnMaq, this.minutos);\r\n      } else {\r\n        this.$emit(\r\n          \"evtError\",\r\n          \"Necessário informar o intervalo de atualizações para obter um melhor funcionamento do programa!\"\r\n        );\r\n        clearInterval(this.relogio);\r\n        this.ss = 0;\r\n        this.mm = 0;\r\n      }\r\n    },\r\n    rodaRelogio() {\r\n      this.ss++;\r\n\r\n      if (this.ss == 60) {\r\n        //deu 59 segundos\r\n        this.mm++;\r\n        this.ss = 0;\r\n        if (this.mm >= this.timeReload) {\r\n          this.mm = 0;\r\n        }\r\n      }\r\n\r\n      let format =\r\n        (this.mm < 10 ? \"0\" + this.mm : this.mm) +\r\n        \":\" +\r\n        (this.ss < 10 ? \"0\" + this.ss : this.ss);\r\n\r\n      return (this.numero = format);\r\n    },\r\n  },\r\n\r\n  beforeDestroy() {\r\n    clearInterval(this.timer);\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#btnNav {\r\n  position: relative;\r\n  display: block;\r\n}\r\n#btn {\r\n  width: 150px;\r\n  height: 30px;\r\n  margin: 5px;\r\n  align-content: center;\r\n}\r\n#btn2 {\r\n  width: 150px;\r\n  height: 30px;\r\n  margin: 5px;\r\n  align-content: center;\r\n}\r\n.modalVfor {\r\n  min-width: 191px;\r\n  margin-bottom: 5px;\r\n  font-size: 14px;\r\n}\r\n.fonteBranca {\r\n  color: white !important;\r\n}\r\n.card-style {\r\n  border: 2px solid black;\r\n}\r\n.tamanhofonth6 {\r\n  font-size: 12px;\r\n  margin-bottom: 0;\r\n}\r\n</style>\r\n"]}]}