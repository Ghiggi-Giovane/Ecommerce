 1\">\r\n        <b-form-group label=\"DepÃ³sito\" label-for=\"input-dep\">\r\n          <b-form-select\r\n            v-model=\"deposito\"\r\n            :options=\"gtxParempDataDepositos(empresa)\"\r\n          >\r\n          </b-form-select>\r\n        </b-form-group>\r\n      </b-col>\r\n      <b-col cols=\"12\" md=\"6\" v-show=\"tipo == 2\">\r\n        <b-form-group label=\"Almoxarifado\" label-for=\"input-dep\">\r\n          <b-form-select v-model=\"deposito\" :options=\"depositosList\">\r\n          </b-form-select>\r\n        </b-form-group>\r\n      </b-col>\r\n    </b-row>\r\n\r\n    <!--\r\n        gtxConfig: {{gtxConfig}} <br>\r\n        gtxConfigParemp: {{gtxConfigParemp}} <br>\r\n        gtxConfigDepos: {{gtxConfigDepos}} <br>\r\n        gtxConfigAlmox: {{gtxConfigAlmox}} <br>\r\n        gtxConfigExped: {{gtxConfigExped}} <br>\r\n        gtxConfigProdu: {{gtxConfigProdu}} <br>\r\n        gtxConfigMater: {{gtxConfigMater}} <br>\r\n        gtxConfigAvari: {{gtxConfigAvari}} <br>\r\n        empresa: {{ empresa }} <br />\r\n        deposito: {{ deposito }} <br />\r\n        gtxAlmoxEmpresaDeposito: {{ gtxAlmoxEmpresaDeposito }} <br />\r\n        depositosList: {{ depositosList }} <br />\r\n        gtxParempDepositosData: {{ gtxParempDepositosData(empresa, tipo) }} <br />\r\n        -->\r\n\r\n    <!-- \r\n        <b-form-input v-model=\"form.tipo\"></b-form-input>\r\n        {{gtxParempDepositosData(form.empresa, form.tipo)}}\r\n\r\n        <p class=\"text-warning\">\r\n            depositos.form: <pre class=\"text-warning\">{{ form }}</pre>\r\n        </p>\r\n\r\n        inputEmit: <pre>{{ inputEmit }}</pre>\r\n-->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions, mapGetters } from \"vuex\";\r\nexport default {\r\n  name: \"EmpresasDepositos\",\r\n\r\n  props: {\r\n    value: {\r\n      type: Object,\r\n      default() {\r\n        return {\r\n          empresa: \"8\",\r\n          deposito: \"37\",\r\n        };\r\n      },\r\n    },\r\n    showempresa: {\r\n      type: Boolean,\r\n      default() {\r\n        return true;\r\n      },\r\n    },\r\n    showdeposito: {\r\n      type: Boolean,\r\n      default() {\r\n        return true;\r\n      },\r\n    },\r\n    tipo: {\r\n      type: Number,\r\n      default() {\r\n        return 2;\r\n      },\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.actParemp();\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\r\n      \"gtxConfig\",\r\n      \"gtxConfigParemp\",\r\n      \"gtxConfigDepos\",\r\n      \"gtxConfigAlmox\",\r\n      \"gtxConfigExped\",\r\n      \"gtxConfigProdu\",\r\n      \"gtxConfigMater\",\r\n      \"gtxConfigAvari\",\r\n      \"gtxConfigDeposito\",\r\n      \"gtxParempData\",\r\n      \"gtxParempDataEmpresas\",\r\n      \"gtxParempDataDepositos\",\r\n      \"gtxParempDataAlmoxarifados\",\r\n      \"gtxAlmoxEmpresaDeposito\",\r\n      \"gtxParempDepositosData\",\r\n    ]),\r\n\r\n    empresa: {\r\n      get() {\r\n        return this.gtxAlmoxEmpresaDeposito.empresa || this.gtxConfigParemp;\r\n      },\r\n      set(value) {\r\n        this.$emit(\"input\", this.empresadeposito);\r\n        this.$emit(\"evt-change\", this.inputEmit);\r\n\r\n        const empdep = {\r\n          empresa: value,\r\n          deposito: this.deposito,\r\n        };\r\n        this.actAlmoxEmpresaDeposito(empdep);\r\n      },\r\n    },\r\n\r\n    deposito: {\r\n      get() {\r\n        return this.gtxAlmoxEmpresaDeposito.deposito;\r\n      },\r\n      set(value) {\r\n        this.$emit(\"input\", this.empresadeposito);\r\n        this.$emit(\"evt-change\", this.inputEmit);\r\n\r\n        const empdep = {\r\n          empresa: this.empresa,\r\n          deposito: value,\r\n        };\r\n        this.actAlmoxEmpresaDeposito(empdep);\r\n\r\n        if (this.tipo == 1) return this.actConfigDepos(value);\r\n        if (this.tipo == 2) return this.actConfigAlmox(value);\r\n        if (this.tipo == 3) return this.actConfigExped(value);\r\n        if (this.tipo == 4) return this.actConfigProdu(value);\r\n        if (this.tipo == 5) return this.actConfigMater(value);\r\n        if (this.tipo == 6) return this.actConfigAvari(value);\r\n      },\r\n    },\r\n\r\n    empresadeposito() {\r\n      return {\r\n        empresa: this.empresa,\r\n        deposito: this.deposito,\r\n      };\r\n    },\r\n\r\n    // busca os depÃ³sitos de acordo com a empresa selecionada ...\r\n    depositosList() {\r\n      return [\r\n        ...[{ value: null, text: \"-- TODOS --\" }],\r\n        ...this.gtxParempDepositosData(this.empresa, this.tipo),\r\n      ];\r\n    },\r\n    almoxList() {\r\n      return this.gtxParempDataAlmoxarifados(this.empresa);\r\n    },\r\n\r\n    inputEmit() {\r\n      return {\r\n        empresa: this.empresa,\r\n        deposito: this.deposito,\r\n        data: {\r\n          empresa: this.gtxParempDataEmpresas.find(\r\n            (e) => e.value == this.empresa\r\n          ),\r\n          deposito: this.gtxParempDataAlmoxarifados(this.empresa).find(\r\n            (a) => a.value == this.deposito\r\n          ),\r\n        },\r\n      };\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    ...mapActions([\r\n      \"actParemp\",\r\n      \"actAlmox\",\r\n      \"actConfigParemp\",\r\n      \"actConfigDepos\",\r\n      \"actConfigAlmox\",\r\n      \"actConfigExped\",\r\n      \"actConfigProdu\",\r\n      \"actConfigMater\",\r\n      \"actConfigAvari\",\r\n      \"actConfigUpdate\",\r\n      \"actAlmoxEmpresaDeposito\",\r\n    ]),\r\n  },\r\n};\r\n</script>\r\n"]}]}