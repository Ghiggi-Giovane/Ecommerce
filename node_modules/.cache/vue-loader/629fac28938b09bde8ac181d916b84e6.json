urn;\r\n      }\r\n    },\r\n\r\n    async getAcompanhamentos() {\r\n      var res = await ApiService.post(\"v2/Acompanhamentos/Header/Get\", {\r\n        Id: this.headerId\r\n      });\r\n\r\n      if (res.data.Success) {\r\n        this.ApiResponse = res.data.Data[0];\r\n      } else {\r\n        return;\r\n      }\r\n    },\r\n\r\n    //TODO editar descricao header\r\n    editDescricaoHeader() {\r\n      console.log(\"Editar descricao Header\");\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.messages {\r\n  overflow-y: auto;\r\n  height: 200px;\r\n}\r\n</style>"]}]}