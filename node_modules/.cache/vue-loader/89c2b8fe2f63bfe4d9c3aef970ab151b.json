aret\r\n            size=\"sm\"\r\n            variant=\"outline-light\"\r\n            class=\"floatmenu\"\r\n            v-if=\"Array.isArray(m.submenus) && m.submenus.length > 1\"\r\n          >\r\n            <template #button-content pill>\r\n              <b-icon-card-list />\r\n            </template>\r\n            <b-dropdown-item\r\n              v-for=\"(sub, index) of m.submenus\"\r\n              :key=\"index\"\r\n              @click.stop=\"subclick(sub)\"\r\n              ><b-icon :icon=\"sub.icon\" class=\"mr-2\" />{{\r\n                sub.caption\r\n              }}</b-dropdown-item\r\n            >\r\n          </b-dropdown>\r\n\r\n          <span class=\"lock\" v-if=\"m.auth\">\r\n            <b-icon-shield-lock />\r\n          </span>\r\n\r\n          <h1><b-icon class=\"mastericon\" :icon=\"m.icon\" /></h1>\r\n          <h3 v-if=\"m.title\">{{ $t(m.title) }}</h3>\r\n          <div class=\"count-title\" v-if=\"m.message\">\r\n            {{ m.message }}\r\n          </div>\r\n        </div>\r\n      </b-card>\r\n    </b-col>\r\n  </b-card-group>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"GridGallery\",\r\n\r\n  props: {\r\n    items: Array,\r\n    default() {\r\n      return [];\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    itemClick(item) {\r\n      if (item.to) {\r\n        if (item.toparam) {\r\n          this.$router.push({\r\n            //path: item.to,\r\n            //query: { param: item.toparam },\r\n            name: item.to.substring(1),\r\n            params: { toparam: item.toparam },\r\n          });\r\n        } else {\r\n          this.$router.push(item.to);\r\n        }\r\n        this.$emit(\"evtItemClick\", item.to);\r\n        return;\r\n      }\r\n      if (item.route) {\r\n        //this.$router.push(item.route)\r\n        console.log(item.route);\r\n        return;\r\n      }\r\n      if (item.anchor) {\r\n        //this.scrollMeTo(item.anchor)\r\n        console.log(item.anchor);\r\n        return;\r\n      }\r\n      if (item.url) {\r\n        //this.openWindow(`https://${item.url}`)\r\n        console.log(item.url);\r\n        return;\r\n      }\r\n    },\r\n    subclick(item) {\r\n      if (item.to) {\r\n        if (item.toparam) {\r\n          this.$router.push({\r\n            //path: item.to,\r\n            //query: { param: item.toparam },\r\n            name: item.to.substring(1),\r\n            params: { toparam: item.toparam },\r\n          });\r\n        } else {\r\n          this.$router.push(item.to);\r\n        }\r\n        this.$emit(\"evtItemClick\", item.to);\r\n        return;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.gallerypanel {\r\n  min-width: 250px;\r\n  min-height: 220px;\r\n  margin-left: 0 !important;\r\n}\r\n.lock {\r\n  position: relative;\r\n  top: -28px;\r\n  float: right;\r\n}\r\n.hand {\r\n  cursor: pointer;\r\n}\r\n.counter {\r\n  background-color: darkred;\r\n  -webkit-transition: -webkit-transform 0.5s ease-in-out;\r\n  -ms-transition: -ms-transform 0.5s ease-in-out;\r\n  transition: background-color 0.5s ease-in-out;\r\n  color: whitesmoke;\r\n  padding: 20px 0;\r\n  border-radius: 5px;\r\n  opacity: 0.85;\r\n  /* border: 2px solid firebrick; */\r\n}\r\n.floatmenu {\r\n  position: absolute;\r\n  top: 5px;\r\n  right: 5px;\r\n  opacity: 0.3;\r\n}\r\n.counter:hover .floatmenu {\r\n  opacity: 1;\r\n  transition: opacity 1s ease-in-out;\r\n}\r\n.counter:hover {\r\n  background-color: indianred;\r\n}\r\n.counter h3 {\r\n  transition: text-shadow 0.5s ease-in-out;\r\n}\r\n.counter .b-icon {\r\n  transition: transform 0.5s ease-in-out;\r\n}\r\n.counter .mastericon {\r\n  animation: lower 1s normal forwards ease-in-out, splash 1;\r\n}\r\n.counter:hover h3 {\r\n  text-shadow: 2px 2px darkslategrey;\r\n}\r\n.counter:hover .mastericon {\r\n  animation: stronger 0.5s normal forwards ease-in-out, splash 1;\r\n  transform: rotate(-5deg);\r\n  -ms-transform: rotate(-5deg);\r\n  -webkit-transform: rotate(-5deg);\r\n}\r\n.count-title {\r\n  font-size: 18px;\r\n  font-weight: normal;\r\n  margin-top: 10px;\r\n  margin-bottom: 0;\r\n  text-align: center;\r\n}\r\n\r\n.count-text {\r\n  font-size: 13px;\r\n  font-weight: normal;\r\n  margin-top: 10px;\r\n  margin-bottom: 0;\r\n  text-align: center;\r\n}\r\n\r\n@keyframes stronger {\r\n  50% {\r\n    font-size: 48px;\r\n  }\r\n  100% {\r\n    font-size: 72px;\r\n  }\r\n}\r\n@keyframes lower {\r\n  0% {\r\n    font-size: 72px;\r\n  }\r\n  100% {\r\n    font-size: 2.5rem;\r\n  }\r\n}\r\n</style>\r\n"]}]}